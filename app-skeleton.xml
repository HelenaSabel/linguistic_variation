<div type="poem" corresp="#A300B991V579">
    <l n="1">
      <app>
         <rdg wit="#A">
            <gap reason="unfinished"/>ouco uos nembra mia se<expan>n<ex>n</ex>
            </expan>or</rdg>
         <rdg wit="#B">Pouco u#09 nenbra mha #s07enhor</rdg>
      </app>
   </l>
    <l n="2">
      <app>
         <rdg wit="#A">quant afan eu por uos leuey</rdg>
         <rdg wit="#B">Quantafam eu por uos leuey</rdg>
      </app>
   </l>
    <l n="3">
      <app>
         <rdg wit="#A">
            <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>quanta coyta por uos ey</rdg>
         <rdg wit="#B">E quanta coyta por uos ey</rdg>
      </app>
   </l>
    <l n="4">
      <app>
         <rdg wit="#A">
            <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>quanto mal me faz amor</rdg>
         <rdg wit="#B">E quanto mal mj faz amor</rdg>
      </app>
   </l>
    <l n="5">
      <app>
         <rdg wit="#A">
            <hi rend="guide">P</hi>or uos <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>non me creedes</rdg>
         <rdg wit="#B">Por uos e non mj creedes</rdg>
      </app>
   </l>
    <l n="6">
      <app>
         <rdg wit="#A">mia coyta nen me ualedes</rdg>
         <rdg wit="#B">Mha coyta nen mj ualedes</rdg>
      </app>
   </l>
    <l n="7">
      <app>
         <rdg wit="#A">
            <hi rend="guide">E</hi> sennor ia perdi o sen</rdg>
         <rdg wit="#B">E #s07#s07enhor ia perdi o#s07#s07en</rdg>
      </app>
   </l>
    <l n="8">
      <app>
         <rdg wit="#A">cuidand en uos et odormir</rdg>
         <rdg wit="#B">Cuydanden uos e o dormir</rdg>
      </app>
   </l>
    <l n="9">
      <app>
         <rdg wit="#A">con gran coyta de uos seruir</rdg>
         <rdg wit="#B">Con gram coyta de u#09 #s02uir</rdg>
      </app>
   </l>
    <l n="10">
      <app>
         <rdg wit="#A">et outro mal muyto me uen</rdg>
         <rdg wit="#B">E outro mal muytomj uen</rdg>
      </app>
   </l>
    <l n="11">
      <app>
         <rdg wit="#A">
            <hi rend="guide">P</hi>or uos <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>non. m. c</rdg>
         <rdg wit="#B">Por uos e non mi creedes</rdg>
      </app>
   </l>
    <l n="12">
      <app>
         <rdg wit="#A">
            <gap reason="economy"/>
         </rdg>
         <rdg wit="#B">Mha coyta nen mj ualedes</rdg>
      </app>
   </l>
    <l n="13">
      <app>
         <rdg wit="#A">
            <hi rend="guide">P</hi>or uos me ue<ex>n</ex>o muyto mal</rdg>
         <rdg wit="#B">Por uos mj u#e02o muyto mal</rdg>
      </app>
   </l>
    <l n="14">
      <app>
         <rdg wit="#A">des aquel dien que uos ui</rdg>
         <rdg wit="#B">De#s07aquel dia que u#09 uj</rdg>
      </app>
   </l>
    <l n="15">
      <app>
         <rdg wit="#A">et uos amey <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>uos serui</rdg>
         <rdg wit="#B">E u#09 amei eu#09 #s07eruj</rdg>
      </app>
   </l>
    <l n="16">
      <app>
         <rdg wit="#A">uiuend en gran coyta mortal</rdg>
         <rdg wit="#B">Vyuendara gram coyta mortal</rdg>
      </app>
   </l>
    <l n="17">
      <app>
         <rdg wit="#A">
            <hi rend="guide">P</hi>or uos <expan>
               <ex>e</ex>
               <am>t</am>
            </expan>non. m. c</rdg>
