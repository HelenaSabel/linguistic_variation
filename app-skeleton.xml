   </l>
    <l n="19">
      <app>
         <rdg wit="#A">
            <hi rend="guide">E</hi> desmesura fazedes</rdg>
         <rdg wit="#B">E de#s07mesura fazedes</rdg>
      </app>
   </l>
    <l n="20">
      <app>
         <rdg wit="#A">que uos de mi non doedes</rdg>
         <rdg wit="#B">Que u#09 de m#j02 non doedes</rdg>
