        <body>
            <div type="poem" xml:id="A1B91">
            <head>
                <title>
                    <app>
                        <rdg wit="#A">A1</rdg>
                        <rdg wit="#B">B91</rdg>
                    </app>
                </title>
                <name role="author">Vasco Praga Sandín</name>
            </head>
            <l n="1">
                <app>
                    <rdg wit="#A"><gap/>guer</rdg>
                    <rdg wit="#B">Deus meu senhor se <expan>u<ex>os</ex></expan> prouguer</rdg>
                </app>       
            </l>
            <l n="2">
                uos
                <app>
                    <rdg wit="#A">me</rdg>
                    <rdg wit="#B" ana="#DC">mj</rdg>
                </app>
                <app>
                    <rdg wit="#A" ana="#synal #PC">tollede</rdg>
                    <rdg wit="#B" ana="#elis #PC">tolhed&apos;</rdg>
                </app>
                este poder
            </l>
            <l n="3">
                que eu
                <app>
                    <rdg wit="#A">ei</rdg>
                    <rdg wit="#B" ana="#y">ey</rdg>
                </app>
                de
                <app>
                <rdg wit="#A">muito</rdg>
                <rdg wit="#B" ana="#y">muyto</rdg>
                </app>
                <app>
                    <rdg wit="#A">uiuer</rdg>
                    <rdg wit="#B" ana="#y">uyuer</rdg>
                </app>
            </l>
            <l n="4">
                ca mentr&apos; eu tal poder
                <app>
                    <rdg wit="#A">ouuer</rdg>
                    <rdg wit="#B" ana="#O">oer</rdg>
                </app>
            </l>
            <l n="5">
                de uiuer nunca
                <app>
                    <rdg wit="#A">perderei</rdg>
                    <rdg wit="#B" ana="#y">perderey</rdg>
                </app>
            </l>
            <l n="6">
                esta
                <app>
                    <rdg wit="#A">coita</rdg>
                    <rdg wit="#B" ana="#y">coyta</rdg>
                </app> 
                <app>
                    <rdg wit="#A" ana="#abbr"><expan>q<ex>ue</ex></expan></rdg>
                    <rdg wit="#B">que</rdg>
                </app>
                <app>
                    <rdg wit="#A">og&apos;</rdg>
                    <rdg wit="#B" ana="#i_g">oi&apos;</rdg>
                </app>
                eu
                <app>
                <rdg wit="#A">ey</rdg>
                <rdg wit="#B" ana="#om"></rdg>
                </app>
            </l>
            <l n="7">
                d&apos; amor eno meu coraçon               
            </l> 
            <milestone unit="stanza"/>
            <l n="8">
                Ca
                <app>
                    <rdg wit="#A">
                        <choice>
                            <reg>mi a</reg>
                            <orig>mia</orig>
                        </choice></rdg>
                    <rdg wit="#B" ana="#h_i">
                        <choice>
                            <reg>mh a</reg>
                            <orig>mha</orig>
                        </choice></rdg>
                </app>
                <app>
                    <rdg wit="#A">faz</rdg>
                    <rdg wit="#B" ana="#graphic">fais</rdg>
                </app>
                <app>
                    <rdg wit="#A">auer</rdg>
                    <rdg wit="#B" ana="#gly">au</rdg>
                </app>
                tal
                <app>
                <rdg wit="#A" ana="#PC">moller</rdg>
                <rdg wit="#B" ana="#abbr #PC">mol<expan>h<ex>er</ex></expan></rdg>
                </app>            
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">que</rdg>                 
                    <rdg wit="#B" ana="#abbr"><expan>q<ex>ue</ex></expan></rdg>
                </app>
                <app>            
                    <rdg wit="#A">nunca</rdg>
                    <rdg wit="#B" ana="#abbr">n<expan>u<ex>n</ex></expan>ca</rdg>
                </app>
                <app>
                    <rdg wit="#A">                  
                   <choice>
                        <reg>mi a</reg>
                        <orig>mia</orig>
                    </choice></rdg>  
                    <rdg wit="#B" ana="#h_i">
                        <choice>
                            <reg>mha</reg>
                            <orig>mh a</orig>
                        </choice></rdg>                    
                </app>
                <app>
                    <rdg wit="#A">ren</rdg>
                    <rdg wit="#B" ana="#abbr">r<expan>e<ex>n</ex></expan></rdg>
                </app>
                de
                <app>
                    <rdg wit="#A">fazer</rdg>
                    <rdg wit="#B" ana="#abbr">fa<expan>z<ex>er</ex></expan></rdg>
                </app>               
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">per</rdg>
                    <rdg wit="#B"><expan>p<ex>er</ex></expan></rdg>
                </app>
                <app>
                    <rdg wit="#A" ana="#gly">q</rdg>
                    <rdg wit="#B" ana="#abbr"><expan>q<ex>ue</ex></expan></rdg>
                </app>
                eu
                <app>
                    <rdg wit="#A">ia possa</rdg>
                    <rdg wit="#B" ana="#metath" cert="medium">possa ia</rdg>
                </app>
                <expan>p<ex>er</ex></expan><expan>d<ex>er</ex></expan>
            </l>
            <l n="11">
                <expan>q<ex>ue</ex></expan>
                <app>
                    <rdg wit="#A" ana="#abbr">enqu<expan>a<ex>n</ex></expan>t&apos;</rdg>
                    <rdg wit="#B" ana="#abbr">en<expan>q<ex>uan</ex></expan>t&apos;</rdg>
                </app>
                eu
                <app>
                    <rdg wit="#A">uiuer</rdg>
                    <rdg wit="#B" ana="#abbr">ui<expan>u<ex>er</ex></expan></rdg>
                </app>
                poder
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">por</rdg>
                    <rdg wit="#B" ana="#abbr"><expan>p<ex>or</ex></expan></rdg>
                </app>
                esto a n<expan>o<ex>n</ex></expan>
                <app>
                    <rdg wit="#A" ana="#y">poderey</rdg>
                    <rdg wit="#B" ana="#abbr">po<expan>d<ex>er</ex></expan>ei</rdg>
                </app>
            </l>
            <l n="13">
                <expan>p<ex>er</ex></expan>der <expan>p<ex>er</ex></expan>
                <app>
                    <rdg wit="#A">ren</rdg>
                    <rdg wit="#B" ana="#abbr">r<expan>e<ex>n</ex></expan></rdg>
                </app>
                <app>
                    <rdg wit="#A">mais</rdg>
                    <rdg wit="#B" ana="#y">mays</rdg>
                </app>
                <app>
                    <rdg wit="#A">auerei</rdg>
                    <rdg wit="#B" ana="#abbr #y">a<expan>u<ex>er</ex></expan>ey</rdg>
                </app>
            </l>
            <l n="14">
                dela
                <app>
                    <rdg wit="#A">mais</rdg>
                    <rdg wit="#B" ana="#y">mays</rdg>
                </app>
                c<expan>o<ex>n</ex></expan>
                <app>
                    <rdg wit="#A">mui</rdg>
                    <rdg wit="#B" ana="#y">muy</rdg>
                </app>
                <app>
                    <rdg wit="#A">gran</rdg>
                    <rdg wit="#B" ana="#m">gram</rdg>
                </app>
                <app>
                <rdg wit="#A" ana="#abbr">raz<expan>o<ex>n</ex></expan></rdg>
                <rdg wit="#B">razon</rdg>
                </app>                
            </l>
            <milestone unit="stanza"/>
            <l n="15">
                <app>
                    <rdg wit="#A" ana="#E">Ca n<expan>o<ex>n</ex></expan> est&apos; e</rdg>
                    <rdg wit="#B" ana="#E">E <expan>q<ex>uan</ex></expan>t&apos; esta</rdg>
                </app>
                <app>
                    <rdg wit="#A" ana="#dip">cuita</rdg>
                    <rdg wit="#B" ana="#y">coyta</rdg>
                </app>
                d&apos; amor
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A"><ex>ũ</ex>a</rdg>
                    <rdg wit="#B" ana="#NEH">h<ex>ũ</ex>a</rdg>
                </app>
                <app>
                    <rdg wit="#B" ana="#ditt">a</rdg>
                </app>
                <expan>q<ex>ue</ex></expan>
                <app>
                    <rdg wit="#A">ome</rdg>
                    <rdg wit="#B" ana="#EH #abbr">ho<expan>m<ex>e</ex></expan></rdg>
                </app>
                <app>
                    <rdg wit="#A">fillar</rdg>
                    <rdg wit="#B" ana="#LF">falar</rdg>
                </app>
                <app>
                    <rdg wit="#A">uen</rdg>
                    <rdg wit="#B" ana="#abbr">u<expan>e<ex>n</ex></expan></rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">se ome</rdg>
                    <rdg wit="#B" ana="#cont">seu am<expan><ex>or</ex></expan></rdg>
                </app>
                leixa
                <app>
                    <rdg wit="#A">sen</rdg>
                    <rdg wit="#B" ana="#abbr">s<expan>e<ex>n</ex></expan></rdg>
                </app>
                seu
                <app>
                    <rdg wit="#A">ben</rdg>
                    <rdg wit="#B" ana="#abbr">b<expan>e<ex>n</ex></expan></rdg>
                </app>
            </l>
            <l n="18">
                ou
                <app>
                    <rdg wit="#A">sen</rdg>
                    <rdg wit="#B" ana="#abbr">s<expan>e<ex>n</ex></expan></rdg>
                    </app>                
                <app>
                    <rdg wit="#A">morte ou se</rdg>
                    <rdg wit="#B" ana="#elis #doubC"><choice>
                            <reg>mort&apos; ou se</reg>
                            <orig>mortousse</orig>
                        </choice></rdg>
                </app>
                faz
                <app>
                    <rdg wit="#A" ana="#PC">mellor</rdg>
                    <rdg wit="#B" ana="#PC #abbr">mel<expan>h<ex>or</ex></expan></rdg>
                </app>
            </l>  
            <l n="19">
                <app>
                    <rdg wit="#A">mais</rdg>
                    <rdg wit="#B" ana="#y">mays</rdg>
                </app>
                <app>
                    <rdg wit="#A">semella</rdg>
                    <rdg wit="#B" ana="#haplog">semela</rdg>
                </app>
                <app>
                    <rdg wit="#A">muit&apos;</rdg>
                    <rdg wit="#B" ana="#y">muyt&apos;</rdg>
                </app>
                <app>
                    <rdg wit="#A">outro</rdg>
                    <rdg wit="#B" ana="#abbr">ou<expan>t<ex>ro</ex></expan></rdg>
                </app>
                mal
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">e quen a</rdg>
                    <rdg wit="#B" ana="#cont">a t<expan>e<ex>n</ex></expan></rdg>
                </app>
                esta
                <app>
                    <rdg wit="#A">cuita</rdg>
                    <rdg wit="#B" ana="#dip #y">coyta</rdg>
                </app>
                tal
            </l>  
            <l n="21">
                macar
                <app>
                    <rdg wit="#A">se</rdg>
                    <rdg wit="#B" ana="#xe">xe</rdg>
                </app>
                morre n<expan>o<ex>n</ex></expan>
                <app>
                <rdg wit="#A" ana="#PC">lle</rdg>
                <rdg wit="#B" ana="#DC #PC">lhi</rdg></app>
                <app>
                    <rdg wit="#A" ana="#abbr"><expan>p<ex>ra</ex></expan>z</rdg>
                    <rdg wit="#B">praz</rdg>
                </app>
            </l>                                            
            </div>
            <div type="poem" xml:id="A2B92">
            <head>
                <title>
                    <app>
                        <rdg wit="#A">A2</rdg>
                        <rdg wit="#B">B92</rdg>
                    </app>
                </title>
                <name role="author">Vasco Praga Sandín</name>
            </head>
                <l n="1">
                    <app>
                        <rdg wit="#A" ana="#PC">Sennor</rdg>
                        <rdg wit="#B" ana="#PC">Senhor</rdg>
                    </app>
                    fremosa 
                    <app>
                        <rdg wit="#A"></rdg>
                        <rdg wit="#B" ana="#cont">muy</rdg>
                    </app>
                <app>
                        <rdg wit="#A">grand&apos;</rdg>
                        <rdg wit="#B" ana="#abbr">gr<expan>a<ex>n</ex></expan>d&apos;</rdg>
                    </app>
                    enueia
                    <app>
                        <rdg wit="#A">ei</rdg>
                        <rdg wit="#B" ana="#y">ey</rdg>
                    </app>
                </l>
                <l n="2">
                    eu a tod&apos; ome que ueio morrer                    
                </l>
                <l n="3">
                    e
                    <app>
                        <rdg wit="#A" ana="#gly">segud&apos;</rdg>
                        <rdg wit="#B">segund&apos;</rdg>
                    </app>
                    ora o meu
                    <app>
                        <rdg wit="#A" ana="#abbr #PC #ç">co<expan>n<ex>n</ex></expan>oçer</rdg>
                        <rdg wit="#B" ana="#PC">conhocer</rdg>
                    </app>
                </l>
                <l n="4">
                    enquant&apos;
                    <app>
                        <rdg wit="#A">este</rdg>
                        <rdg wit="#B" ana="#haplog">est</rdg>
                    </app>
                    faço
                    <app>
                        <rdg wit="#A">mui</rdg>
                        <rdg wit="#B" ana="#y">muy</rdg>
                    </app>
                    <app>
                        <rdg wit="#A">gran</rdg>
                        <rdg wit="#B" ana="#m">gram</rdg>
                    </app>
                    razon
                </l>
                <l n="5">
                    ca
                    <app>
                        <rdg wit="#A">ei</rdg>
                        <rdg wit="#B" ana="#y">ey</rdg>
                    </app>
                    por uos e no meu coraçon
                </l>
                <l n="6">
                    tan
                    <app>
                        <rdg wit="#A">gran</rdg>
                        <rdg wit="#B" ana="#m">gram</rdg>
                    </app>
                    <app>
                        <rdg wit="#A" ana="#dip">cuita</rdg>
                        <rdg wit="#B" ana="#y">coyta</rdg>
                    </app>
                    que mil uezes
                    <app>
                        <rdg wit="#A">meten</rdg>
                        <rdg wit="#B" ana="#m">metem</rdg>
                    </app>
                </l>
                <l n="7">
                    <app>
                        <rdg wit="#A" ana="#PC">sennor</rdg>
                        <rdg wit="#B" ana="#PC">senhor</rdg>
                    </app>
                    sen
                    <app>
                        <rdg wit="#A" ana="#doubC">falla</rdg>
                        <rdg wit="#B">fala</rdg>
                    </app>
                    <app>
                        <rdg wit="#A" ana="#archG">et sen</rdg>
                        <rdg wit="#B" ana="#doubC #m"><choice>
                                <reg> e sem</reg>
                                <orig>essem</orig>
                            </choice></rdg>
                    </app> 
                    tod&apos; o
                    <app>
                        <rdg wit="#A">sen</rdg>
                        <rdg wit="#B" ana="#m">sem</rdg>
                    </app>
                </l>
                <l n="8">
                    e
                    <app>
                        <rdg wit="#A" ana="#abbr">n<expan>o<ex>n</ex></expan></rdg>
                        <rdg wit="#B">non</rdg>
                    </app>
                    <app>
                        <rdg wit="#A">uus</rdg>
                        <rdg wit="#B" ana="#abbr">u<ex>us</ex></rdg>
                    </app>
                    queredes
                    <app>
                        <rdg wit="#A">de</rdg>
                        <rdg wit="#B" ana="#om">d</rdg>
                    </app>
                    <app>
                        <rdg wit="#A" ana="#min">min</rdg>
                        <rdg wit="#B" ana="#j">mj</rdg>
                    </app>
                    doer
                </l>
                <l n="9">
                    Pero
                    <app>
                        <rdg wit="#A" ana="#abbr #PC">s<expan>e<ex>n</ex></expan>nor</rdg>
                        <rdg wit="#B" ana="#PC ">senhor</rdg>
                    </app>
                    <app>
                        <rdg wit="#A"><ex>ũ</ex>a</rdg>
                        <rdg wit="#B" ana="#NEH">h<ex>ũ</ex>a</rdg>
                    </app>
                    <app>
                        <rdg wit="#A">ren</rdg>
                        <rdg wit="#B" ana="#abbr">r<expan>e<ex>n</ex></expan></rdg>
                    </app>
                    u<expan><ex>us</ex></expan>
                    <app>
                        <rdg wit="#A">direi</rdg>
                        <rdg wit="#B" ana="#y">direy</rdg>
                    </app>
                </l>
                <l n="10">
                    <app>
                        <rdg wit="#A">con</rdg>
                        <rdg wit="#B" ana="#abbr">c<expan>o<ex>n</ex></expan></rdg>
                    </app>
                    tod&apos; est&apos; ora n<expan>o<ex>n</ex></expan>
                    <app>
                        <rdg wit="#A">ei</rdg>
                        <rdg wit="#B" ana="#y">ey</rdg>
                    </app>
                    eu poder
                </l>
                <l n="11">
                    <expan>p<ex>er</ex></expan>
                    <app>
                        <rdg wit="#A"> b<ex>õ</ex>a fe</rdg>
                        <rdg wit="#B" ana="#doubC"><choice>
                            <reg>bõa fe</reg>
                            <orig>b<ex>õ</ex>affe</orig>
                        </choice>
                           </rdg>
                    </app>
                    de
                    <app>
                        <rdg wit="#A" ana="#elis">null&apos;</rdg>
                        <rdg wit="#B" ana="#synal #langT #archG" cert="medium">nulla</rdg>
                    </app>
                     enueia auer
                </l>
                <l n="12">
                    a
                    <app>
                        <rdg wit="#A">null&apos;</rdg>
                        <rdg wit="#B" ana="#langT #archG" cert="medium">null&apos;</rdg>
                    </app>
                    ome de
                    <app>
                        <rdg wit="#A">quantos</rdg>
                        <rdg wit="#B" ana="#abbr">qu<expan>a<ex>n</ex></expan>t<expan><ex>us</ex></expan></rdg>
                    </app>
                    uiuos
                    <app>
                        <rdg wit="#A" ana="#abbr">s<expan>o<ex>n</ex></expan></rdg>
                        <rdg wit="#B">son</rdg>
                    </app>
                </l>
                <l n="13">
                    <app>
                        <rdg wit="#A">mais</rdg>
                        <rdg wit="#B" ana="#y">mays</rdg>
                    </app>
                    faç&apos; eu esto por <expan>q<ex>ue</ex></expan>
                    <app>
                        <rdg wit="#A">sei</rdg>
                        <rdg wit="#B" ana="#y">sey</rdg>
                    </app>
                    ca
                    <app>
                        <rdg wit="#A" ana="#abbr">n<expan>o<ex>n</ex></expan></rdg>
                        <rdg wit="#B">non</rdg>
                    </app>
                </l>
                <l n="14">
                    <app>
                        <rdg wit="#A">uiue</rdg>
                        <rdg wit="#B" ana="#y">uyue</rdg>
                    </app>
                    <app>
                        <rdg wit="#A">null&apos;</rdg>
                        <rdg wit="#B" ana="#langT #archG" cert="medium">null&apos;</rdg>
                    </app>
                    <app>
                        <rdg wit="#A">ome</rdg>
                        <rdg wit="#B" ana="#abbr">o<expan>m<ex>e</ex></expan></rdg>
                    </app>
                    <expan>q<ex>ue</ex></expan> de uos
                    <app>
                        <rdg wit="#A">mais</rdg>
                        <rdg wit="#B" ana="#y">mays</rdg>
                    </app>
                    <app>
                        <rdg wit="#A" ana="#abbr">b<expan>e<ex>n</ex></expan></rdg>
                        <rdg wit="#B">ben</rdg>
                    </app>
                </l>
                <l n="15">
                    aia de 
                    <app>
                        <rdg wit="#A">mi</rdg>
                        <rdg wit="#B" ana="#j #min #abbr">m<expan>j<ex>n</ex></expan></rdg>
                    </app>
                    <app>
                        <rdg wit="#A">que</rdg>
                        <rdg wit="#B" ana="#abbr"><expan>q<ex>ue</ex></expan></rdg>
                    </app>
                    n<expan>o<ex>n</ex></expan>
