<tei>
    <div type="poem" corresp="#A118B234">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">A</hi> mellor dona que eu nunca ui</rdg>
                    <rdg wit="#B">A melhor dona q<am>&#772;</am><ex>ue</ex> eu nu<am>&#772;</am><ex>n</ex>ca uj</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">per bo<am>&#772;</am><ex>&#771;</ex>a fe nen que oy dizer</rdg>
                    <rdg wit="#B">per bo<am>&#772;</am><ex>&#771;</ex>a fe nen que oy dizer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">e a que de<am>&#42863;</am><ex>us</ex> fez mellor pareçer</rdg>
                    <rdg wit="#B">e a que de<am>&#42863;</am><ex>us</ex> fez melhor parecer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">mia sennor est e sennor das que ui</rdg>
                    <rdg wit="#B">mha senhor _ e senh<am>&#7651;</am><ex>or</ex> das que uj</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">de mui bon preço e de mui bon sen</rdg>
                    <rdg wit="#B">de muj bon preço e de muj bon sen</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">per bo<am>&#772;</am><ex>&#771;</ex>a fe e de tod' outro ben</rdg>
                    <rdg wit="#B">per boa fe e de tod' outro ben</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">de quant' eu nunca doutra don<del rend="scrape"><gap reason="illegible"/></del> oy</rdg>
                    <rdg wit="#B">a quant' eu nu<am>&#772;</am><ex>n</ex>ca doutra dona oy</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> ben creede de pran que e si</rdg>
                    <rdg wit="#B">E ben creede de pra<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex> e assi</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">e sera ia en quant' ela uiuer</rdg>
                    <rdg wit="#B">e sera ia en qua<am>&#772;</am><ex>n</ex>t' ela uiuer</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">e q<am>&#772;</am><ex>ue</ex>n a uir e a ben connoçer</rdg>
                    <rdg wit="#B">e q<am>&#772;</am><ex>ue</ex>n a uir e a be<am>&#772;</am><ex>n</ex> conhoçer</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">sei eu de pran que dira q<am>&#772;</am><ex>ue</ex>e si</rdg>
                    <rdg wit="#B">sei eu de pra<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex> dira q<am>&#772;</am><ex>ue</ex> e assi</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">ainda u<am>&#42863;</am><ex>os</ex> de seu ben mais direi</rdg>
                    <rdg wit="#B">ai<am>&#772;</am><ex>n</ex>dau<am>&#42863;</am><ex>os</ex> de sen be<am>&#772;</am><ex>n</ex> mays direi</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">e muit' amada p<am>̲</am><ex>er</ex>o q<am>&#772;</am><ex>ue</ex> non sei</rdg>
                    <rdg wit="#B">e muyt' amada p<am>̲</am><ex>er</ex>o q<am>&#772;</am><ex>ue</ex> no<am>&#772;</am><ex>n</ex> sei</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">que<am>&#772;</am><ex>n</ex>na tan muito ame com'*e mi<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex>na ta<am>&#772;</am><ex>n</ex> muyt' ame <!--co#m02--> mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> por tod' esto mal dia naçi</rdg>
                    <rdg wit="#B">E p<am>&#7651;</am><ex>or</ex> tod' esto mal dia naçi</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">por q<am>&#772;</am><ex>ue</ex> lle sei tamanno ben q<am>&#772;</am><ex>ue</ex>rer</rdg>
                    <rdg wit="#B">p<am>&#7651;</am><ex>or</ex> q<am>&#772;</am><ex>ue</ex> lhi soube tam_gram ben <!--q<am>&#772;</am><ex>ue</ex>r#05$05--></rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">como ll' eu q<am>&#772;</am><ex>ue</ex>r e <choice><orig>ueio me</orig><reg>ueio-me</reg></choice> morrer</rdg>
                    <rdg wit="#B">como lh' eu q<am>&#772;</am><ex>ue</ex>r e <choice><orig>ueiome</orig><reg>ueio-me</reg></choice> moirer</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">e non a ueg' e mal dia naçi</rdg>
                    <rdg wit="#B">e non a uei' e mal dia naçi</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">mais rog' a d<am>&#x035B;</am><ex>eu</ex>s que lle tanto ben fez</rdg>
                    <rdg wit="#B">mays rog' a d<am>&#x035B;</am><ex>eu</ex>s q<am>&#772;</am><ex>ue</ex> lhi ta<am>&#772;</am><ex>n</ex>to ben fez</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">que el me guise como algu<am>&#772;</am><ex>n</ex>a uez</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> el mj guise com' algu<am>&#772;</am><ex>&#771;</ex>a uez</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">a ueia çed' u m' eu d' ela p<am>̲</am><ex>er</ex>ti</rdg>
                    <rdg wit="#B">a ueia ced' u m' eu d' ela parti</rdg>
                </app>
            </l>
            <l n="22">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>on mellor coraçon. es contra mi</rdg>
                    <rdg wit="#B">Con melhor coraço<am>&#772;</am><ex>n</ex> e<am>&#7836;</am><ex>ser</ex> coutra mj</rdg>
                </app>
            </l>
        </lg>
    </div>
    
    <div type="poem" corresp="#A119B235">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>uan muit' eu <!--am ua<am>&#772;</am><ex>n</ex>--> moller</rdg>
                    <rdg wit="#B">Quam muyt' eu <!--amu<am>&#772;</am><ex>n</ex>ha--> molher</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">non o sabe nostro sennor <!--<del>nen ar sabe</del>--></rdg>
                    <rdg wit="#B">non o sabe nostro senhor _</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A"><!--<del>n<ex>ost</ex>r<am>&#773;</am>o sennor</del>--> ne ar sabe quam gran pauor</rdg>
                    <rdg wit="#B">_ nen ar sabe quam gram pauor</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">ey og' eu dela <choice><orig>cuidom eu</orig><reg>cuido-m' eu</reg></choice></rdg>
                    <rdg wit="#B">ei oi' eu dela <choice><orig>cuydomeu</orig><reg>cuydo-m' eu</reg></choice></rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">ca se o soubesse sei eu</rdg>
                    <rdg wit="#B">ca se o soubesse sei eu</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">ca se doeria de min</rdg>
                    <rdg wit="#B"><choice><orig>casse</orig><reg>ca se</reg></choice> doeria de mj</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">e non me faria assi</rdg>
                    <rdg wit="#B">e non mj faria assy</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">querer ben a que me mal quer</rdg>
                    <rdg wit="#B">querer ben a quen mj mal quer</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A"><hi rend="guide">P</hi>ero que dizen que negar</rdg>
                    <rdg wit="#B">Pero q<am>&#772;</am><ex>ue</ex> dizen q<am>&#772;</am><ex>ue</ex> negar</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">non xe lle pode nulla ren</rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> xe lhi pode nulha re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">que el non sabia sei eu ben</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> el no<am>&#772;</am><ex>n</ex> sabha sey eu be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">que aind' el non sabe qual</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> anida el no<am>&#772;</am><ex>n</ex> sabe q<am>&#x1DD3;</am><ex>ua</ex>l</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">ben ll' eu quero nen sab' o mal</rdg>
                    <rdg wit="#B">ben lh' eu q<am>&#772;</am><ex>ue</ex>ro ne<am>&#772;</am><ex>n</ex> sab' o mal</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A"><!--quem ela--> por si faz auer</rdg>
                    <rdg wit="#B"><!--q<am>&#772;</am><ex>ue</ex> mela--> p<am>&#7651;</am><ex>or</ex> ssi faz auer</rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">ca se o soubesse doer</rdg>
                    <rdg wit="#B">ca se o soubesse doer</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">sia de mi a meu coidar</rdg>
                    <rdg wit="#B">ssia de mj<am>&#772;</am><ex>n</ex> a meu cuydar</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a d<am>&#x035B;</am><ex>eu</ex>s de tal coraçon e</rdg>
                    <rdg wit="#B">Ca d<am>&#x035B;</am><ex>eu</ex>s de tal coraço<am>&#772;</am><ex>n</ex> e</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">que tanto que sabe q<am>&#772;</am><ex>ue</ex> ten</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> ta<am>&#772;</am><ex>n</ex>to q<am>&#772;</am><ex>ue</ex> sabe q<am>&#772;</am><ex>ue</ex> ten</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">eno seu mui gran coita alguen</rdg>
                    <rdg wit="#B">eno seu muj g<am>&#x1DD3;</am><ex>ra</ex>m cuyt' algue<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">que logo lli consello pon</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> logo lhi <am>&#42863;</am><ex>con</ex>selho po<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">e por esto sei eu que non</rdg>
                    <rdg wit="#B">e por esto sey eu q<am>&#772;</am><ex>ue</ex> no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="22">
                <app>
                    <rdg wit="#A">sab' el a coita que eu ei</rdg>
                    <rdg wit="#B">sab' el a coyta q<am>&#772;</am><ex>ue</ex> eu ey</rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A">nen eu nunca o creerey</rdg>
                    <rdg wit="#B">ne<am>&#772;</am><ex>n</ex> eu nu<am>&#772;</am><ex>n</ex>ca a creerey</rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">por aq<am>&#772;</am><ex>ue</ex>sto p<am>̲</am><ex>er</ex> bo<am>&#772;</am><ex>&#771;</ex>a fe</rdg>
                    <rdg wit="#B">por aquesto per bo<am>&#772;</am><ex>&#771;</ex>a fe</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A120B236">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">O</hi>m' a que deus ben quer fazer</rdg>
                    <rdg wit="#B">Om' a que de<am>&#42863;</am><ex>us</ex> ben quer fazer</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">no<am>&#772;</am><ex>n</ex> lle faz tal sennor amar</rdg>
                    <rdg wit="#B">non lhi faz tal senhor amar</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">a que non ouse ren dizer</rdg>
                    <rdg wit="#B">a que non ouse ren dizer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">con gran pauor de lle pesar</rdg>
                    <rdg wit="#B">con gram pauor de lhi pesar</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">nen o ar fez longe morar</rdg>
                    <rdg wit="#B">nen _ ar fez longi morar</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">d' u ela e sen seu prazer</rdg>
                    <rdg wit="#B">d' u ela e sen seu prazer</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>om' agora min faz uiuer</rdg>
                    <rdg wit="#B">Com' agora mj<am>&#772;</am><ex>n</ex> faz uiuer</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">que me non sei consell' achar</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> me no<am>&#772;</am><ex>n</ex> sei <am>&#42863;</am><ex>con</ex>selh' achar</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">con tan gran coita de soffrer</rdg>
                    <rdg wit="#B">co<am>&#772;</am><ex>n</ex> ta<am>&#772;</am><ex>n</ex> gra<am>&#772;</am><ex>n</ex> coyta de sofrer</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">en qual m' eu ora uei' andar</rdg>
                    <rdg wit="#B">en q<am>&#x1DD3;</am><ex>ua</ex>l m' eu ora uei' a<am>&#772;</am><ex>n</ex>dar</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">com' auer senpr' a deseiar</rdg>
                    <rdg wit="#B">com' auer semp<am>&#x035B;</am><ex>re</ex><!--7--> deseiar</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">mais doutra ren de a ueer</rdg>
                    <rdg wit="#B">mays doutra re<am>&#772;</am><ex>n</ex> de a ueer</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A"><hi rend="guide">M</hi>ais non pod' aquesto saber</rdg>
                    <rdg wit="#B">Mays no<am>&#772;</am><ex>n</ex> pod' aq<am>&#772;</am><ex>ue</ex>sto saber</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">se no<am>&#772;</am><ex>n</ex> a quen d<am>&#x035B;</am><ex>eu</ex>s q<am>&#x365;</am><ex>ui</ex><!----> dar</rdg>
                    <rdg wit="#B">se no<am>&#772;</am><ex>n</ex> a q<am>&#772;</am><ex>ue</ex>no de<am>&#42863;</am><ex>us</ex> q<am>&#772;</am><ex>ue</ex>r dar</rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">a coita que el fez auer</rdg>
                    <rdg wit="#B">a coyta q<am>&#772;</am><ex>ue</ex> el fez auer</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">a min des que me foi mostr<!--*--></rdg>
                    <rdg wit="#B">a mj<am>&#772;</am><ex>n</ex> des q<am>&#772;</am><ex>ue</ex>me foy mostrar</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">a que el fez mellor falar</rdg>
                    <rdg wit="#B">a q<am>&#772;</am><ex>ue</ex> el fez melhor falar</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">do mundo e _ mellor pareçer</rdg>
                    <rdg wit="#B">do mu<am>&#772;</am><ex>n</ex>d' e de melhor parecer</rdg>
                </app>
            </l>
        </lg>
    </div>
    
    <div type="poem" corresp="#A121B237">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">S</hi>ennor fremosa que senp<ex>re</ex>r serui</rdg>
                    <rdg wit="#B">Senhor fremosa q<am>&#772;</am><ex>ue</ex> se<am>&#772;</am><ex>n</ex>pre seruj</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">se deus me leixe d<del>o</del> uos ben au <!--.--></rdg>
                    <rdg wit="#B">se de<am>&#42863;</am><ex>us</ex> me leixe deuos ben auer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">pero mio uos non queredes creer</rdg>
                    <rdg wit="#B">pero mho uos non queredes creer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">des aquel dia sennor que uus ui</rdg>
                    <rdg wit="#B">des aquel dia senh<am>&#7651;</am><ex>or</ex> que u<am>&#42863;</am><ex>os</ex> uj</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">sen uosso g*do me uos faz amor</rdg>
                    <rdg wit="#B">Sen<add place="above">o</add> uosso grado me u<am>&#42863;</am><ex>os</ex> fez amor</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">e sen o meu q<am>&#772;</am><ex>ue</ex>rer gran ben sennor</rdg>
                    <rdg wit="#B">e sen o meu querer gram ben senhor</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> mia se<am>&#772;</am><ex>n</ex>nor assi d<am>&#x035B;</am><ex>eu</ex>s me p<am>̲</am><ex>er</ex> don</rdg>
                    <rdg wit="#B">E mha senh<am>&#7651;</am><ex>or</ex> assi d<am>&#x035B;</am><ex>eu</ex>s mj p<am>̲</am><ex>er</ex>don</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">e me de çedo se<am>&#772;</am><ex>n</ex>nor de uos ben</rdg>
                    <rdg wit="#B">e mj de cedo senhor de uos ben</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">que eu deseio mais que out* ren</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> eu deseio mays ca outra re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">des que uus ui. mia se<am>&#772;</am><ex>n</ex>nor des enton</rdg>
                    <rdg wit="#B">des q<am>&#772;</am><ex>ue</ex> u<am>&#42863;</am><ex>os</ex> uj mha senh<am>&#7651;</am><ex>or</ex> des e<am>&#772;</am><ex>n</ex>to<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A"><hi rend="guide">S</hi>en uosso grado. m. u. f. a. m</rdg>
                    <rdg wit="#B"><!--Sen uosso grado me fez amor--></rdg>
                </app>
            </l>
            <l n="12">
