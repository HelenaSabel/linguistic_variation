<?xml version="1.0" encoding="UTF-8"?>
<div type="poem" corresp="#A226B401V11">
    <lg type="cobra">
        <l n="1">
            <app>
                <rdg wit="#A">-</rdg>
                <rdg wit="#B">Senhor fremosa creede per mj</rdg>
                <rdg wit="#V">Senhor fremosa creede per mi</rdg>
            </app>
        </l>
        <l n="2">
            <app>
                <rdg wit="#A">çon</rdg>
                <rdg wit="#B">queu<am>ꝯ</am><ex>os</ex> amo ia muj de coraçon</rdg>
                <rdg wit="#V">queu<am>ꝯ</am><ex>os</ex> amoia mmde corazon</rdg>
            </app>
        </l>
        <l n="3">
            <app>
                <rdg wit="#A">e gran dereit faç e mui gra<am>̄</am><ex>n</ex>razon</rdg>
                <rdg wit="#B">e gram dereyto face gram razon</rdg>
                <rdg wit="#V">egram dereyto face gram razon</rdg>
            </app>
        </l>
        <l n="4">
            <app>
                <rdg wit="#A">sennor ca nunca outra dona ui</rdg>
                <rdg wit="#B">senhor ca nunca outra dona uj</rdg>
                <rdg wit="#V">senhor ca nunca oura dona ui</rdg>
            </app>
        </l>
        <l n="5">
            <app>
                <rdg wit="#A"> <gap reason="unfinished"/>an mansa ne<am>̄</am><ex>n</ex> tan a posto catar</rdg>
                <rdg wit="#B">Tan ma<am>̄</am><ex>n</ex> sa nentan aposto catar</rdg>
                <rdg wit="#V">tam mansa ne<am>̄</am><ex>n</ex>tan a posto catar</rdg>
            </app>
        </l>
        <l n="6">
            <app>
                <rdg wit="#A">nen tan fremosa nen tan ben falar</rdg>
                <rdg wit="#B">nen ta<am>̄</am><ex>n</ex> fremosa nen tam ben falar</rdg>
                <rdg wit="#V">nen tan fremosa nen tam ben falar</rdg>
            </app>
        </l>
        <l n="7">
            <app>
                <rdg wit="#A">Come uos sennor e pois assi e</rdg>
                <rdg wit="#B">Come uos senhor epois assy e</rdg>
                <rdg wit="#V">Come uos senhor epois assy e</rdg>
            </app>
        </l>
        <l n="8">
            <app>
                <rdg wit="#A">mui gran dereito façen uus q<am>̄</am><ex>ue</ex>rer</rdg>
                <rdg wit="#B">mui gam d<am>&#x035B;</am><ex>eu</ex>eito face nu<am>ꝯ</am><ex>os</ex> q<am>̄</am><ex>ue</ex>rer</rdg>
                <rdg wit="#V">mui gam deito fazanu<am>ꝯ</am><ex>os</ex> q<am>̄</am><ex>ue</ex>rer</rdg>
            </app>
        </l>
        <l n="9">
            <app>
                <rdg wit="#A">mui gran ben ca nunca pude ueer</rdg>
                <rdg wit="#B">muj gam be<am>̄</am><ex>n</ex> ca uu<am>̄</am><ex>n</ex>ca pudi ueer</rdg>
                <rdg wit="#V">mui gam be<am>̄</am><ex>n</ex> ca nu<am>̄</am><ex>n</ex>ca pudi ueer</rdg>
            </app>
        </l>
        <l n="10">
            <app>
                <rdg wit="#A">outra dona sennor per bo<am>̄</am><ex>n</ex>a fe</rdg>
                <rdg wit="#B">outa dona f<am>&#x035B;</am><ex>re</ex>mosa p<am>̲</am><ex>er</ex> bo<am>̄</am><ex>n</ex>a fe</rdg>
                <rdg wit="#V">outa dona f<am>&#x035B;</am><ex>re</ex>mosa per bona fe</rdg>
            </app>
        </l>
        <l n="11">
            <app>
                <rdg wit="#A">Tan mansa nen tan a posto</rdg>
                <rdg wit="#B">Tan massa ne<am>̄</am><ex>n</ex> ta<am>̄</am><ex>n</ex> aposto catar</rdg>
                <rdg wit="#V">tan manssa ne<am>̄</am><ex>n</ex> ta<am>̄</am><ex>n</ex> aposto catar</rdg>
            </app>
        </l>
        <l n="12">
            <app>
                <rdg wit="#A">Come uos por que çedo morrerei</rdg>
                <rdg wit="#B">Come uos p<am>&#x1DD1;</am><ex>or</ex> q<am>̄</am><ex>ue</ex> cedo moirerey</rdg>
                <rdg wit="#V">Come uos p<am>&#x1DD1;</am><ex>or</ex> q<am>̄</am><ex>ue</ex> cedo morrerey</rdg>
            </app>
        </l>
        <l n="13">
            <app>
                <rdg wit="#A">pero direi uus ant ua<am>̄</am><ex>n</ex> ren</rdg>
                <rdg wit="#B">p<am>̲</am><ex>er</ex>o direyu<am>ꝯ</am><ex>os</ex> antu<am>̄</am><ex>n</ex>ha re<am>̄</am><ex>n</ex> </rdg>
                <rdg wit="#V">pero direyu<am>ꝯ</am><ex>os</ex> antu<am>̄</am><ex>n</ex>ha re<am>̄</am><ex>n</ex> </rdg>
            </app>
        </l>
        <l n="14">
            <app>
                <rdg wit="#A">dereito faç en uus querer gran ben</rdg>
                <rdg wit="#B">d<am>&#x035B;</am><ex>er</ex>eyto façenu<am>ꝯ</am><ex>os</ex> q<am>̄</am><ex>ue</ex>rer gam ben</rdg>
                <rdg wit="#V">d<am>&#x035B;</am><ex>er</ex>eyto fazenu<am>ꝯ</am><ex>os</ex> q<am>̄</am><ex>ue</ex>rer gam ben</rdg>
            </app>
        </l>
        <l n="15">
            <app>
                <rdg wit="#A">ca nunca dona ui nen ueerei</rdg>
                <rdg wit="#B">ca uu<am>̄</am><ex>n</ex>ca dona uj ne<am>̄</am><ex>n</ex> ueerey</rdg>
                <rdg wit="#V">ca uu<am>̄</am><ex>n</ex>ca dona ui ne<am>̄</am><ex>n</ex> ueerey</rdg>
            </app>
        </l>
        <l n="16">
            <app>
                <rdg wit="#A"> <hi rend="guide">T</hi>an mansa nen tan aposto catar</rdg>
                <rdg wit="#B">Ta<am>̄</am><ex>n</ex> ma<am>̄</am><ex>n</ex>ssa</rdg>
                <rdg wit="#V">ta<am>̄</am><ex>n</ex> ma<am>̄</am><ex>n</ex>ssa</rdg>
            </app>
        </l>
    </lg>
</div>

