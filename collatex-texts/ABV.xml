<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem"
        corresp="#A246B811V395">
        <l n="1">
            <app>
                <rdg wit="#A"><hi rend="guide">A</hi> dona que ome sennor deuia</rdg>
                <rdg wit="#B">A dona que home senhor deuia</rdg>
                <rdg wit="#V">A dona que <add rend="interlinha">ho</add>me senhor deuia</rdg>
            </app>
        </l>
        <l n="2">
            <app>
                <rdg wit="#A">con dereito chamar per boa fe</rdg>
                <rdg wit="#B">con dereyt'_a chamar per boa fe</rdg>
                <rdg wit="#V">con dereyt'_a chamar per boa fe</rdg>
            </app>
        </l>
        <l n="3">
            <app>
                <rdg wit="#A">meus amigos <choice><orig>direiu<ex>os</ex></orig><reg>direi-uos</reg></choice> u qual e</rdg>
                <rdg wit="#B">me<ex>us</ex> amig<ex>os</ex> <choice><orig>direiu<ex>os</ex></orig><reg>direi-uos</reg></choice> eu quen e</rdg>
                <rdg wit="#V">me<ex>us</ex> amig<ex>os</ex> <choice><orig>direi u<ex>os</ex></orig><reg>direi-uos</reg></choice> eu quen e</rdg>
            </app>
        </l>
        <l n="4">
            <app>
                <rdg wit="#A">ua<ex>n</ex> dona que eu ui noutro dia</rdg>
                <rdg wit="#B">hunha dona q<ex>ue</ex> eu uj noutro dia</rdg>
                <rdg wit="#V">hunha dona q<ex>ue</ex> eu ui noutro dia</rdg>
            </app>
        </l>
        <l n="5">
            <app>
                <rdg wit="#A">e non <del rend="underdot">llousey</del><add rend="margem" who="revisor">ousem</add> mais daquesto dizer</rdg>
                <rdg wit="#B">e non lh' ousei mays datanto dizer</rdg>
                <rdg wit="#V">e non lh' ousey mays datanto dizer</rdg>
            </app>
        </l>
        <l n="6">
            <app>
                <rdg wit="#A">mais quen a uisse podess' entender</rdg>
                <rdg wit="#B">mais quen a uisse podess' entender</rdg>
                <rdg wit="#V">mays quen a uisse podess' entender</rdg>
            </app>
        </l>
        <l n="7">
            <app>
                <rdg wit="#A">todo seu ben sennor la chamaria</rdg>
                <rdg wit="#B">todo seu ben senhor a chamaria</rdg>
                <rdg wit="#V">todo seu ben senhor a chamaria</rdg>
            </app>
        </l>
        <l n="8">
            <app>
                <rdg wit="#A"><hi rend="guide">C</hi>a se<ex>n</ex>nor e de muito ben e uia</rdg>
                <rdg wit="#B">Ca senhor e de muyto be<ex>n</ex> e uya</rdg>
                <rdg wit="#V">Ca senhor e de muyto be<ex>n</ex> e uya</rdg>
            </app>
        </l>
        <l n="9">
            <app>
                <rdg wit="#A">eu por meu mal <choice><orig>sey o</orig><reg>sey-o</reg></choice> p* boa fe</rdg>
                <rdg wit="#B">_ p*olo meu mal <choice><orig>sseyo</orig><reg>ssey-o</reg></choice> p*<del rend="overstrike">e</del> boa fe</rdg>
                <rdg wit="#V">_ polo meu mal <choice><orig>sseyo</orig><reg>ssey-o</reg></choice> p* bo<ex>n</ex>a fe</rdg>
            </app>
        </l>
        <l n="10">
            <app>
                <rdg wit="#A">e <del rend="underdot">de</del> morrer por e<ex>n</ex> gran dereit' e</rdg>
                <rdg wit="#B">e de moirer p<ex>or</ex> ela _ d<ex>er</ex>eit' e</rdg>
                <rdg wit="#V">e de morrer p<ex>or</ex> ela _ d<ex>er</ex>eit' e</rdg>
            </app>
        </l>
        <l n="11">
            <app>
                <rdg wit="#A">ca ben soub' eu qua<ex>n</ex>to m' end' auerria</rdg>
                <rdg wit="#B">ca be<ex>n</ex> soub' eu qua<ex>n</ex>to m' end' aueiria</rdg>
                <rdg wit="#V">ca ben soub' en quanto m' end' auerria</rdg>
            </app>
        </l>
        <l n="12">
            <app>
                <rdg wit="#A">morrer assi com' eu moyr' e perder</rdg>
                <rdg wit="#B">moirer assy com' eu moyr' e p<ex>er</ex>der</rdg>
                <rdg wit="#V">morrer assy com' eu moyr' e p<ex>er</ex>der</rdg>
            </app>
        </l>
        <l n="13">
            <app>
                <rdg wit="#A">meus amigos o corp' e no<ex>n</ex> poder</rdg>
                <rdg wit="#B">me<ex>us</ex> amig<ex>os</ex> o corp' e no<ex>n</ex> <del rend="overstrike">p*oder p<ex>er</ex>der</del><add rend="interlinha">poder</add></rdg>
                <rdg wit="#V">me<ex>us</ex> amig<ex>os</ex> o corp' e no<ex>n</ex> poder</rdg>
            </app>
        </l>
        <l n="14">
            <app>
                <rdg wit="#A">ueer ela quando ueer querria</rdg>
                <rdg wit="#B">ueer ela q<ex>ue</ex>_eu ueer q<ex>ue</ex>iria</rdg>
                <rdg wit="#V">ueer ela q<ex>ue</ex>_eu ueer q<ex>ue</ex>rria</rdg>
            </app>
        </l>
        <l n="15">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi> tod' aquesto m' ant' eu entendia</rdg>
                <rdg wit="#B">E tod' aq<ex>ue</ex>sto m' a<ex>n</ex>t' eu entendia</rdg>
                <rdg wit="#V">E tod' aq<ex>ue</ex>sto m' a<ex>n</ex>t' eu entendia</rdg>
            </app>
        </l>
        <l n="16">
            <app>
                <rdg wit="#A">que a uisse mas tant' oy falar</rdg>
                <rdg wit="#B">q<ex>ue</ex> a uisse mays ta<ex>n</ex>to oy falar</rdg>
                <rdg wit="#V">q<ex>ue</ex> a uisse mays ta<ex>n</ex>to oy falar</rdg>
            </app>
        </l>
        <l n="17">
            <app>
                <rdg wit="#A">no seu ben que me non soube guadar</rdg>
                <rdg wit="#B">de sseu ben q<ex>ue</ex> me no<ex>n</ex> soubi guardar</rdg>
                <rdg wit="#V">de sseu ben q<ex>ue</ex> me no<ex>n</ex> soubi guardar</rdg>
            </app>
        </l>
        <l n="18">
            <app>
                <rdg wit="#A">nen_cuidaua_que_tan_ben_pareçia</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">nen er cuydey que ta<ex>n</ex> ben pareçia</rdg>
            </app>
        </l>
        <l n="19">
            <app>
                <rdg wit="#A"> que log' eu fosse por ela morrer</rdg>
                <rdg wit="#B"> q<ex>ue</ex> logu' eu fosse p<ex>or</ex> ela moirer</rdg>
                <rdg wit="#V"><del rend="overstrike"></del><add rend="interlinha">parecia</add> que logn eu fosse p<ex>or</ex> ela morrer</rdg>
            </app>
        </l>
        <l n="20">
            <app>
                <rdg wit="#A">mais u eu ui o seu bon pareçer</rdg>
                <rdg wit="#B">mays qua<ex>n</ex>d' eu uj o seu bo<ex>n</ex> parecer</rdg>
                <rdg wit="#V">mays qua<ex>n</ex>d' eu ui o seu bo<ex>n</ex> parecer</rdg>
            </app>
        </l>
        <l n="21">
            <app>
                <rdg wit="#A">ui amigos que mia morte seria</rdg>
                <rdg wit="#B">uj amig<ex>os</ex> q<ex>ue</ex> mha morte seria</rdg>
                <rdg wit="#V">ui amig<ex>os</ex> q<ex>ue</ex> mha morte seria</rdg>
            </app>
        </l>
        <l n="22">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi>_por_esto_que_ben_consellaria</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="23">
            <app>
                <rdg wit="#A">quantos_oyren_no_seu_be<ex>n</ex>_falar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="24">
            <app>
                <rdg wit="#A">non_a_ueian_e_poden_se_guardar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="25">
            <app>
                <rdg wit="#A">mellor_ca_m'_end'_eu_guardei_q<ex>ue</ex>_mor<add rend="interlinha">e</add>ria</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="26">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi>_dixe_mal_mais_fez_me_d*s_auer</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="27">
            <app>
                <rdg wit="#A">tal_uentura_quando_a_fuy_ueer</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="28">
            <app>
                <rdg wit="#A">que_nunca_dix'_o_que_dizer_q<ex>ue</ex>ria</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
    </div>
    <div type="poem"
        corresp="#A255B842V428">
        <l n="1">
            <app>
                <rdg wit="#A">A mia sennor que por mal destes meus</rdg>
                <rdg wit="#B">A mha se<ex>n</ex>h<ex>or</ex> q<ex>ue</ex> p<ex>or</ex> mal destes me<ex>us</ex></rdg>
                <rdg wit="#V">A mha senhor que por mal destes me<ex>us</ex></rdg>
            </app>
        </l>
        <l n="2">
            <app>
                <rdg wit="#A">ollos eu_ui <choice><orig>fuylle</orig><reg>fuy-lle</reg></choice> gran ben querer</rdg>
                <rdg wit="#B">olhos auy <choice><orig>fuylhe</orig><reg>fuy-lhe</reg></choice> gra<ex>n</ex> be<ex>n</ex> q<ex>ue</ex>rer</rdg>
                <rdg wit="#V">olhos auy <choice><orig>fuylhe</orig><reg>fuy-lhe</reg></choice> gra<ex>n</ex> ben querer</rdg>
            </app>
        </l>
        <l n="3">
            <app>
                <rdg wit="#A">e o mellor que dela pod/poyd auer</rdg>
                <rdg wit="#B">* o melhor q<ex>ue</ex> dela pud' auer</rdg>
                <rdg wit="#V">* o melhor que dela pud' auor</rdg>
            </app>
        </l>
        <l n="4">
            <app>
                <rdg wit="#A">des que a ui <choice><orig>direi uolo</orig><reg>direi-uo-lo</reg></choice> par deus</rdg>
                <rdg wit="#B">des que a uy dysey_uolo par deus</rdg>
                <rdg wit="#V">des que a uy dyr_uolo par de<ex>us</ex></rdg>
            </app>
        </l>
        <l n="5">
            <app>
                <rdg wit="#A"><gap reason="unfinished"/>isso moge <del rend="del">ca<ex>n</ex> m*e</del>_que ca me queria ben</rdg>
                <rdg wit="#B">⌐Disse moie q<ex>ue</ex> me queria ben</rdg>
                <rdg wit="#V">disse moie que me queria bem</rdg>
            </app>
        </l>
        <l n="6">
            <app>
                <rdg wit="#A">pero que nunca me faria ben</rdg>
                <rdg wit="#B">p*o que nu<ex>n</ex>ca me feria bem</rdg>
                <rdg wit="#V">po que nu<ex>n</ex>ca me faria bem</rdg>
            </app>
        </l>
        <l n="7">
            <app>
                <rdg wit="#A">E por esto que me disso cuydou</rdg>
                <rdg wit="#B">ATENCOME por esto q<ex>ue</ex> me disse cuydou</rdg>
                <rdg wit="#V">ATENCOME por esto q<ex>ue</ex> me disse cuydou</rdg>
            </app>
        </l>
        <l n="8">
            <app>
                <rdg wit="#A">min a guarir que ia moiro mais non</rdg>
                <rdg wit="#B">mj a guarir q<ex>ue</ex> ia moyro mays no<ex>n</ex></rdg>
                <rdg wit="#V">mi a guarir q<ex>ue</ex> ia moyro mays no<ex>n</ex></rdg>
            </app>
        </l>
        <l n="9">
            <app>
                <rdg wit="#A">p<ex>er</ex>di poren coita do coraçon</rdg>
                <rdg wit="#B">p<ex>er</ex>dy pore<ex>n</ex> coyta do coraçom</rdg>
                <rdg wit="#V">p<ex>er</ex>dy pore<ex>n</ex> coyta do coracom</rdg>
            </app>
        </l>
        <l n="10">
            <app>
                <rdg wit="#A">pero ben foy mais do que me matou</rdg>
                <rdg wit="#B">p*o be<ex>n</ex> foy mays do q<ex>ue</ex> me<ex>us</ex> matou</rdg>
                <rdg wit="#V">p*o be<ex>n</ex> foy mays do q<ex>ue</ex> me<ex>us</ex> matou</rdg>
            </app>
        </l>
        <l n="11">
            <app>
                <rdg wit="#A"><hi rend="guide">D</hi>isso moge ca_me_queria_ben</rdg>
                <rdg wit="#B">Disse moie</rdg>
                <rdg wit="#V"></rdg>
            </app>
        </l>
        <l n="12">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi> por aquesto cuida que seu prez</rdg>
                <rdg wit="#B">E por aquesto cuyda q<ex>ue</ex> seu p*z</rdg>
                <rdg wit="#V">E por aquesto cuyda q<ex>ue</ex> seu p*z</rdg>
            </app>
        </l>
        <l n="13">
            <app>
                <rdg wit="#A">tod' a p<ex>er</ex>dud'<del>o</del> e uedes qual sennor</rdg>
                <rdg wit="#B">tod' a p<ex>er</ex>dud' e ued<ex>e</ex>s qual senhor</rdg>
                <rdg wit="#V">tod' a perdud' e ued<ex>e</ex>s qual senhor</rdg>
            </app>
        </l>
        <l n="14">
            <app>
                <rdg wit="#A">me faz amar muito d<ex>eu</ex>s e amor</rdg>
                <rdg wit="#B">me faz amar _ d<ex>eu</ex>s _ amor</rdg>
                <rdg wit="#V">me faz amar _ d<ex>eu</ex>z <ex>e</ex> amor</rdg>
            </app>
        </l>
        <l n="15">
            <app>
                <rdg wit="#A">* o mellor que mela nunca fez</rdg>
                <rdg wit="#B">may omelhor q<ex>ue</ex> me la nu<ex>n</ex>ca fez</rdg>
                <rdg wit="#V">may o melhor q<ex>ue</ex> mela nu<ex>n</ex>ca fez</rdg>
            </app>
        </l>
        <l n="16">
            <app>
                <rdg wit="#A"><hi rend="guide">D</hi>isso_moie ca_me_queria_ben</rdg>
                <rdg wit="#B">⌐Disse_moie</rdg>
                <rdg wit="#V">disemoie.</rdg>
            </app>
        </l>
        <l n="17">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi>_entendeu_ca_me_quer_atal_ben</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="18">
            <app>
                <rdg wit="#A">en_que_non_p<ex>er</ex>de_nen_gaan(o)_eu_ren</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
    </div>
</tei>
