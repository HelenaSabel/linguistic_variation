<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem"
        corresp="#A248B816V400">
        <l n="1">
            <app>
                <rdg wit="#A"><hi rend="guide">O</hi>y eu sempre mia sennor dizer</rdg>
                <rdg wit="#A2"><hi rend="guide">O</hi>y eu sempre mia sennor dizer</rdg>
                <rdg wit="#B">Oy eu sempre mha senhor dizer</rdg>
                <rdg wit="#V">Oy eu sempre mha senhor dizer</rdg>
            </app>
        </l>
        <l n="2">
            <app>
                <rdg wit="#A">que peor he de soffrer o gran</rdg>
                <rdg wit="#A2">que peor e de soffrer o gran ben</rdg>
                <rdg wit="#B">que peyor e de sofrer o gra<ex>n</ex> ben</rdg>
                <rdg wit="#V">que peyor e de sofrer o gram be<ex>n</ex></rdg>
            </app>
        </l>
        <l n="3">
            <app>
                <rdg wit="#A">ben ca o gran mal. e marauillo|men</rdg>
                <rdg wit="#A2">ca o gran mal. e marauillo|men</rdg>
                <rdg wit="#B">que o gram mal. e marauilhomen</rdg>
                <rdg wit="#V">que o gram mal e marauilhomen</rdg>
            </app>
        </l>
        <l n="4">
            <app>
                <rdg wit="#A">eno no pude nen posso creer</rdg>
                <rdg wit="#A2">e non o pudy nen posso creer</rdg>
                <rdg wit="#B">e non o pudi nen posso creer</rdg>
                <rdg wit="#V">e non o pudi nen posso creer</rdg>
            </app>
        </l>
        <l n="5">
            <app>
                <rdg wit="#A">ca soffro eu mal. por uos qual mal sennor</rdg>
                <rdg wit="#A2">ca soffro eu mal. por uos qual mal sennor</rdg>
                <rdg wit="#B">ca sofreu mal por uos qual mal se<ex>n</ex>hor</rdg>
                <rdg wit="#V">ca sofreu mal por uos qual uos mal senhor</rdg>
            </app>
        </l>
        <l n="6">
            <app>
                <rdg wit="#A"> me quer matar. e guaria mellor</rdg>
                <rdg wit="#A2">me quer matar e gual<del rend="erasure">rr</del>rria mellor</rdg>
                <rdg wit="#B">me q<ex>ue</ex>r matar. e guarria melhor</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="7">
            <app>
                <rdg wit="#A">se me uos ben quisesedes fazer</rdg>
                <rdg wit="#A2">se me uos ben quisesedes fazer</rdg>
                <rdg wit="#B">se mj uos ben quise<ex>n</ex>des fazer</rdg>
                <rdg wit="#V">se mi uos ben quisesdes façer</rdg>
            </app>
        </l>
        <l n="8">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi> se eu ben de uos podess auer</rdg>
                <rdg wit="#A2"><hi rend="guide">E</hi> se eu ben de uos podess' auer</rdg>
                <rdg wit="#B">Esse eu be<ex>n</ex> de uos podess' auer</rdg>
                <rdg wit="#V">E sse eu be<ex>n</ex> de uos podess' auer</rdg>
            </app>
        </l>
        <l n="9">
            <app>
                <rdg wit="#A">ficass o mal que por uos ey a quen</rdg>
                <rdg wit="#A2">ficass' o mal que por uos ey a quem</rdg>
                <rdg wit="#B">ficass' o mal q<ex>ue</ex> p<ex>or</ex> uos ey aque<ex>n</ex></rdg>
                <rdg wit="#V">ficass' o mal q<ex>ue</ex> p<ex>or</ex> uos ey aque<ex>n</ex></rdg>
            </app>
        </l>
        <l n="10">
            <app>
                <rdg wit="#A">a questo diz e o queassi ten </rdg>
                <rdg wit="#A2">aquesto diz e o que assi ten</rdg>
                <rdg wit="#B">aq<ex>ue</ex>sto diz e o q<ex>ue</ex> assy te<ex>n</ex></rdg>
                <rdg wit="#V">aq<ex>ue</ex>sto diz e o q<ex>ue</ex> assy te<ex>n</ex></rdg>
            </app>
        </l>
        <l n="11">
            <app>
                <rdg wit="#A">o mal en pouco façao uiuer</rdg>
                <rdg wit="#A2">o mal en pouco façao uiuer</rdg>
                <rdg wit="#B">o mal en pouco façao uiuer</rdg>
                <rdg wit="#V">o mal en pouco façao uiuer</rdg>
            </app>
        </l>
        <l n="12">
            <app>
                <rdg wit="#A">deus con mal senpr e con coita damor</rdg>
                <rdg wit="#A2">deus con mal sempr' e co<ex>n</ex> coyta d'amor</rdg>
                <rdg wit="#B">d*s co<ex>n</ex>mal sempr' e co<ex>n</ex> coita d' amor</rdg>
                <rdg wit="#V">ds* co<ex>n</ex> mal sempr' e co<ex>n</ex> coita d' amor</rdg>
            </app>
        </l>
        <l n="13">
            <app>
                <rdg wit="#A">e podassi ueer qual e peor</rdg>
                <rdg wit="#A2">e pod' assi ueer qual e peor</rdg>
                <rdg wit="#B">e podesse ueer qual e peyor</rdg>
                <rdg wit="#V">e podesse ueer qual e peyor</rdg>
            </app>
        </l>
        <l n="14">
            <app>
                <rdg wit="#A">do g<ex>ra</ex>n ben ou do g<ex>ra</ex>n mal de soffrer</rdg>
                <rdg wit="#A2">do gran ben ou do gran mal de soffrer</rdg>
                <rdg wit="#B">do muj gram be<ex>n</ex> ou do gra<ex>n</ex> mal sofrer</rdg>
                <rdg wit="#V">do mui gram be<ex>n</ex> ou do gra<ex>n</ex> mal sofrer</rdg>
            </app>
        </l>
        <l n="15">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi>_o_que_esto_diz_non_sab_amar</rdg>
                <rdg wit="#A2"><hi rend="guide">E</hi>_o_que_esto_diz_non_sab'_amar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="16">
            <app>
                <rdg wit="#A">neua<ex>n</ex> cousa_tan_de_coraçon</rdg>
                <rdg wit="#A2">neua<ex>n</ex>_cousa_tan_de_coraçon</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="17">
            <app>
                <rdg wit="#A">comeu_sennor_amo_uos_de_mais_non</rdg>
                <rdg wit="#A2">como_eu_se<ex>n</ex>nor_amo_uos_de_mais_non</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="18">
            <app>
                <rdg wit="#A">creo_que_sabe_que_xe_deseiar</rdg>
                <rdg wit="#A2">creo_que_sabe_que_xe_deseiar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="19">
