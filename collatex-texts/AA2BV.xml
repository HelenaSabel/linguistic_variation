                <rdg wit="#A">o_uosso_bon_pareçer_que_dessi</rdg>
                <rdg wit="#A2">o_uosso_bon_pareçer_que_de_si</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="21">
            <app>
                <rdg wit="#A">me_faz_por_uos_muitas_coitas_leuar</rdg>
                <rdg wit="#A2">me_faz_por_uos_muitas_coitas_leuar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="22">
            <app>
                <rdg wit="#A"><hi rend="guide">E</hi>_de_qual_eu_sennor_ouço_contar</rdg>
                <rdg wit="#A2"><hi rend="guide">E</hi>_de_q<ex>ue</ex>l_eu_se<ex>n</ex>nor_ouço_cu<ex>n</ex>tar</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="23">
            <app>
                <rdg wit="#A">que_o ben_est_e faz_g<ex>ra</ex>n_trayçion</rdg>
                <rdg wit="#A2">que_o_ben_est_faz_gran_trayçion</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="24">
            <app>
                <rdg wit="#A">o_que_ben_a se_o seu_coracon</rdg>
                <rdg wit="#A2">o_que_ben_a_se_o_seu_coraçon</rdg>
                <rdg wit="#B">_</rdg>
                <rdg wit="#V">_</rdg>
            </app>
        </l>
        <l n="25">
