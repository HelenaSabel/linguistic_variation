<root>
    <div type="poem" corresp="#A199B350">
      <l n="1">
         <app>
            <rdg wit="#A">A mia sennor que me foi amostrar</rdg>
            <rdg wit="#B">A mha senhor que me foy mostrar</rdg>
         </app>
      </l>
      <l n="2">
         <app>
            <rdg wit="#A">deus por meu mal por uus eu non mentir</rdg>
            <rdg wit="#B">de<ex>us</ex> por meu mal por u<ex>os</ex> eu no<ex>n</ex> mentir</rdg>
         </app>
      </l>
      <l n="3">
