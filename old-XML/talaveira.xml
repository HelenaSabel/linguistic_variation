<root>
    <div type="poem" corresp="#A242B430V42">
      <l n="1">
         <app>
            <rdg wit="#A"><gap reason="unfinished"/>vito ando triste no meu coracon</rdg>
            <rdg wit="#B">Muyt' ando triste no meu coraçon</rdg>
            <rdg wit="#V">Muyt' ando triste no meu corazon</rdg>
         </app>
      </l>
      <l n="2">
         <app>
            <rdg wit="#A">porque sei que m' ei mui çed' a quitar</rdg>
            <rdg wit="#B">porque sey que m' ey muj ced' a quytar</rdg>
            <rdg wit="#V">porque sey que m' ey mui ced' a quytar</rdg>
         </app>
      </l>
      <l n="3">
         <app>
            <rdg wit="#A">de uos sennor e yr alur morar</rdg>
            <rdg wit="#B">de uos senh<ex>or</ex> e hir alhur morar</rdg>
            <rdg wit="#V">de uos senh<ex>or</ex> e hir alhur morar</rdg>
         </app>
      </l>
      <l n="4">
         <app>
            <rdg wit="#A">e_pesar_mia en si deus me perdon</rdg>
            <rdg wit="#B">epesar_mha eu se de<ex>us</ex> mj p<ex>er</ex>don</rdg>
            <rdg wit="#V">epesarmha en se de<ex>us</ex> mi perdon</rdg>
         </app>
      </l>
      <l n="5">
         <app>
            <rdg wit="#A"><gap reason="unfinished"/>e me partir de uos per nulla ren</rdg>
            <rdg wit="#B">De me partir de uos per nulha ren</rdg>
            <rdg wit="#V">de me partir de uos per nulha ren</rdg>
         </app>
      </l>
      <l n="6">
         <app>
            <rdg wit="#A">e yr morar allur sen uosso ben</rdg>
            <rdg wit="#B">e hir morar alhur sen uosso ben</rdg>
            <rdg wit="#V"><gap reason="error"/> hir morar alhur se uosso ben</rdg>
         </app>
      </l>
      <l n="7">
         <app>
            <rdg wit="#A"><hi rend="guide">P</hi>orque sei que ei tal coita soffrer</rdg>
            <rdg wit="#B">Porq<ex>ue</ex> sei q<ex>ue</ex> ey tal coita sofrer</rdg>
            <rdg wit="#V">Porq<ex>ue</ex> sei q<ex>ue</ex> ey tal coita sofrer</rdg>
         </app>
      </l>
      <l n="8">
         <app>
            <rdg wit="#A">qual soffri ia outra uez mia sen<ex>n</ex>or</rdg>
            <rdg wit="#B">q<ex>ua</ex>l sofri ia out<ex>ra</ex> nez mha senhor</rdg>
            <rdg wit="#V">q<ex>ua</ex>l sofri ia out<ex>ra</ex> uez mha senhor</rdg>
         </app>
      </l>
      <l n="9">
         <app>
            <rdg wit="#A">e no<ex>n</ex> auera y al pois eu for</rdg>
            <rdg wit="#B">e no<ex>n</ex> au<ex>er</ex>a hy al pois eu for</rdg>
            <rdg wit="#V">e no<ex>n</ex> au<ex>er</ex>a hy al poys eu for</rdg>
         </app>
      </l>
      <l n="10">
         <app>
            <rdg wit="#A">que non aia gran pesar a prender</rdg>
            <rdg wit="#B">q<ex>ue</ex> no<ex>n</ex> aia g<ex>ra</ex>n pesar a p<ex>re</ex>nder</rdg>
            <rdg wit="#V">q<ex>ue</ex> no<ex>n</ex> aia g<ex>ra</ex>m pesar a p<ex>re</ex>nder</rdg>
         </app>
      </l>
      <l n="11">
         <app>
            <rdg wit="#A"><hi rend="guide">D</hi>e me p<ex>er</ex>tir de uos p<ex>er</ex>_nulla_ren</rdg>
            <rdg wit="#B">De me p<ex>ar</ex>tir de uos <gap reason="economy"/></rdg>
            <rdg wit="#V">de me p<ex>ar</ex>tir de uos <gap reason="economy"/></rdg>
         </app>
      </l>
      <l n="12">
         <app>
            <rdg wit="#A"><gap reason="economy"/></rdg>
            <rdg wit="#B"><gap reason="economy"/></rdg>
            <rdg wit="#V"><gap reason="economy"/></rdg>
         </app>
      </l>
      <l n="13">
         <app>
            <rdg wit="#A"><hi rend="guide">C</hi>a mi aue<ex>ẽ</ex>o assi outra uez ia</rdg>
            <rdg wit="#B">Ca mh au<ex>ẽ</ex>o assy out<ex>ra</ex> uez ia</rdg>
            <rdg wit="#V">Ca mb au<ex>ẽ</ex>o assy out<ex>ra</ex> uez ia</rdg>
         </app>
      </l>
      <l n="14">
         <app>
            <rdg wit="#A">mia sen<ex>n</ex>or fremosa q<ex>ue</ex> me q<ex>u</ex>itey</rdg>
            <rdg wit="#B">mha senh<ex>or</ex> f<ex>re</ex>mosa q<ex>ue</ex> me q<ex>ui</ex>cey</rdg>
            <rdg wit="#V">ma senh<ex>or</ex> f<ex>re</ex>mosa q<ex>ue</ex> me q<ex>ui</ex>tey</rdg>
         </app>
      </l>
      <l n="15">
         <app>
            <rdg wit="#A">de uos e sen meu grad allur morei</rdg>
            <rdg wit="#B">de uos e se<ex>n</ex> meu grad' alhur morey</rdg>
            <rdg wit="#V">de uos e sem meu grad' alhur morey</rdg>
         </app>
      </l>
      <l n="16">
         <app>
            <rdg wit="#A">mais este mui gran pesar me sera</rdg>
            <rdg wit="#B">mays este mui g<ex>ra</ex>m pesar mj sera</rdg>
            <rdg wit="#V">mays este muj g<ex>ra</ex>m pesar mi sera</rdg>
         </app>
      </l>
      <l n="17">
         <app>
            <rdg wit="#A"><hi rend="guide">D</hi>e me partir de uos p<ex>er</ex>_nulla_ren</rdg>
            <rdg wit="#B">De me p<ex>ar</ex>tir de uos <gap reason="economy"/></rdg>
            <rdg wit="#V">de me partir de uos <gap reason="economy"/></rdg>
         </app>
      </l>
      <l n="18">
         <app>
            <rdg wit="#A"><gap reason="economy"/></rdg>
            <rdg wit="#B"><gap reason="economy"/></rdg>
            <rdg wit="#V"><gap reason="economy"/></rdg>
         </app>
      </l>
      <l n="19">
         <app>
            <rdg wit="#A"><hi rend="guide">E</hi> quando meu de uos partir por en</rdg>
            <rdg wit="#B">E quando meu de uos p<ex>ar</ex>tir p<ex>or</ex> en</rdg>
            <rdg wit="#V">E quando meu de uos p<ex>ar</ex>tir p<ex>or</ex> en</rdg>
         </app>
      </l>
      <l n="20">
         <app>
            <rdg wit="#A">ou morrerei ou perderei o sen</rdg>
            <rdg wit="#B">ou moirerey ou p<ex>er</ex>d<ex>er</ex>ey o sen</rdg>
            <rdg wit="#V">ou morrerey ou perdey o sen</rdg>
         </app>
      </l>
   </div>
   <div type="poem" corresp="#A243B431V43">
      <l n="1">
         <app>
            <rdg wit="#A"><hi rend="guide">P</hi>arti_meu de uos mia sennor</rdg>
            <rdg wit="#B">Partir_men de nos mha seuh<ex>or</ex></rdg>
            <rdg wit="#V">Partir_meu de uos ma senh<ex>or</ex></rdg>
         </app>
      </l>
      <l n="2">
         <app>
            <rdg wit="#A">sen meu grad <ex>ũ</ex>a uez aqui</rdg>
            <rdg wit="#B">sen meu grado hunha uez aqui</rdg>
            <rdg wit="#V">sen meu grado hunha uez aqui</rdg>
         </app>
      </l>
      <l n="3">
         <app>
            <rdg wit="#A">e na terra u eu uiui</rdg>
            <rdg wit="#B">e na terra hu eu uiuj</rdg>
            <rdg wit="#V">e na terra hu eu uiui</rdg>
         </app>
      </l>
      <l n="4">
         <app>
            <rdg wit="#A">andei sempre tan sen sabor</rdg>
            <rdg wit="#B">andey sempre tam sen sabor</rdg>
            <rdg wit="#V">andey sempre tan sen sabor</rdg>
         </app>
      </l>
      <l n="5">
         <app>
            <rdg wit="#A"><hi rend="guide">Q</hi>ue nunca eu pude ueer</rdg>
            <rdg wit="#B">Que nu<ex>n</ex>ca eu pudi ueer</rdg>
            <rdg wit="#V">que nunca eu pudi ueer</rdg>
         </app>
      </l>
      <l n="6">
         <app>
            <rdg wit="#A">de rem u uos non ui prazer</rdg>
            <rdg wit="#B">de rem hu u<ex>os</ex> non ui prazer</rdg>
            <rdg wit="#V">de rem hu u<ex>os</ex> no<ex>n</ex> ui prazer</rdg>
         </app>
      </l>
      <l n="7">
         <app>
            <rdg wit="#A"><hi rend="guide">N</hi>a terra u me fez morar</rdg>
            <rdg wit="#B">Da c<ex>er</ex>ira hu me fez morar</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="8">
         <app>
            <rdg wit="#A">muito sen uos mia se<ex>n</ex>nor d<ex>eu</ex>s</rdg>
            <rdg wit="#B">muyto sen uos mha senh<ex>or</ex> d<ex>eu</ex>s</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="9">
         <app>
            <rdg wit="#A">fez me chorar dos ollos meus</rdg>
            <rdg wit="#B">fez me chorar d<ex>o</ex> olh<ex>os</ex> me<ex>us</ex></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="10">
         <app>
            <rdg wit="#A">e fez me tan coitad' andar</rdg>
            <rdg wit="#B">e fez me ta<ex>n</ex> cuytad' andar</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="11">
         <app>
            <rdg wit="#A"><hi rend="guide">Q</hi>ue nunca desque_me._d._q.</rdg>
            <rdg wit="#B">Que nu<ex>n</ex>ca eu_<gap reason="economy"/></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="12">
         <app>
            <rdg wit="#A"><gap reason="economy"/></rdg>
            <rdg wit="#B"><gap reason="economy"/></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="13">
         <app>
            <rdg wit="#A"><hi rend="guide">E</hi> des que m' eu de uos quitei</rdg>
            <rdg wit="#B">E des q<ex>ue</ex> m' eu de uos q<ex>ui</ex>tey</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="14">
         <app>
            <rdg wit="#A">fezo me sempr' auer de pran</rdg>
            <rdg wit="#B">fez me semp<ex>re</ex> au<ex>er</ex> de pra<ex>n</ex></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="15">
         <app>
            <rdg wit="#A">n<ex>ost</ex>ro sen<ex>n</ex>or mui grand' affan</rdg>
            <rdg wit="#B">n<ex>ost</ex>ro senh<ex>or</ex> muj g<ex>ra</ex>nd' afam</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="16">
         <app>
            <rdg wit="#A">e sempre tan coitad andei</rdg>
            <rdg wit="#B">e semp<ex>re</ex> muy cuytad' andey</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="17">
         <app>
            <rdg wit="#A"><hi rend="guide">Q</hi>ue nunca eu pude_ueer</rdg>
            <rdg wit="#B">Que nu<ex>n</ex>ca eu <gap reason="economy"/></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="18">
         <app>
            <rdg wit="#A"><gap reason="economy"/></rdg>
            <rdg wit="#B"><gap reason="economy"/></rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="19">
         <app>
            <rdg wit="#A"><hi rend="guide">E</hi> non poderia prazer</rdg>
            <rdg wit="#B">E no<ex>n</ex> pod<ex>er</ex>ia p<ex>ra</ex>zer</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
      <l n="20">
         <app>
            <rdg wit="#A">u eu uos non uisse ueer</rdg>
            <rdg wit="#B">hu eu uos no<ex>n</ex> uisse ueer</rdg>
            <rdg wit="#V"/>
         </app>
      </l>
   </div>
   <div type="poem" corresp="#A245B433V45">
      <l n="1">
         <app>
            <rdg wit="#A"><hi rend="guide">E</hi>stes que ora dizen mia sennor</rdg>
            <rdg wit="#B">Estes que ora dizem mha senh<ex>or</ex></rdg>
            <rdg wit="#V">Este<add rend="sup">s</add><del rend="stroke"/> que ora dizem mha senh<ex>or</ex></rdg>
         </app>
      </l>
      <l n="2">
         <app>
            <rdg wit="#A">que saben ca uus quer' eu muy gran ben</rdg>
            <rdg wit="#B">que sabem ca u<ex>os</ex> quer' eu muj gra<ex>n</ex> ben</rdg>
            <rdg wit="#V">que sabem ca u<ex>os</ex> quer' eu mui gra<ex>n</ex> ben</rdg>
         </app>
      </l>
      <l n="3">
         <app>
            <rdg wit="#A">pois en nunca per mi souberen ren</rdg>
            <rdg wit="#B">poys en nunca per mj<ex>n</ex> souberou rem</rdg>
            <rdg wit="#V">poys en nunca per mi<ex>n</ex> soubero<ex>n</ex> rem</rdg>
         </app>
      </l>
      <l n="4">
         <app>
            <rdg wit="#A">querri<del>a</del>' agora seer sabedor</rdg>
            <rdg wit="#B">queri' agora seer sabedor</rdg>
            <rdg wit="#V">queri' agora seer sabedor</rdg>
         </app>
      </l>
      <l n="5">
         <app>
            <rdg wit="#A"><del><gap reason="unfinished"/></del>er quen o poderon eles saber</rdg>
            <rdg wit="#B">Per quem o poderom eles saber</rdg>
            <rdg wit="#V">per quem o poderom eles saber</rdg>
         </app>
      </l>
      <l n="6">
         <app>
            <rdg wit="#A">pois mi o uos nunca quisestes creer</rdg>
            <rdg wit="#B">poys mh o uos nunca quisestes creer</rdg>
            <rdg wit="#V">poys mh o uos nunca quisestes creer</rdg>
         </app>
      </l>
      <l n="7">
         <app>
            <rdg wit="#A"><hi rend="guide">C</hi>a mia se<ex>n</ex>nor semp<ex>re</ex> o eu neguei</rdg>
            <rdg wit="#B">Ca mha senhor senp<ex>re</ex> o eu neguey</rdg>
            <rdg wit="#V">Ca mha senhor senp<ex>re</ex> o eu neguey</rdg>
         </app>
      </l>
      <l n="8">
         <app>
            <rdg wit="#A">q<ex>ua</ex>nt' eu mais pude assi d<ex>eu</ex>s me p<ex>er</ex>don</rdg>
            <rdg wit="#B">qua<ex>n</ex>t' eu mays pud' assy d<ex>eu</ex> mj p<ex>er</ex>don</rdg>
            <rdg wit="#V">qua<ex>n</ex>t' eu mays pud' assy d<ex>eu</ex>s mi p<ex>er</ex>don</rdg>
         </app>
      </l>
      <l n="9">
         <app>
            <rdg wit="#A">e dizen ora quantos aqui son</rdg>
            <rdg wit="#B">e dizem ora qua<ex>n</ex>t<ex>os</ex> aq<ex>ui</ex> son</rdg>
            <rdg wit="#V">e dizen ora qua<ex>n</ex>t<ex>os</ex> aq<ex>ui</ex> son</rdg>
         </app>
      </l>
      <l n="10">
         <app>
            <rdg wit="#A">que o saben mais como saberei</rdg>
            <rdg wit="#B">q<ex>ue</ex> sab_q<ex>ue</ex> sabem mays como saberey</rdg>
            <rdg wit="#V">q<ex>ue</ex> <add rend="?">e</add> sabem mays como saberey</rdg>
         </app>
      </l>
      <l n="11">
         <app>
            <rdg wit="#A"><hi rend="guide">P</hi>er que no poderon eles saber</rdg>
            <rdg wit="#B">Per q<ex>ue</ex> o p<ex>er</ex>od<ex>er</ex>om eles saber</rdg>
            <rdg wit="#V">per q<ex>ue</ex> o p<ex>er</ex>od<ex>er</ex>om eles saber</rdg>
         </app>
      </l>
      <l n="12">
         <app>
            <rdg wit="#A"><gap reason="economy"/></rdg>
            <rdg wit="#B"><gap reason="economy"/></rdg>
            <rdg wit="#V"><gap reason="economy"/></rdg>
         </app>
      </l>
   </div>
