            <rdg wit="#A"><choice><orig>fara*mela</orig><reg>fara-m'*ela</reg></choice> <choice><orig>dessi</orig><reg>de*si</reg></choice> partir</rdg>
            <rdg wit="#B"><choice><orig>faramela</orig><reg>fara-m'*ela</reg></choice> <choice><orig>dessy</orig><reg>de*sy</reg></choice> p<ex>ar</ex>tir</rdg>
         </app>
      </l>
      <l n="21">
