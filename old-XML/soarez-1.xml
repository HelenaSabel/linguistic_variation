            <rdg wit="#A">mui trist' e muit' a meu pesar</rdg>
            <rdg wit="#B">muj t<ex>ri</ex>ste muyt' a meu pesar</rdg>
         </app>
      </l>
      <l n="22">
         <app>
            <rdg wit="#A">Por en no<ex>n</ex> deuia quitar</rdg>
            <rdg wit="#B">Por en no<ex>n</ex> deuia aq<ex>ui</ex>tar</rdg>
