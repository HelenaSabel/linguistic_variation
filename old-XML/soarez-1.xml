            <rdg wit="#A">por q<ex>ue</ex> os quitaria d' y</rdg>
            <rdg wit="#B">p<ex>or</ex> q<ex>ue</ex> os q<ex>ui</ex>taria d' i</rdg>
         </app>
      </l>
      <l n="27">
         <app>
            <rdg wit="#A">por tal cuit' auer come mi</rdg>
            <rdg wit="#B">p<ex>or</ex> tal coyta au<ex>er</ex> com<ex>e</ex> mj<ex>n</ex></rdg>
