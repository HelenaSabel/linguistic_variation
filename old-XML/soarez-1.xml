   <div type="poem" corresp="#A41B153">
      <l n="1">
         <app>
            <rdg wit="#A">Qual sennor deuia fillar</rdg>
            <rdg wit="#B">Qual senhor deuia filhar</rdg>
         </app>
      </l>
      <l n="2">
         <app>
            <rdg wit="#A">quen a ben soubesse escoller</rdg>
            <rdg wit="#B">quen a ben soubess' escolher</rdg>
         </app>
      </l>
      <l n="3">
         <app>
            <rdg wit="#A">essa faz a min de<ex>us</ex> amar</rdg>
            <rdg wit="#B">essa faz a mj de<ex>us</ex> amar</rdg>
         </app>
      </l>
      <l n="4">
         <app>
            <rdg wit="#A">e*essa*me*tem*en*poder</rdg>
            <rdg wit="#B">
               <gap reason="unknown"/>
           </rdg>
         </app>
      </l>
      <l n="5">
         <app>
            <rdg wit="#A">e essa est a mia se<ex>n</ex>nor</rdg>
            <rdg wit="#B">e essa est a mha senhor</rdg>
         </app>
      </l>
      <l n="6">
         <app>
            <rdg wit="#A">e essa me faz o mayor</rdg>
            <rdg wit="#B">e essa mj faz o mayor</rdg>
         </app>
      </l>
      <l n="7">
         <app>
            <rdg wit="#A">ben deste mundo deseiar</rdg>
            <rdg wit="#B">ben deste mu<ex>n</ex>do deseiar</rdg>
         </app>
      </l>
      <l n="8">
         <app>
            <rdg wit="#A">O seu ben que no<ex>n</ex> ay par</rdg>
            <rdg wit="#B">O seu be<ex>n</ex> q<ex>ue</ex> no<ex>n</ex> ahi par</rdg>
         </app>
      </l>
      <l n="9">
         <app>
            <rdg wit="#A">tan muito a faz d<ex>eu</ex>s ualer</rdg>
            <rdg wit="#B">tam muyto a faz d<ex>eu</ex>s ualer</rdg>
         </app>
      </l>
      <l n="10">
         <app>
            <rdg wit="#A">por bon prez <ex>e</ex> por ben falar</rdg>
            <rdg wit="#B">p<ex>or</ex> bo<ex>n</ex> p<ex>ra</ex>z e p<ex>er</ex> bo<ex>n</ex> falar</rdg>
         </app>
      </l>
      <l n="11">
         <app>
            <rdg wit="#A">per bon <gap reason="error"/> <ex>e</ex> p<ex>er</ex> pareçer</rdg>
            <rdg wit="#B">p<ex>er</ex> bo<ex>n</ex> sen e p<ex>er</ex> parecer</rdg>
         </app>
      </l>
      <l n="12">
         <app>
            <rdg wit="#A">
               <ex>e</ex> d'*atal dona o seu ben</rdg>
            <rdg wit="#B">e de*'tal dona o seu ben</rdg>
         </app>
      </l>
      <l n="13">
         <app>
            <rdg wit="#A">no<ex>n</ex> sey og' eu no mu<ex>n</ex>do que<ex>n</ex>
           </rdg>
            <rdg wit="#B">no<ex>n</ex> sey oi' eu no mu<ex>n</ex>do que<ex>n</ex></rdg>
         </app>
      </l>
      <l n="14">
         <app>
            <rdg wit="#A">o podesse saber osmar</rdg>
            <rdg wit="#B">o podesse sabere osmar</rdg>
         </app>
      </l>
      <l n="15">
         <app>
            <rdg wit="#A">Nen a mia coit' a meu coidar</rdg>
            <rdg wit="#B">Nen a mha coyt' a <gap/> cuydar</rdg>
         </app>
      </l>
      <l n="16">
         <app>
            <rdg wit="#A">en que m' og' eu ueio uiuer</rdg>
            <rdg wit="#B">en q<ex>ue</ex> m' oi' eu ueio uiuer</rdg>
         </app>
      </l>
      <l n="17">
         <app>
            <rdg wit="#A">ca m' ei datal dona guardar</rdg>
            <rdg wit="#B">ca m' ei de*tal dona guardar</rdg>
         </app>
      </l>
      <l n="18">
         <app>
            <rdg wit="#A">de qual mi or' oystes dizer</rdg>
            <rdg wit="#B">de q<ex>ua</ex>l mh ora oystes diz<ex>er</ex></rdg>
         </app>
      </l>
      <l n="19">
         <app>
            <rdg wit="#A">de a ueer ca se a uir</rdg>
            <rdg wit="#B">de a ueer ca se a uir</rdg>
         </app>
      </l>
      <l n="20">
