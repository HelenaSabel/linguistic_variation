<tei>
    <div type="poem" corresp="#A111B224">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A">De uos sennor querria eu saber</rdg>
                    <rdg wit="#B">De uos senhor querria eu saber</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">pois deseiades mia mort a ue<add place="above">e</add>r</rdg>
                    <rdg wit="#B">poys deseiades mha mort a ueer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">e eu non moir' e querria morrer</rdg>
                    <rdg wit="#B">e eu non moyr' e queria morrer</rdg>
                </app>
            </l>
            <l n="4" type="refrao">
                <app>
                    <rdg wit="#A">que me digades que farey eu y</rdg>
                    <rdg wit="#B">que mj digades que farey eu hi</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="5">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>on mia mort me seria gran ben</rdg>
                    <rdg wit="#B">Con mha morte mj seria g<am>&#x1DD3;</am><ex>ra</ex>m be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">porque sei ca u<am>&#42863;</am><ex>os</ex> prazeria en</rdg>
                    <rdg wit="#B">p<am>&#7651;</am><ex>or</ex>q<am>&#772;</am><ex>ue</ex> sey ca u<am>&#42863;</am><ex>os</ex> p<am>&#x1DD3;</am><ex>ra</ex>zeria en</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">e pois non moiro uenn' a uos por en</rdg>
                    <rdg wit="#B">e pois no<am>&#772;</am><ex>n</ex> moyro uenh' a uos p<am>&#7651;</am><ex>or</ex> en</rdg>
                </app>
            </l>
            <l n="8" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">Q</hi>ue me digades que farei eu y</rdg>
                    <rdg wit="#B">Que mj digades q<am>&#772;</am><ex>ue</ex> farey eu hi</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="9">
                <app>
                    <rdg wit="#A"><hi rend="guide">P</hi>or mia morte que u<am>&#42863;</am><ex>os</ex> ui deseiar</rdg>
                    <rdg wit="#B">Por mha morte q<am>&#772;</am><ex>ue</ex>u<am>&#42863;</am><ex>os</ex> uj deseiar</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">rogu' eu a d<am>&#x035B;</am><ex>eu</ex>s sempr'_e non mi a q<am>&#772;</am><ex>ue</ex>r dar</rdg>
                    <rdg wit="#B">rogu' eu a d<am>&#x035B;</am><ex>eu</ex>s semp<am>&#x1DD3;</am><ex>r' e</ex> no<am>&#772;</am><ex>n</ex> mh a q<am>&#772;</am><ex>ue</ex>r dar</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">e <choice><orig>uenno u<am>&#42863;</am><ex>os</ex></orig><reg>uenno-u<am>&#42863;</am><ex>os</ex></reg></choice> mia se<am>&#772;</am><ex>n</ex>nor p<am>&#772;</am><ex>re</ex>guntar</rdg>
                    <rdg wit="#B">e uenha_nos mha senhor p<am>&#x1DD3;</am><ex>re</ex>gu<am>&#772;</am><ex>n</ex>tar</rdg>
                </app>
            </l>
            <l n="12" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">Q</hi>ue me digades que farei eu y</rdg>
                    <rdg wit="#B">Que mj digades q<am>&#772;</am><ex>ue</ex> farei eu hi</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="13">
                <app>
                    <rdg wit="#A"><hi rend="guide">P</hi>or mia morte roguei d<am>&#x035B;</am><ex>eu</ex>s e amor</rdg>
                    <rdg wit="#B">Por mha morte roguei d<am>&#x035B;</am><ex>eu</ex>s e amor</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">e no<am>&#772;</am><ex>n</ex> mi a dan por me fazer peor</rdg>
                    <rdg wit="#B">e no<am>&#772;</am><ex>n</ex> mh a deu p<am>&#7651;</am><ex>or</ex> mi fazer peor</rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">estar conuosqu' e uenn' a uos se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">estar <am>ꝯ</am><ex>con</ex>uosq<am>&#772;</am><ex>u' e</ex> uenh' a uos senhor</rdg>
                </app>
            </l>
            <l n="16" type="refrao">
                <app>
                    <rdg wit="#A">que me digades que farei eu_y</rdg>
                    <rdg wit="#B">Que mj digades q<am>&#772;</am><ex>ue</ex> farei <gap reason="economy"/></rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A112B225">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>on me queredes mia sennor</rdg>
                    <rdg wit="#B">Non mj queredes mha senhor</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">fazer ben. enquant' eu uiuer</rdg>
                    <rdg wit="#B">fazer ben enqua<am>&#772;</am><ex>n</ex>t' eu uiuer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">e pois eu por uos morto for</rdg>
                    <rdg wit="#B">e poys eu por uos morto for</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">no<am>&#772;</am><ex>n</ex> mi o podedes fazer</rdg>
                    <rdg wit="#B">non mh o poderedes fazer</rdg>
                </app>
            </l>
            <l n="5" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>a non ui eu quen fezesse</rdg>
                    <rdg wit="#B">Ca non uj eu quen fezesse</rdg>
                </app>
            </l>
            <l n="6" type="refrao">
                <app>
                    <rdg wit="#A">nunca ben se non podesse</rdg>
                    <rdg wit="#B">nunca ben se non podesse</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="7">
                <app>
                    <rdg wit="#A"><hi rend="guide">P</hi>odedes u<am>&#42863;</am><ex>os</ex> nenbrar ben leu</rdg>
                    <rdg wit="#B">Podedes uos ne<am>&#772;</am><ex>n</ex>brar ben lheu</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">de min que soffro muito mal</rdg>
                    <rdg wit="#B">de mj<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex> sofro muyto mal</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">por uos. e <choice><orig>digo uol</orig><reg>digo-uo-l'</reg></choice> ant' eu</rdg>
                    <rdg wit="#B">p<am>&#7651;</am><ex>or</ex> uos e <choice><orig>digouol</orig><reg>digo-uo-l'</reg></choice> ant' eu</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">que pois me non faredes al</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> pois me no<am>&#772;</am><ex>n</ex> faredes al</rdg>
                </app>
            </l>
            <l n="11" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a non ui eu quen fezesse</rdg>
                    <rdg wit="#B">Ca non uj eu <gap reason="economy"/></rdg>
                </app>
            </l>
            <l n="12" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="economy"/></rdg>
                    <rdg wit="#B"><gap reason="economy"/></rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="13">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">P</hi>odedes u<am>&#42863;</am><ex>os</ex></orig><reg><hi rend="guide">P</hi>odedes-u<am>&#42863;</am><ex>os</ex></reg></choice> nenbrar de min</rdg>
                    <rdg wit="#B"><choice><orig>Podedesu<am>&#42863;</am><ex>os</ex></orig><reg>Podedes-u<am>&#42863;</am><ex>os</ex></reg></choice> ne<am>&#772;</am><ex>n</ex>brar de mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">depois mia morte sen al ren</rdg>
                    <rdg wit="#B">depoys mha morte sen al re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">e_se eu faça bo<am>&#772;</am><ex>&#771;</ex>a fin</rdg>
                    <rdg wit="#B">assy eu faça bo<am>&#772;</am><ex>&#771;</ex>a fin</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">non me faredes outro ben</rdg>
                    <rdg wit="#B">mo<am>&#772;</am><ex>n</ex> mj faredes out<am>&#870;</am><ex>ro</ex> be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="17" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a non ui eu quen fezesse</rdg>
                    <rdg wit="#B">Ca no<am>&#772;</am><ex>n</ex> uj eu q<am>&#772;&#x0321;</am><ex>uen</ex> fezesse</rdg>
                </app>
            </l>
            <l n="18" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="economy"/></rdg>
                    <rdg wit="#B"><gap reason="economy"/></rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
           
            <l n="19">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">F</hi>azede mi</orig></choice> e <choice><orig>graçir uolei</orig></choice></rdg>
                    <rdg wit="#B"><choice><orig>Faze demj</orig></choice> ben e <choice><orig>graçiruoloey</orig></choice></rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">ben mentr' ando uiuo ca no<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B">_ mentr' ando uiuo ca non</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">mi o faredes eu ben o sei</rdg>
                    <rdg wit="#B">mh o faredes eu ben o sey</rdg>
                </app>
            </l>
            <l n="22">
                <app>
                    <rdg wit="#A">pois eu morrer por tal razo<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B">pois eu morrer p<am>&#7651;</am><ex>or</ex> tal razon</rdg>
                </app>
            </l>
            <l n="23" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a no<am>&#772;</am><ex>n</ex> ui. eu. <gap reason="economy"/></rdg>
                    <rdg wit="#B">Ca no<am>&#772;</am><ex>n</ex> uj eu <gap reason="economy"/></rdg>
                </app>
            </l>
            <l n="24" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="economy"/></rdg>
                    <rdg wit="#B"><gap reason="economy"/></rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A113B226">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">R</hi>ogaria eu mia sennor</rdg>
                    <rdg wit="#B">Rogaria eu mha senhor</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">por deus que me fezesse ben</rdg>
                    <rdg wit="#B">por de<am>&#42863;</am><ex>us</ex> que mj fezesse ben</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">mais ei dela tan gran pauor</rdg>
                    <rdg wit="#B">mays ey dela ta<am>&#772;</am><ex>n</ex> gram pauor</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">que lle non ouso falar ren</rdg>
                    <rdg wit="#B">que lhi non ouso falar ren</rdg>
                </app>
            </l>
            <l n="5" type="refrao">
                <app>
                    <rdg wit="#A">con medo de se m' assannar</rdg>
                    <rdg wit="#B">con medo de xi m' assanhar</rdg>
                </app>
            </l>
            <l n="6" type="refrao">
                <app>
                    <rdg wit="#A">e me non que querer pois falar</rdg>
                    <rdg wit="#B">e mj non _ qerer depoys falar</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="7">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">D</hi>irialleu</orig></choice> de coraçon</rdg>
                    <rdg wit="#B"><choice><orig>Dirialheu</orig></choice> de coraço</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">como me faz p<am>̲</am><ex>er</ex>der_o sen</rdg>
                    <rdg wit="#B">como mj faz p<am>̲</am><ex>er</ex>delo sen</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">o seu bon pareçer mais no<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B">o seu bo<am>&#772;</am><ex>n</ex> parecer mays no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">ous'e todo aquesto mi auen</rdg>
                    <rdg wit="#B">ousei tod' aq<am>&#772;</am><ex>ue</ex>st'a mj<am>&#772;</am><ex>n</ex> auen</rdg>
                </app>
            </l>
            <l n="11" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>on medo de_se_mi_assannar</rdg>
                    <rdg wit="#B">Con medo <gap reason="economy"/></rdg>
                </app>
            </l>
            <l n="12" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="economy"/></rdg>
                    <rdg wit="#B"><gap reason="economy"/></rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="13">
                <app>
                    <rdg wit="#A"><hi rend="guide">P</hi>ois me d<am>&#x035B;</am><ex>eu</ex>s tal uentura deu</rdg>
                    <rdg wit="#B">Pois me d<am>&#x035B;</am><ex>eu</ex>s tal uent<am>ᵉ</am><ex>ur</ex>a deu</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">que m' en tamanna coita ten</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> m' en tamanha coita re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">amor. ia senpr'eu ia serei seu</rdg>
                    <rdg wit="#B">amor ia senp<am>&#x1DD3;</am><ex>re</ex> _ serey seu</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">mais non a rogarei por_en</rdg>
                    <rdg wit="#B">mays non a rogarey _</rdg>
                </app>
            </l>
            <l n="17" type="refrao">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>on medo de_se_m'_assannar</rdg>
                    <rdg wit="#B">Con medo <gap reason="economy"/></rdg>
                </app>
            </l>
            <l n="18" type="refrao">
                <app>
                    <rdg wit="#A"><gap reason="economy"/></rdg>
                    <rdg wit="#B"><gap reason="economy"/></rdg>
