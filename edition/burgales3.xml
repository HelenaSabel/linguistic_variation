<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem" corresp="#A95B202">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><gap reason="damage"/></rdg>
                    <rdg wit="#B">Por_muy_coytado_per_tenh'_eu</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A"><gap reason="damage"/>uay_querer_ben_tal._moller</rdg>
                    <rdg wit="#B">quen_uay_querer_ben_atal_molher</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">que seu seruiço non lle quer</rdg>
                    <rdg wit="#B">que seu seruiço non lhi quer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">per nulla guisa gradeçer</rdg>
                    <rdg wit="#B">per nulha guisa gradecer</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">e mal pecad' assi uiu' eu</rdg>
                    <rdg wit="#B">e mal peccad' assi uiu' eu</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">cuitad' e que demo mi<del rend="underdot">o</del> deu</rdg>
                    <rdg wit="#B">coytad' e que demo mj deu</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">cuita pola nunca perder</rdg>
                    <rdg wit="#B">coyta pola no<am>&#772;</am><ex>n</ex> perder</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">N</hi>on por al senon polo seu</rdg>
                    <rdg wit="#B">Non p<am>&#7651;</am><ex>or</ex> al seno<am>&#772;</am><ex>n</ex> polo seu</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">bon pareçer da mia se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">bo<am>&#772;</am><ex>n</ex> parecer da mha senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">que nunca ome <del rend="scrape"><gap reason="unknown"/></del> mellor</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> nu<am>&#772;</am><ex>n</ex>ca home uyo melh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A"><del rend="scrape"><gap reason="unknown"/></del> tal se d<am>&#x035B;</am><ex>eu</ex>s me leix' auer</rdg>
                    <rdg wit="#B">ne<am>&#772;</am><ex>n</ex> tal se d<am>&#x035B;</am><ex>eu</ex>s mj leix' au<am>&#x035B;</am><ex>er</ex></rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">dela ben. e me mostr' o_seu</rdg>
                    <rdg wit="#B">dela be<am>&#772;</am><ex>n</ex> e mj mo<am>&#772;</am><ex>n</ex>str' osseu</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">bon pareçer que lle d<am>&#x035B;</am><ex>eu</ex>s deu</rdg>
                    <rdg wit="#B">bo<am>&#772;</am><ex>n</ex> parecer q<am>&#772;</am><ex>ue</ex> lhi d<am>&#x035B;</am><ex>eu</ex>s deu</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">por ia senpr'_a mi<am>&#772;</am><ex>n</ex> mal fazer</rdg>
                    <rdg wit="#B">p<am>&#7651;</am><ex>or</ex> ia semp<am>&#x1DD3;</am><ex>r' a</ex> mj mal fazer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a <choice><orig>deu la</orig><reg>deu-la</reg></choice> fez por mal de min</rdg>
                    <rdg wit="#B">Ca <choice><orig>deula</orig><reg>deu-la</reg></choice> fez p<am>&#7651;</am><ex>or</ex> mal de mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">mais fremosa de quantas son</rdg>
                    <rdg wit="#B">mays f<am>&#x035B;</am><ex>re</ex>mosa de q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>tas son</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">no mundo si d<am>&#x035B;</am><ex>eu</ex>s me p<am>̲</am><ex>er</ex>don</rdg>
                    <rdg wit="#B">no mu<am>&#772;</am><ex>n</ex>do sse d<am>&#x035B;</am><ex>eu</ex>s mj p<am>̲</am><ex>er</ex>don</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">e uedes que miar fez por en</rdg>
                    <rdg wit="#B">e uedes q<am>&#772;</am><ex>ue</ex> m' ar fez p<am>&#7651;</am><ex>o</ex>r e<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A"><choice><orig>fez mia</orig><reg>fez-mi-a</reg></choice> ueer por mal de mi<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B"><choice><orig>fez mha</orig><reg>fez-mh-a</reg></choice> ueer p<am>&#7651;</am><ex>or</ex> mal de mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">ca non por al ca <choice><orig>poila</orig><reg>poi-la</reg></choice> ui</rdg>
                    <rdg wit="#B">ca no<am>&#772;</am><ex>n</ex> p<am>&#7651;</am><ex>or</ex> al ca <choice><orig>poila</orig><reg>poi-la</reg></choice> uj</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">nunca m' ar paguei doutra ren</rdg>
                    <rdg wit="#B">nu<am>&#772;</am><ex>n</ex>ca m' ar paguey dout<am>&#x1DD3;</am><ex>ra</ex> re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="22">
                <app>
                    <rdg wit="#A"><hi rend="guide">S</hi>enon dela de que assi</rdg>
                    <rdg wit="#B">Seno<am>&#772;</am><ex>n</ex> dela de q<am>&#772;</am><ex>ue</ex> assy</rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A">estou como u<am>&#42863;</am><ex>os</ex> eu direi</rdg>
                    <rdg wit="#B">estou como u<am>&#42863;</am><ex>os</ex> <gap reason="error"/> direy</rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">que todo quant' auer cuidei</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> todo qua<am>&#772;</am><ex>n</ex>t' au<am>&#x035B;</am><ex>er</ex> cuidei</rdg>
                </app>
            </l>
            <l n="25">
                <app>
                    <rdg wit="#A">dela <choice><orig>poila</orig><reg>poi-la</reg></choice> ui. <choice><orig>eyo</orig><reg>ey-o</reg></choice> en</rdg>
                    <rdg wit="#B">dela <choice><orig>poila</orig><reg>poi-la</reg></choice> uj <choice><orig>eyo</orig><reg>ey-o</reg></choice> en</rdg>
                </app>
            </l>
            <l n="26">
                <app>
                    <rdg wit="#A">uedes por que o dig' assi</rdg>
                    <rdg wit="#B">uedes p<am>&#7651;</am><ex>or</ex> q<am>&#772;</am><ex>ue</ex> o dig' assy</rdg>
                </app>
            </l>
            <l n="27">
                <app>
                    <rdg wit="#A">coidei dela des q<am>&#772;</am><ex>ue</ex> a ui</rdg>
                    <rdg wit="#B">cuydei dela des q<am>&#772;</am><ex>ue</ex> a uj</rdg>
                </app>
            </l>
            <l n="28">
                <app>
                    <rdg wit="#A">auer g<am>&#x1DD3;</am><ex>ra</ex>n coita sen seu ben</rdg>
                    <rdg wit="#B">aueer g<am>&#x1DD3;</am><ex>ra</ex>m coyta sen seu ben</rdg>
                </app>
            </l>
        </lg>
        <lg type="finda">
            <l n="29">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>a nunca dela cuidei al</rdg>
                    <rdg wit="#B">Ca nu<am>&#772;</am><ex>n</ex>ca dela cuydei al</rdg>
                </app>
            </l>
            <l n="30">
                <app>
                    <rdg wit="#A">auer par deus que pod' e ual</rdg>
                    <rdg wit="#B">auer par d<am>&#x035B;</am><ex>eu</ex>s q<am>&#772;</am><ex>ue</ex> pod' e ual</rdg>
                </app>
            </l>
            <l n="31">
                <app>
                    <rdg wit="#A">erg' esta coita que me uen</rdg>
                    <rdg wit="#B">ergu' esta coyta q<am>&#772;</am><ex>ue</ex> mj uen</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A99B206">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">M</hi>ais de mil uezes coid' eu eno dia</rdg>
                    <rdg wit="#B">Mays de mil uezes cuid' eu eno dia</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">quando non posso mia sennor ueer</rdg>
                    <rdg wit="#B">quando no<am>&#772;</am><ex>n</ex> posso mha senhor ueer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">ca lle direi se a uir toda uia</rdg>
                    <rdg wit="#B">ca lhi direi se a <gap reason="error"/> toda uia</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">a mui gran coita que me faz soffrer</rdg>
                    <rdg wit="#B">a muj gram coyta que me faz sofrer</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">e <choice><orig>poila</orig><reg>poi-la</reg></choice> ueio uedes que mi auen</rdg>
                    <rdg wit="#B">e <choice><orig>poy la</orig><reg>poy-la</reg></choice> ueio uedes que mh auen</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">non lle digo de quanto coido ren</rdg>
                    <rdg wit="#B">non lhi digo de quanto cuido rem</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">ant' o_seu mui fremoso_pareçer</rdg>
                    <rdg wit="#B">ant' <choice><orig>osseu</orig></choice> muj <gap reason="error"/></rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">que_me faz quanto coido escaeçer</rdg>
                    <rdg wit="#B"><gap reason="error"/> faz quanto cuid' escaecer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="9">
                <app>
                    <rdg wit="#A"><hi rend="guide">C</hi>a <choice><orig>poila</orig><reg>poi-la</reg></choice> ueio non lle digo nada</rdg>
                    <rdg wit="#B">Ca <choice><orig>poila</orig><reg>poi-la</reg></choice> ueio no<am>&#772;</am><ex>n</ex> lhi digo nada</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">de quanto coid' ante q<am>&#772;</am><ex>ue</ex> lle direi</rdg>
                    <rdg wit="#B">de q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>to cuyd' ante q<am>&#772;</am><ex>ue</ex> lhi direi</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">u a non ueg' e par d<am>&#x035B;</am><ex>eu</ex>s mui coitada</rdg>
                    <rdg wit="#B">hu a no<am>&#772;</am><ex>n</ex> uei' e par d<am>&#x035B;</am><ex>eu</ex>s muj coytado</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">mente uiu' e por d<am>&#x035B;</am><ex>eu</ex>s que farei</rdg>
                    <rdg wit="#B">ment<am>&#x035B;</am><ex>re</ex> uiu' e p<am>&#7651;</am><ex>or</ex> d<am>&#x035B;</am><ex>eu</ex>s q<am>&#772;</am><ex>ue</ex> farei</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">ca <choice><orig>poila</orig><reg>poi-la</reg></choice> ueio coi<del rend="scrape">ta</del>do senpr<del rend="scrape"><gap reason="illegible"/></del>_enton</rdg>
                    <rdg wit="#B">ca <choice><orig>poila</orig><reg>poi-la</reg></choice> ueio cuido semp<am>&#x035B;</am><ex>r' e</ex>nto<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">no seu fremoso pareçer e non</rdg>
                    <rdg wit="#B">no seu f<am>&#x035B;</am><ex>re</ex>moso parecer e no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">me nenbra nada ca todo me fal</rdg>
                    <rdg wit="#B">me ne<am>&#772;</am><ex>n</ex>bra nada ca todo me fal</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">quanto lle coid' a dizer e dig' al</rdg>
                    <rdg wit="#B">q<am>&#x1DD3;</am><ex>ua</ex>nto lhi cuid' a diz<am>&#x035B;</am><ex>er</ex> e digu' al</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A101B208">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">M</hi>y mia sennor e meu lum' e meu ben</rdg>
                    <rdg wit="#B">Dy mha senhor e meu lum' e meu be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">per bo<am>&#772;</am><ex>&#771;</ex>a fe uerdade uus direi</rdg>
                    <rdg wit="#B">per boa fe uerdade u<am>&#42863;</am><ex>os</ex> direy</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">e sennor nunca uus eu mentirei</rdg>
                    <rdg wit="#B">e senhor nunca u<am>&#42863;</am><ex>os</ex> eu mentirey</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">ca uus q<am>&#772;</am><ex>ue</ex>ro muy mellor doutra ren</rdg>
                    <rdg wit="#B">ca u<am>&#42863;</am><ex>os</ex> quero melhor doutra rem</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">non me de. deus de uos ben nen de_si</rdg>
                    <rdg wit="#B">non mj de de<am>&#42863;</am><ex>us</ex> de uos ben nen <choice><orig>dessi</orig><reg>de si</reg></choice></rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">se nunca tan fremosa dona ui</rdg>
                    <rdg wit="#B">se nunca tam fremosa dona uj</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">com' e uos. e <choice><orig>confonda me</orig><reg>confonda-me</reg></choice> por en</rdg>
                    <rdg wit="#B">com' e uos e cofonda_mj por en</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A">E mia se<am>&#772;</am><ex>n</ex>nor e meu lum' e meu ben</rdg>
                    <rdg wit="#B">E mha senhor e meu lum' e meu be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">p<am>̲</am><ex>er</ex> o que m' eu muitas t<am>&#x035B;</am><ex>er</ex>ras andei</rdg>
                    <rdg wit="#B">p<am>̲</am><ex>er</ex>o q<am>&#772;</am><ex>ue</ex> m' eu muytas t<am>&#772;</am><ex>e</ex>iras andei</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">nunca y tan fremosa dona achei</rdg>
                    <rdg wit="#B">nu<am>&#772;</am><ex>n</ex>ca hi ta<am>&#772;</am><ex>n</ex> f<am>&#x1DD3;</am><ex>re</ex>mosa dona achei</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">com' e uos p<am>̲</am><ex>er</ex> q<am>&#772;</am><ex>ue</ex> me muito mal uen</rdg>
                    <rdg wit="#B">com' e uos p<am>̲</am><ex>er</ex> q<am>&#772;</am><ex>ue</ex> mj muyto mal ue<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">e <choice><orig>fez uus</orig><reg>fez-uus</reg></choice> d<am>&#x035B;</am><ex>eu</ex>s nacer por mal de mi</rdg>
                    <rdg wit="#B">e <choice><orig>fez u<am>&#42863;</am><ex>os</ex></orig></choice> d<am>&#x035B;</am><ex>eu</ex>s nacer p<am>&#7651;</am><ex>or</ex> mal demj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">se<am>&#772;</am><ex>n</ex>nor fremosa ca p<am>̲</am><ex>er</ex> uos perdi</rdg>
                    <rdg wit="#B">senhor fremosa ca p<am>̲</am><ex>er</ex> uos perdi</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">d<am>&#x035B;</am><ex>eu</ex>s. e amigos. e esforç' e_sen</rdg>
                    <rdg wit="#B">d<am>&#x035B;</am><ex>eu</ex>s e amigu' e esforc' <choice>essem</choice></rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A">Ca nunca eu no mu<am>&#772;</am><ex>n</ex>do pud' achar</rdg>
                    <rdg wit="#B">Ca nu<am>&#772;</am><ex>n</ex>ca eu no mu<am>&#772;</am><ex>n</ex>do pud' achar</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">des quando me u<am>&#42863;</am><ex>os</ex> d<am>&#x035B;</am><ex>eu</ex>s fez ueer</rdg>
                    <rdg wit="#B">des qua<am>&#772;</am><ex>n</ex>do mi u<am>&#42863;</am><ex>os</ex> d<am>&#x035B;</am><ex>eu</ex>s fez ueer</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">dona que me fezess' escaeçer</rdg>
                    <rdg wit="#B">dona q<am>&#772;</am><ex>ue</ex> mj fezess' escaeçer</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">uos a que d<am>&#x035B;</am><ex>eu</ex>s no mu<am>&#772;</am><ex>n</ex>do no<am>&#772;</am><ex>n</ex> fez par</rdg>
                    <rdg wit="#B">uos a q<am>&#772;</am><ex>ue</ex> d<am>&#x035B;</am><ex>eu</ex>s no mu<am>&#772;</am><ex>n</ex>do no<am>&#772;</am><ex>n</ex> fez par</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">ca u<am>&#42863;</am><ex>os</ex> fez de todo ben sabedor</rdg>
                    <rdg wit="#B">ca u<am>&#42863;</am><ex>os</ex> fez de todo be<am>&#772;</am><ex>n</ex> sabedor</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">e se no<am>&#772;</am><ex>n</ex> d<am>&#x035B;</am><ex>eu</ex>s no<am>&#772;</am><ex>n</ex> me de uoss' amor</rdg>
                    <rdg wit="#B">e se no<am>&#772;</am><ex>n</ex> d<am>&#x035B;</am><ex>eu</ex>s no<am>&#772;</am><ex>n</ex> mj de uosso amor</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">nen uosso ben que me faz deseiar</rdg>
                    <rdg wit="#B">ne<am>&#772;</am><ex>n</ex> uosso be<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex>m j faz deseiar</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="22">
