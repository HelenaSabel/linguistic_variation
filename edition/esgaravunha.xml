<tei>
    <div type="poem" corresp="#A115B231">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>uen uus foy dizer mia sennor</rdg>
                    <rdg wit="#B">Quen u<am>&#42863;</am><ex>os</ex> foy mha senh<am>&#7651;</am><ex>or</ex> dizer</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">que eu deseiaua mais al</rdg>
                    <rdg wit="#B">que eu deseiaua mays al</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">ca uus mentiu senon mal</rdg>
                    <rdg wit="#B">ca uos mentiu senon mal</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">me uenna de uos e de deus</rdg>
                    <rdg wit="#B">me uenha de uos e de de<am>&#42863;</am><ex>us</ex></rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">e se non nunca estes meus</rdg>
                    <rdg wit="#B">e se no<am>&#772;</am><ex>n</ex> nunca estes me<am>&#42863;</am><ex>us</ex></rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">ollos ueiam niun prazer</rdg>
                    <rdg wit="#B">olhos ueiam ne_hu<am>&#772;</am><ex>n</ex>u prazer</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">de quant' al deseian ueer</rdg>
                    <rdg wit="#B">de quant'e al deseiam ueer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> ueia eu de uos se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">E ueia eu de uos senhor</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">e de quant' al amo pesar</rdg>
                    <rdg wit="#B">e de qua<am>&#772;</am><ex>n</ex>t' al amo pesar</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">se nunca no uosso logar</rdg>
                    <rdg wit="#B">se nu<am>&#772;</am><ex>n</ex>ca no uosso log<am>&#7651;</am><ex>a</ex>r</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">tiue ren no meu coraçon</rdg>
                    <rdg wit="#B">tiue re<am>&#772;</am><ex>n</ex> no meu coraço<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">atanto deus non me p<am>̲</am><ex>er</ex>don</rdg>
                    <rdg wit="#B">ata<am>&#772;</am><ex>n</ex>to d<am>&#x035B;</am><ex>eu</ex>s no<am>&#772;</am><ex>n</ex> mi p<am>̲</am><ex>er</ex>don</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">nen me de nunca de uos ben</rdg>
                    <rdg wit="#B">ne<am>&#772;</am><ex>n</ex> mj de nu<am>&#772;</am><ex>n</ex>ca de uos be<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">que deseg' eu mais doutra ren</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> desei' eu mays doutra ren</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> per bo<am>&#772;</am><ex>&#771;</ex>a fe mia se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">E p<am>̲</am><ex>er</ex> bo<am>&#772;</am><ex>&#771;</ex>a fe mha senhor</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A"><choice><orig>amei u<am>&#42863;</am><ex>os</ex></orig></choice> muito mais ca mi</rdg>
                    <rdg wit="#B"><choice><orig>ameiu<am>&#42863;</am><ex>os</ex></orig></choice> muyto mays ca mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">e_se o non fezesse assi</rdg>
                    <rdg wit="#B"><choice><orig>esse</orig></choice> o no<am>&#772;</am><ex>n</ex> fezess' assy</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">de dur uerri' aqui mentir</rdg>
                    <rdg wit="#B">de dur ueiri' aq<am>&#x365;</am><ex>ui</ex> me<am>&#772;</am><ex>n</ex>tir</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">a uos nen m'iria partir</rdg>
                    <rdg wit="#B">a uos ne<am>&#772;</am><ex>n</ex> meria partir</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">d' u eu amasse outra moller</rdg>
                    <rdg wit="#B">d' u eu amass' outra molher</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">_ mais ca uos mais pois q<am>&#772;</am><ex>ue</ex> d<am>&#x035B;</am><ex>eu</ex>s q<am>&#772;</am><ex>ue</ex>r</rdg>
                    <rdg wit="#B"><del rend="overstrike">melhor</del> mays ca uos mays po<gap reason="damage"/> _ d<am>&#x035B;</am><ex>eu</ex>s q<am>&#772;</am><ex>ue</ex>r</rdg>
                </app>
            </l>
        </lg>
        <lg type="finda">
            <l n="22">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>ue eu a uos queira mellor</rdg>
                    <rdg wit="#B">Q eu a uos q<am>&#772;</am><ex>ue</ex>ra melh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A"><choice><orig>ualla mel</orig></choice> contra uos sennor</rdg>
                    <rdg wit="#B"><choice><orig>ualhamel</orig></choice> <am>&#42862;&#772;</am><ex>contra</ex> uos senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">ca muito me per e mester</rdg>
                    <rdg wit="#B">ca muyto mj p<am>̲</am><ex>er</ex> e mester</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A116B232">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><hi rend="guide">S</hi>ennor fremosa <choice><orig>conuen mia</orig></choice> rogar</rdg>
                    <rdg wit="#B">Senhor fremosa <choice><orig>co<am>&#772;</am><ex>n</ex>uenmh a</orig></choice> rogar</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">por uosso mal enquant' eu uiuo for</rdg>
                    <rdg wit="#B">por uosso mal enqua<am>&#772;</am><ex>n</ex>t' eu uiuo for</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">a deus ca faz me tanto mal amor</rdg>
                    <rdg wit="#B">a de<am>&#42863;</am><ex>us</ex> ca mj faz tanto mal amor</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">que eu ia senpr' assi ll' ei de rogar</rdg>
                    <rdg wit="#B">que eu ja senpr' assi lh' ei de rogar</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">que el <choice><orig>cofonda uos</orig></choice> e uosso sen</rdg>
                    <rdg wit="#B">que el <choice><orig>cofunda uos</orig></choice> e uosso sen</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">e min sennor que uus quero ben</rdg>
                    <rdg wit="#B">e mj senhor porque u<am>&#42863;</am><ex>os</ex> q<am>&#772;</am><ex>ue</ex>ro ben</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">e o amor que me uos faz amar</rdg>
                    <rdg wit="#B">e o amor q<am>&#772;</am><ex>ue</ex> me u<am>&#42863;</am><ex>os</ex> faz amar</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> uosso sen que por en mi errar</rdg>
                    <rdg wit="#B">E uosso sen q<am>&#772;</am><ex>ue</ex> p<am>&#7651;</am><ex>or</ex> en mj<am>&#772;</am><ex>n</ex> eirar</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">u<am>&#42863;</am><ex>os</ex> faz tan muito serei rogador</rdg>
                    <rdg wit="#B">u<am>&#42863;</am><ex>os</ex> faz tan muyto serey rogador</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">a d<am>&#x035B;</am><ex>eu</ex>s assi que confonda se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">a d<am>&#x035B;</am><ex>eu</ex>s assi q<am>&#772;</am><ex>ue</ex> cofonda senhor</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">_ el muit'e uos. e min en que errar</rdg>
                    <rdg wit="#B"><del rend="overstrike">el muyto eal mar <am>&#42863;</am><ex>us</ex>ue<am>&#772;</am><ex>n</ex></del> el muyto uos e mj<am>&#772;</am><ex>n</ex> en q<am>&#772;</am><ex>ue</ex> eirar</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">uus el faz tanto. e al mi ar conuen</rdg>
                    <rdg wit="#B">u<am>&#42863;</am><ex>os</ex> _ faz ta<am>&#772;</am><ex>n</ex>_muyto e al m' ar que<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">de lle rogar que ar cofonda quen</rdg>
                    <rdg wit="#B">de lhi rogar q<am>&#772;</am><ex>ue</ex> ar cofonda que<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">me non leixa conuusco mais morar</rdg>
                    <rdg wit="#B">me no<am>&#772;</am><ex>n</ex> leixa uosco mays morar</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> os meus ollos a que uus mostrar</rdg>
                    <rdg wit="#B">E os me<am>&#42863;</am><ex>us</ex> olh<am>&#42863;</am><ex>os</ex> a q<am>&#772;</am><ex>ue</ex> u<am>&#42863;</am><ex>os</ex> most<am>&#x1DD3;</am><ex>ra</ex>r</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">fuy eu por que uiu' oge na mayor</rdg>
                    <rdg wit="#B">fuj eu p<am>&#7651;</am><ex>or</ex> q<am>&#772;</am><ex>ue</ex> uiu' oie na mayor</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">coita do mundo ca non ei sabor</rdg>
                    <rdg wit="#B">coita do mud e no<am>&#772;</am><ex>n</ex> ei sabor</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">de nulla ren u <choice><orig>uolles</orig></choice> eu mostrar</rdg>
                    <rdg wit="#B">de nulha re<am>&#772;</am><ex>n</ex> hu <choice><orig>uolhis</orig></choice> eu most<am>&#x1DD3;</am><ex>ra</ex>r</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">non poss' e d<am>&#x035B;</am><ex>eu</ex>s cofonda mi<am>&#772;</am><ex>n</ex> por en</rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> poss' e de<am>&#42863;</am><ex>us</ex> confonda mj p<am>&#7651;</am><ex>or</ex> en</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">_ e uos se<am>&#772;</am><ex>n</ex>nor e eles e quen ten</rdg>
                    <rdg wit="#B"><del rend="overstrike">euos</del> e uos senh<am>&#7651;</am><ex>or</ex> e eles e q<am>&#771;&#x0321;</am><ex>uen</ex> ten</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">en coraçon de me uosco mezcrar</rdg>
                    <rdg wit="#B">en coraço<am>&#772;</am><ex>n</ex> de me uosco mizcrar</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A117B233">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><gap reason="unfinished"/>ennor fremosa quant' eu cofondi</rdg>
                    <rdg wit="#B">Sennor fremosa qua<am>&#772;</am><ex>n</ex>t' eu cofondi</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">o uosso sen. e uos e uoss' amor</rdg>
                    <rdg wit="#B">o uosso sen e uos e uoss' amor</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">con sanna _ que ouue mia sennor</rdg>
                    <rdg wit="#B">con sanha foy que ouuj mha senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">e con gran coita que me faz assi</rdg>
                    <rdg wit="#B">e co<am>&#772;</am><ex>n</ex> gram coita que me fez assy</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">sennor perder de tal guisa meu sen</rdg>
                    <rdg wit="#B">senhor perder de tal guisa meu sen</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">que cofondi uos en que tanto ben</rdg>
                    <rdg wit="#B">que cofondi uos en que ta<am>&#772;</am><ex>n</ex>to ben</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">a quanto nunca doutra don'oy</rdg>
                    <rdg wit="#B">a quanto nunca doutra dona_vy</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">M</hi>ais <choice><orig>ualla me</orig></choice> contra_uos_por_d<am>&#x035B;</am><ex>eu</ex>s y</rdg>
                    <rdg wit="#B">Mays <choice><orig>ualhamj</orig></choice> por_d<am>&#x035B;</am><ex>eu</ex>s_<am>&#42862;&#772;</am><ex>contra</ex>_uos hi</rdg>
