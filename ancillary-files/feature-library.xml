    <text>
        <body>
            <ab/>

            <fLib xml:id="linguistic" n="recurrent features">

                <!-- Main linguistic categories  -->
                <f name="phonetic" xml:id="phon">
                    <symbol value="ling"/>
                </f>
                <f name="morphosyntactic" xml:id="morph">
                    <symbol value="ling"/>
                </f>
                <f name="lexical" xml:id="lex">
                    <symbol value="ling"/>
                </f>
                <f name="languageTransfer" xml:id="transfer">
                    <symbol value="ling"/>
                </f>


                <!-- Parts of speech categories -->

                <f name="pronoum" xml:id="pr">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="verb" xml:id="vb">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="substantive" xml:id="n">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="conjuction" xml:id="conj">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="preposition" xml:id="prep">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="adverb" xml:id="adv">
                    <fs n="type" feats="#morph"/>
                </f>
                <f name="determiner" xml:id="det">
                    <fs n="type" feats="#morph"/>
                </f>



                <!-- Recurrent phonetic phenomena features-->

                <f name="consonantism" xml:id="cons">
                    <fs type="#phon">
                        <f name="constriction">
                            <binary value="true"/>
                        </f>
                    </fs>
                </f>
                <f name="vocalism" xml:id="vowel">
                    <fs type="#phon">
                        <f name="constriction">
                            <binary value="false"/>
                        </f>
                    </fs>
                </f>
                <f name="nasalization" xml:id="nasal">
                    <fs n="type" feats="#cons">
                        <f name="process">
                            <symbol value="transformation"/>
                        </f>
                    </fs>
                </f>
                <f name="palatalization" xml:id="pal">
                    <fs n="type" feats="#cons">
                        <f name="process">
                            <symbol value="transformation"/>
                        </f>
                    </fs>
                </f>
                <f name="sibiliants" xml:id="sibil">
                    <fs n="type" feats="#cons"> </fs>
                </f>
                <f name="diphthong" xml:id="diphthong">
                    <fs n="type" feats="#vowel"/>
                </f>



                <!-- Morphological phenomena features-->

                <f name="pastTheme" xml:id="past">
                    <fs n="type" feats="#vb">
                        <f name="regular">
                            <binary value="false"/>
                        </f>
                    </fs>
                </f>
                <f name="presentTheme" xml:id="pres">
                    <fs n="type" feats="#vb"/>
                </f>


                <!-- Other features-->
                <f name="analogy" xml:id="anal"/>
                <f name="doublet" xml:id="doublet"/>
                <f name="archaism" xml:id="archaism"/>

            </fLib>




            <fvLib corresp="#linguistic" n="linguistic variants">

                <!--Sound change-->

                <fs xml:id="apocope">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="reduction"/>
                            <f name="position">
                                <symbol value="end"/></f>
                        </fs>
                        <string xml:lang="en">Apocope</string>
                        <string xml:lang="pt">Apócope</string>
                    </f>
                </fs>
                <fs xml:id="prothesis">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="addition"/>
                            <f name="position">
                                <symbol value="start"/></f>
                        </fs>
                        <string xml:lang="en">Prothetic vowel</string>
                        <string xml:lang="pt">Prótese</string>
                    </f>
                </fs>
                <fs xml:id="yod">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="addition"/>
                            <f name="position">
                                <symbol value="middle"/></f>
                        </fs>
                        <string xml:lang="en">Epenthetic yod</string>
                        <string xml:lang="pt">Presença de iode epentético</string>
                    </f>
                </fs>
                <fs xml:id="paragoge">
                    <f name="type">
                        <fs feats="#phon #vowel">
                            <f name="process" select="addition"/>
                            <f name="position">
                                <symbol value="end"/></f>
                        </fs>
                        <string xml:lang="en">Paragoge</string>
                        <string xml:lang="pt">Paragoge</string>
                    </f>
                </fs>
                <fs xml:id="non-crasis">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="addition"/>
                        </fs>
                        <string xml:lang="en">Preservation of hiatus</string>
                        <string xml:lang="pt">Conservação dum hiato</string>
                    </f>
                </fs>
                <fs xml:id="apheresis">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="reduction"/>
                            <f name="position">
                                <symbol value="start"/></f>
                        </fs>
                        <string xml:lang="en">Apheresis</string>
                        <string xml:lang="pt">Aférese</string>
                    </f>
                </fs>
                <fs xml:id="pro-nas">
                    <f name="type">
                        <fs feats="#nasal">
                            <f name="direction" select="progressive"/>
                        </fs>
                        <string xml:lang="en">Progressive nasalization</string>
                        <string xml:lang="pt">Nasalização progressiva</string>
                    </f>
                </fs>
                <fs xml:id="ana-nas">
                    <f name="type">
                        <fs feats="#nasal #anal">
                            <f name="direction" select="analogy"/>
                        </fs>
                        <string xml:lang="en">Analog nasalization</string>
                        <string xml:lang="pt">Nasalização por analogia</string>
                    </f>
                </fs>
                <fs xml:id="denas">
                    <f name="type">
                        <fs feats="#nasal">
                            <f name="direction" select="none"/>
                        </fs>
                        <string xml:lang="en">Denasalization</string>
                        <string xml:lang="pt">Desnasalização</string>
                    </f>
                </fs>
                <fs xml:id="metaph">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="assimilation">
                                <symbol value="long-distance"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Vowel harmony</string>
                        <string xml:lang="pt">Harmonização vocálica</string>
                    </f>
                </fs>
                <fs xml:id="cons-assim">
                    <f name="type">
                        <fs feats="#cons">
                            <f name="process" select="assimilation">
                                <symbol value="adjacent"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Consonant assimilation</string>
                        <string xml:lang="pt">Assimilação consonántica</string>
                    </f>
                </fs>
                <fs xml:id="diss">
                    <f name="type">
                        <fs feats="#vowel">
                            <f name="process" select="dissimilation">
                                <symbol value="long-distance"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Vowel dissimilation</string>
                        <string xml:lang="pt">Dissimilação vocálica</string>
                    </f>
                </fs>
                <fs xml:id="sib-cognate">
                    <f name="type">
                        <fs feats="#sibil #doublet"/>
                        <string xml:lang="en">Double result in sibiliants evolution</string>
                        <string xml:lang="pt">Duplo resultado na evolução de sibilantes</string>
                    </f>
                </fs>
                <fs xml:id="dip">
                    <f name="type">
                        <fs feats="#diphthong"/>
                        <string xml:lang="en">Variation concerning diphthongs: 'oi/ui'</string>
                        <string xml:lang="pt">Vacilação na ditongação: 'oi/ui'</string>
                    </f>
                </fs>
                <fs xml:id="conf-sib">
                    <f name="type">
                        <fs feats="#sibil">
                            <f name="process">
                                <vAlt>
                                    <string>lenition</string>
                                    <string>fortition</string>
                                </vAlt>
                            </f>
                        </fs>
                        <string xml:lang="en">Likely confussion in the sonority feature of
                            sibiliants</string>
                        <string xml:lang="pt">Possível confusão na sonoridade de sibilantes</string>
                    </f>
                </fs>
                <fs xml:id="pal-nas">
                    <f name="type">
                        <fs feats="#pal">
                            <f name="process" select="mutation"/>
                        </fs>
                        <string xml:lang="en">Nassal palatal from nassalized hiatus</string>
                        <string xml:lang="pt">Desenvolvimento nasal palatal a partir de
                            hiato</string>
                    </f>
                </fs>


                <!--Morphological change: verbs-->

                
                <fs xml:id="pal-stem">
                    <f name="type">
                    <!--Incomplete-->
                        <string xml:lang="en">Palatalized past stem in irregular verbs</string>
                        <string xml:lang="pt">Palatalização tema perfeito pretéritos fortes</string>
                    </f>
                </fs>
                <fs xml:id="vow-stem">
                    <f name="type">
                        <!--Incomplete-->
                        <string xml:lang="en">Analog vowel past stem</string>
                        <string xml:lang="pt">Vogal analógica no tema de perfeito</string>
                    </f>
                </fs>
                <fs xml:id="foi">
                    <f name="type">
                        <fs feats="#past">
                            <f name="process" select="stem-change"/>
                            <f name="lexeme">
                                <vAlt>
                                    <string>seer</string>
                                    <string>ir</string>
                                </vAlt>
                            </f>
                            <f name="person">
                                <symbol value="1"/></f>
                            <f name="number">
                                <symbol value="sg"/>
                            </f>
                        </fs>
                        <fs feats="#diphthong" n="secondary"/>
                        <string xml:lang="en">P1 of perfect indicative of 'seer/ir' as
                            'foi'</string>
                        <string xml:lang="pt">P1 do perfeito de 'seer/ir' em 'foi'</string>
                    </f>
                </fs>
                <fs xml:id="fui">
                    <f name="type">
                        <fs feats="#past">
                            <f name="process" select="stem-change"/>
                            <f name="lexeme">
                                <vAlt>
                                    <string>seer</string>
                                    <string>ir</string>
                                </vAlt>
                            </f>
                            <f name="person">
                                <symbol value="3"/></f>
                            <f name="number">
                                <symbol value="sg"/>
                            </f>
                        </fs>
                        <fs feats="#diphthong" n="secondary"/>
                        <string xml:lang="en">P3 of perfect indicative of 'seer/ir' as
                            'fui'</string>
                        <string xml:lang="pt">P3 do perfeito de 'seer/ir' em 'fui'</string>
                    </f>
                </fs>
                <fs xml:id="p1i">
                    <f name="type">
                        <fs feats="#past">
                            <f name="process" select="inflectional-morpheme"/>
                            <f name="person">
                                <symbol value="1"/></f>
                            <f name="number">
                                <symbol value="sg"/>
                            </f>
                        </fs>
                        <fs n="secondary" feats="#archaism"/>
                        <string xml:lang="en">Perfect indicative personal ending of P1 as
                            "-i"</string>
                        <string xml:lang="pt">P1 do perfeito em '-i'</string>
                    </f>
                </fs>
                <fs xml:id="oer">
                    <f name="type">
                        <fs feats="#past">
                            <f name="process" select="stem-change"/>
                            <f name="lexeme" select="haver"/>
                        </fs>
                        <string xml:lang="en">Past stem of 'haver' as 'o-'</string>
                        <string xml:lang="pt">Tema de perfeito de 'haver' em 'o-'</string>
                    </f>
                </fs>
                <fs xml:id="anal-o">
                    <f name="type">
                        <fs feats="#past">
                            <f name="process" select="inflectional-morpheme"/>
                            <f name="person">
                                <symbol value="3"/></f>
                            <f name="number">
                                <symbol value="sg"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Analog 'o' in the P3 perfect indicative</string>
                        <string xml:lang="pt">SNP '-o' analógico na P3 dos pretéritos
                            fortes</string>
                    </f>
                </fs>


                <!--Morphological change: pronouns-->

                <fs xml:id="dat">
                    <f name="type">
                        <fs feats="#pr">
                            <f name="case">
                                <symbol value="accusative"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Dative contamination</string>
                        <string xml:lang="pt">Contaminação do dativo</string>
                    </f>
                </fs>
                <fs xml:id="dub-dat">
                    <f name="type">
                        <fs copyOf="#dat"/>
                        <string xml:lang="en">Dative contamination confronted with vocalic
                            crasis</string>
                        <string xml:lang="pt">Contaminação do dativo contrastada com uma crase
                            vocálica</string>
                    </f>
                </fs>
                <fs xml:id="xe">
                    <f name="type">
                        <fs feats="#pr">
                            <f name="person">
                                <symbol value="3"/></f>
                        </fs>
                        <fs feats="#pal" n="secondary"/>
                        <string xml:lang="en">Palatalized 'se'</string>
                        <string xml:lang="pt">Palatalização do pronome 'se'</string>
                    </f>
                </fs>
                <fs xml:id="li">
                    <f name="type">
                        <fs feats="#pr #archaism">
                            <f name="person">
                                <symbol value="3"/></f>
                            <f name="number">
                                <symbol value="sg"/>
                            </f>
                            <f name="case">
                                <symbol value="dative"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Dative pronoun 'li'</string>
                        <string xml:lang="pt">Pronome dativo 'li'</string>
                    </f>
                </fs>
                <fs xml:id="weak-pr">
                    <f name="type">
                        <fs feats="#pr"/>
                        <string xml:lang="en">Unstressed pronoun allomorph</string>
                        <string xml:lang="pt">Forma do pronome fraco</string>
                    </f>
                </fs>


                <!--Other morphological changes-->


                <fs xml:id="art">
                    <f name="type">
                        <fs feats="#det">
                            <f name="process" select="addition"/>
                        </fs>
                        <string xml:lang="en">Use of the article preceding a possessive</string>
                        <string xml:lang="pt">Uso do artigo com o posessivo</string>
                    </f>
                </fs>
                <fs xml:id="art-al">
                    <f name="type">
                        <fs feats="#det #phon"/>
                        <string xml:lang="en">Allomorphic form of the article</string>
                        <string xml:lang="pt">Forma alomorfa do artigo</string>
                    </f>
                </fs>


                <!--Other phenomena -->

                
                <fs xml:id="cognate">
                    <f name="type">
                        <fs feats="#doublet"/>
                        <string xml:lang="en">Doublet</string>
                        <string xml:lang="pt">Cognado</string>
                    </f>
                </fs>
                <fs xml:id="cast-transfer">
                    <f name="type">
                        <fs feats="#transfer">
                            <f name="language">
                                <symbol value="sp"/>
                            </f>
                        </fs>
                        <string xml:lang="en">Language transfer from Castilian</string>
                        <string xml:lang="pt">Interferência linguística do castelhano</string>
                    </f>
                </fs>
                <fs xml:id="par">
                    <f name="type">
                        <fs feats="#prep #lex"/>
                        <string xml:lang="en">Use of 'par' as preposition</string>
                        <string xml:lang="pt">Uso da preposição 'par'</string>
                    </f>
                </fs>
                <fs xml:id="per">
                    <f name="type">
                        <fs feats="#prep #lex"/>
                        <string xml:lang="en">Alternation 'per/por'</string>
                        <string xml:lang="pt">Alternância 'per/por'</string>
                    </f>
                </fs>
                <fs xml:id="ora">
                    <f name="type">
                        <fs feats="#adv #lex">
                            <f name="category" select="time"/>
                        </fs>
                        <string xml:lang="en">Use of 'ora' as a temporal adverb (expected
                            'agora')</string>
                        <string xml:lang="pt">Uso do advérbio temporal 'ora' (sendo esperável
                            'agora')</string>
                    </f>
                </fs>
                <fs xml:id="agora">
                    <f name="type">
                        <fs feats="#adv #lex">
                            <f name="category" select="time"/>
                        </fs>
                        <string xml:lang="en">Use of 'agora' (expected 'ora')</string>
                        <string xml:lang="pt">Uso de 'agora' (sendo esperável 'ora')</string>
                    </f>
                </fs>
                <fs xml:id="comp-ca">
                    <f name="type">
                        <fs feats="#conj">
                            <f name="lexeme" select="ca"/>
                            <f name="function" select="comparative"/>
                        </fs>
                        <fs n="secondary" feats="#lex"/>
                        <string xml:lang="en">Use of 'ca' as a comparative conjunction</string>
                        <string xml:lang="pt">Uso de 'ca' comparativo</string>
                    </f>
                </fs>
                <fs xml:id="compl-ca">
                    <f name="type">
                        <fs feats="#conj">
                            <f name="lexeme" select="ca"/>
                            <f name="function" select="complement"/>
                        </fs>
                        <fs n="secondary" feats="#lex"/>
                        <string xml:lang="en">Use of 'ca' as a complementizern</string>
                        <string xml:lang="pt">Uso de 'ca' completivo</string>
                    </f>
                </fs>
                <fs xml:id="comp-que">
                    <f name="type">
                        <fs feats="#conj">
                            <f name="lexeme" select="que"/>
                            <f name="function" select="comparative"/>
                        </fs>
                        <fs n="secondary" feats="#lex"/>
                        <string xml:lang="en">Use of 'que' as a comparative conjunction</string>
                        <string xml:lang="pt">Uso de 'que' comparativo</string>
                    </f>
                </fs>
                <fs xml:id="causal-que">
                    <f name="type">
                        <fs feats="#conj">
                            <f name="lexeme" select="que"/>
                            <f name="function" select="causal"/>
                        </fs>
                        <fs n="secondary" feats="#lex"/>
                        <string xml:lang="en">Use of 'que' as a causal conjuction</string>
                        <string xml:lang="pt">Uso de 'que' causal</string>
                    </f>
                </fs>
                <fs xml:id="ar">
                    <f name="type">
                        <fs feats="#adv">
                            <f name="lexeme">
                                <vAlt>
                                    <string>ar</string>
                                    <string>er</string>
                                </vAlt>
                            </f>
                        </fs>
                        <string xml:lang="en">Alternation 'ar/er'</string>
                        <string xml:lang="pt">Alternância 'ar/er'</string>
                    </f>
                </fs>
                <fs xml:id="depois">
                    <f name="type">
                        <fs feats="#adv">
                            <f name="lexeme">
                                <vAlt>
                                    <string>depois</string>
                                    <string>despois</string>
                                </vAlt>
                            </f>
                        </fs>
                        <string xml:lang="en">Alternation 'depois/despois'</string>
                        <string xml:lang="pt">Alternância 'depois/despois'</string>
                    </f>
                </fs>
                <fs xml:id="mas">
                    <f name="type">
                        <fs feats="#adv">
                            <f name="lexeme">
                                <vAlt>
                                    <string>mas</string>
                                    <string>mais</string>
                                </vAlt>
                            </f>
                        </fs>
                        <string xml:lang="en">Alternation 'mas/mais'</string>
                        <string xml:lang="pt">Alternância 'mas/mais'</string>
                    </f>
                </fs>
                <fs xml:id="inda">
                    <f name="type">
                        <fs feats="#adv">
                            <f name="lexeme">
                                <vAlt>
                                    <string>inda</string>
                                    <string>ainda</string>
                                </vAlt>
                            </f>
                        </fs>
                        <string xml:lang="en">Alternation 'inda/ainda'</string>
                        <string xml:lang="pt">Alternância 'inda/ainda'</string>
                    </f>
                </fs>
            </fvLib>



            <!-- Scribal errors -->


            <fLib n="recurrent scribal errors" xml:id="scribal">
                <f name="error" xml:id="gen-error"/>
                <f name="model" xml:id="model"/>
                <f name="ling-interference" xml:id="ling-interference"/>
            </fLib>

            <fvLib n="scribal errors" corresp="#scribal">

                <fs xml:id="misread">
                    <f name="type">
                        <fs feats="#model #gen-error"/>
                        <string xml:lang="en">Misreading</string>
                        <string xml:lang="pt">Erro de leitura do modelo</string>
                    </f>
                </fs>
                <fs xml:id="error">
                    <f name="type">
                        <fs feats="#gen-error"/>
                        <string xml:lang="en">Scribal error</string>
                        <string xml:lang="pt">Erro de cópia</string>
                    </f>
                </fs>
                <fs xml:id="ling-inter">
                    <f name="type">
                        <fs feats="#gen-error #ling-interference"/>
                        <string xml:lang="en">Linguistically meaningful scribal error</string>
                        <string xml:lang="pt">Erro de cópia com possíveis implicações
                            linguísticas</string>
                    </f>
                </fs>
            </fvLib>

            <fvLib n="equipolent readings">
                <fs xml:id="equip">
                    <f name="type">
                        <string xml:lang="en">Equipolent reading</string>
                        <string xml:lang="pt">Lição equipolente</string>
                    </f>
                </fs>
                <fs xml:id="material">
                    <f name="type">
                        <string xml:lang="en">Variant caused by the material conditions</string>
                        <string xml:lang="pt">Variante provocada pelas condições materiais</string>
                    </f>
                </fs>
                <fs xml:id="rev">
                    <f name="type">
                        <string xml:lang="en">Variant that contains a revision</string>
                        <string xml:lang="pt">Variante que contem uma correção</string>
                    </f>
                </fs>
            </fvLib>



            <!-- Graphic variants -->


            <!--The following is the tipology of variants I’m interested in.
            My intention is to calculate by computational methods its distribution,
            -->

            <!--<fvLib n="graphic variants">
                <fs xml:id="arch-graph" feats="#archaism #graph" n="">
                    <f name="type">
                        <string xml:lang="en">Archaic spelling</string>
                        <string xml:lang="pt">Grafia arcaica</string>
                    </f>
                </fs>
                <fs xml:id="innova">
                    <f name="type">
                        <string xml:lang="en">Innovatory spelling</string>
                        <string xml:lang="pt">Inovação gráfica</string>
                    </f>
                </fs>
                <fs xml:id="reg">
                    <f name="type">
                        <string xml:lang="en">Regular graphic variant</string>
                        <string xml:lang="pt">Variante gráfica regular</string>
                    </f>
                </fs>
                <fs xml:id="irreg">
                    <f name="type">
                        <string xml:lang="en">Irregular graphic variant</string>
                        <string xml:lang="pt">Variante gráfica com distribuição irregular</string>
                    </f>
                </fs>
                <fs xml:id="trend">
                    <f name="type">
