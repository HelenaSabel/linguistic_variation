                <f name="pronoum" xml:id="pr"/>
                <f name="verb" xml:id="vb"/>
                <f name="substantive" xml:id="n"/>
                <f name="conjuction" xml:id="conj"/>
                <f name="preposition" xml:id="prep"/>
                <f name="adverb" xml:id="adv"/>
                <f name="dativeContamination" xml:id="datCon"/>
                <f name="irregularVerb" xml:id="irregV"/>
                <f name="pastTheme" xml:id="past"/>
                <f name="presentTheme" xml:id="pres"/>
                <f name="HABERE" xml:id="habere"/>
                <f name="nasalization" xml:id="nasal"/>
                <f name="progressiveNasalization" xml:id="proNas"/>
                <f name="analogy" xml:id="anal"/>
                <f name="epentheticYod" xml:id="eYod"/>
                <f name="denasalization" xml:id="deNas"/>
                <f name="allomorph" xml:id="allom"/>
                <f name="apheresis" xml:id="apher"/>
                <f name="palatalization" xml:id="pal"/>
                <f name="foiP1" xml:id="foi1"/>
                <f name="fuiP1" xml:id="fui1"/>
                <f name="XE" xml:id="XE"/>
                <f name="dipthong" xml:id="dipthong"/>
                <f name="archaismLing" xml:id="archaismL"/>
                <f name="perfP1i" xml:id="p1I"/>
                <f name="loan" xml:id="Loan"/>
                <f name="par" xml:id="Par"/>
                <f name="per" xml:id="Per"/>
                <f name="agora" xml:id="Agora"/>
                <f name="ora" xml:id="Ora"/>
                <f name="vus" xml:id="uus"/>
                <f name="ca" xml:id="Ca"/>
                <f name="que" xml:id="Que"/>
                <f name="cognate" xml:id="Cognate"></f>
            </fLib>


            <fvLib n="Linguistic Variants">
                <fs xml:id="dat" feats="#datCon #pr #morph #ling"/>
                <fs xml:id="langT" feats="#transfer #ling"/>
                <fs xml:id="o" feats="#habere #past #irregV #vb #morph #ling"/>
                <fs xml:id="dip" feats="#dipthong #phon #ling"/>
                <fs xml:id="foi" feats="#foi1 #past #irregV #vb #morph #ling"/>
                <fs xml:id="fui" feats="#fui1 #past #irregV #vb #morph #ling"/>
                <fs xml:id="xe" feats="#XE #pr #morph #pal #phon #ling"/>
                <fs xml:id="min" feats="#proNas #nasal #phon #pr #ling"/>
                <fs xml:id="yod" feats="#eYod #phon #ling"/>
                <fs xml:id="p1i" feats="#p1I #past #irregV #vb #morph #ling"/>
                <fs xml:id="dNas" feats="#deNas #phon #ling"/>
                <fs xml:id="loan" feats="#Loan #ling"/>
                <fs xml:id="par" feats="#Par #prep #ling #lex"/>
                <fs xml:id="per" feats="#Per #prep #ling #lex"/>
                <fs xml:id="ora" feats="#Ora #adv #ling #lex"/>
                <fs xml:id="agora" feats="#Agora #adv #ling #lex"/>
                <fs xml:id="vus" feats="#uus #pr #ling"/>
                <fs xml:id="ca" feats="#Ca #conj #ling"/>
                <fs xml:id="que" feats="#Que #conj #ling"/>
                <fs xml:id="apheresis" feats="#apher #phon #ling"/>
                <fs xml:id="cognate" feats="#Cognate #phon #ling"/>
            </fvLib>


            <fLib n="Graphic Variants">
                <f name="graphicVariant" xml:id="graph"/>
                <f name="archaism" xml:id="archaism"/>
                <f name="innovation" xml:id="innovation"/>
                <f name="regularVar" xml:id="regular"/>
                <f name="irregularVar" xml:id="irregular"/>
                <f name="tendency" xml:id="tendency"/>
                <f name="linguisticRelevance" xml:id="lingRelevance"/>
            </fLib>

            <fvLib n="Graphic Variants">
                <fs xml:id="archG" feats="#archaism #graph"/>
                <fs xml:id="innova" feats="#innovation #graph"/>
                <fs xml:id="reg" feats="#regular #graph"/>
                <fs xml:id="irreg" feats="#irregular #graph"/>
                <fs xml:id="trend" feats="#tendency #graph"/>
                <fs xml:id="lingR" feats="#lingRelevance #graph"/>
            </fvLib>

            <fLib n="Scribal errors">
                <f name="scribalError" xml:id="Error"/>
                <f name="misreading" xml:id="misreading"/>
                <f name="lingInterference" xml:id="lingInter"/>
            </fLib>

            <fvLib n="Scribal errors">
                <fs xml:id="misread" feats="#misreading #Error"/>
                <fs xml:id="lingInt" feats="#lingInter #Error"/>
                <fs xml:id="error" feats="#Error"/>
            </fvLib>


            <fvLib n="Equipolent Readings">
                <fs xml:id="equip"/>
                <fs xml:id="material"/>
            </fvLib>
            
        </body>
    </text>
</TEI>
