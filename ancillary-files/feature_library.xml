            <fvLib n="Linguistic Variants">
                <fs xml:id="dat"
                feats="#datCon #pr #morph #ling"
                n="Contaminação do dativo">
                    <!--Contaminação do dativo-->
                    <!--Dative contamination-->
                </fs>
