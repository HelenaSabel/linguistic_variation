                feats="#transfer #ling"
                n="Interfererência linguística">
                    <!--Interfererência linguística-->
                    <!--Language transfer-->
                </fs>
                <fs xml:id="oer"
                feats="#habere #past #irregV #vb #morph #ling"
                n="Tema de perfeito de 'haver' em 'o-'">
                    <!--Tema de perfeito de 'haver' em 'o-'-->
                    <!--Past stem of 'haver' as 'o-'-->
                </fs>
                <fs xml:id="dip"
