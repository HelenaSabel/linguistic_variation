                <f name="loan" xml:id="Loan"/>
                <f name="par" xml:id="Par"/>
                <f name="per" xml:id="Per"/>
                <f name="agora" xml:id="Agora"/>
                <f name="ora" xml:id="Ora"/>
                <f name="vus" xml:id="uus"/>
                <f name="ca" xml:id="Ca"/>
                <f name="que" xml:id="Que"/>
                <f name="cognate" xml:id="Cognate"/>
            </fLib>


            <fvLib n="Linguistic Variants">
                <fs xml:id="dat" feats="#datCon #pr #morph #ling">
                    <!--Contaminación do dativo-->
                    <!--Dative contamination-->
                </fs>
                <fs xml:id="langT" feats="#transfer #ling">
                    <!--Interfererencia lingüística-->
                    <!--Language transfer-->
                </fs>
                <fs xml:id="o" feats="#habere #past #irregV #vb #morph #ling">
                    <!--Tema de perfeito de 'haver' en "o-"-->
                    <!--Past stem of 'haver' as "o-"-->
                </fs>
                <fs xml:id="dip" feats="#dipthong #phon #ling">
                    <!--Variación relacionada coa diptongación-->
                    <!--Variation concerning dipthongs-->
                </fs>
                <fs xml:id="foi" feats="#foi1 #past #irregV #vb #morph #dipthong #phon #ling">
                    <!--"foi" como P1 do perfeito de 'seer'-->
                    <!--P1 of perfect indicative of 'seer' as "foi"-->
                </fs>
                <fs xml:id="fui" feats="#fui1 #past #irregV #vb #morph #dipthong #phon #ling">
                    <!--"fui" como P1 do perfeito de 'seer'-->
                    <!--P1 of perfect indicative of 'seer' as "fui"-->
                </fs>
                <fs xml:id="xe" feats="#palSe #pr #morph #pal #phon #ling">
                    <!--Palatalización do pronome 'se'-->
                    <!--Palatalized 'se'-->
                </fs>
                <fs xml:id="min" feats="#proNas #nasal #phon #pr #ling">
                    <!--Nasalización progresiva de 'mi'-->
                    <!--Progressive nasalization of 'mi'-->
                </fs>
                <fs xml:id="yod" feats="#eYod #phon #ling">
                    <!--Presenza de iode epentético-->
                    <!--Epenthetic yod-->
                </fs>
                <fs xml:id="p1i" feats="#p1I #past #irregV #vb #morph #ling">
                    <!--P1 do perfeito en "-i"-->
                    <!--Perfect indicative personal ending of P1 as "-i" -->
                </fs>
                <fs xml:id="dNas" feats="#deNas #phon #ling">
                    <!--Desnasalización-->
                    <!--Denasalization-->
                </fs>
                <fs xml:id="loan" feats="#Loan #ling">
                    <!--Empréstimo lingüístico-->
                    <!--Loan-->
                </fs>
                <fs xml:id="par" feats="#Par #prep #ling #lex">
                    <!--Uso da preposición "par" -->
                    <!--Use of "par" as preposition-->
                </fs>
                <fs xml:id="per" feats="#Per #prep #ling #lex">
                    <!--Uso da "preposición "per"-->
                    <!--Use of the form "per"-->
                </fs>
                <fs xml:id="ora" feats="#Ora #adv #ling #lex">
                    <!--Uso do adverbio temporal "ora"-->
                    <!--Use of "ora" as a temporal adverb-->
                </fs>
                <fs xml:id="agora" feats="#Agora #adv #ling #lex">
                    <!--Uso do adverbio temporal "agora"-->
                    <!--Use of "agora" as a temporal adverb-->
                </fs>
                <fs xml:id="vus" feats="#uus #pr #ling">
                    <!--??-->
                    <!--Maybe graphic? /o/ as <u>-->
                </fs>
                <fs xml:id="ca" feats="#Ca #conj #ling">
                    <!--Uso de "ca" comparativo-->
                    <!--Use of "ca" as a comparative conjunction-->
                </fs>
                <fs xml:id="que" feats="#Que #conj #ling">
                    <!--Uso de "que" comparativo-->
                    <!--Use of "que" as a comparative conjunction-->
                </fs>
                <fs xml:id="apheresis" feats="#apher #phon #ling">
                    <!--Aférese-->
                    <!--Apheresis-->
                </fs>
                <fs xml:id="cognate" feats="#Cognate #phon #ling">
                    <!--Doblete léxico-->
                    <!--Cognate-->
                </fs>
            </fvLib>


            <fLib n="Graphic Variants">
                <f name="graphicVariant" xml:id="graph"/>
                <f name="archaism" xml:id="archaism"/>
                <f name="innovation" xml:id="innovation"/>
                <f name="regularVar" xml:id="regular"/>
                <f name="irregularVar" xml:id="irregular"/>
                <f name="tendency" xml:id="tendency"/>
                <f name="linguisticRelevance" xml:id="lingRelevance"/>
            </fLib>

            <fvLib n="Graphic Variants">
                <fs xml:id="archG" feats="#archaism #graph">
                    <!--Grafía arcaica-->
                    <!--Archaic spelling-->
                </fs>
                <fs xml:id="innova" feats="#innovation #graph">
                    <!--Innovación gráfica-->
                    <!--Innovatory spelling-->
                </fs>
                <fs xml:id="reg" feats="#regular #graph">
                    <!--Variante gráfica regular-->
                    <!--Regular graphic variant-->
                </fs>
                <fs xml:id="irreg" feats="#irregular #graph">
                    <!--Variante gráfica con distribución irregular -->
                    <!--Irregular graphic variant-->
                </fs>
                <fs xml:id="trend" feats="#tendency #graph">
                    <!--Tendencia ortográfica-->
                    <!--Ortographic tendency (not completely regular)-->
                </fs>
                <fs xml:id="lingR" feats="#lingRelevance #graph">
                    <!--Variante gráfica con posíbeis implicacións lingüísticas-->
                    <!--Linguistically meaningful graphic variant-->
                </fs>
            </fvLib>

            <fLib n="Scribal errors">
                <f name="scribalError" xml:id="Error"/>
                <f name="misreading" xml:id="misreading"/>
                <f name="lingInterference" xml:id="lingInter"/>
            </fLib>

            <fvLib n="Scribal errors">
                <fs xml:id="misread" feats="#misreading #Error">
                    <!--Erro de lectura do modelo-->
                    <!--Misreading-->
                </fs>
                <fs xml:id="lingInt" feats="#lingInter #Error">
                    <!--Erro de copia con posíbeis implicacións lingüísticas-->
                    <!--Linguistically meaningful scribal error-->
                </fs>
                <fs xml:id="error" feats="#Error">
                    <!--Erro de copia-->
                    <!--Scribal error-->
                </fs>
            </fvLib>
            <fvLib n="Equipolent Readings">
                <fs xml:id="equip">
                    <!--Lección equipolente-->
                    <!--Equipolent reading-->
                </fs>
                <fs xml:id="material">
                    <!--Variante provocada polas condicións materiais-->
                    <!--Variant caused by the material conditions-->
                </fs>
            </fvLib>

        </body>
