    <text>
        <front>
            <div>
                <listWit>
                    <witness xml:id="A">Cancioneiro da Ajuda</witness>
                    <witness xml:id="B">Cancioneiro da Biblioteca Nacional</witness>
                </listWit>
            </div>
        </front>
        <body>
<div type="poem" corresp="#A47B159">
            <head>
               <title>
                  <app>
                     <rdg wit="#A" hand="#a">
                        <idno>A47</idno>
                        <locus from="11v" to="11v"/>
                     </rdg>
                     <rdg wit="#B" hand="#d">
                        <idno>B159</idno>
                        <locus from="40v" to="41r"/>
                     </rdg>
                  </app>
               </title>
               <name role="author" ref="#MartSrz"/>
            </head>
            <l n="1">
               <app>
                  <rdg wit="#A #B">Quando me nenbra de uos mia sennor</rdg>
               </app>
            </l>
            <l n="2">
               <app>
                  <rdg wit="#A #B">en qual affan me fazedes uiuer</rdg>
               </app>
            </l>
            <l n="3">
               <app>
                  <rdg wit="#A">
                     <ex>e</ex>
                  </rdg>
                  <rdg wit="#B" ana="#error">
                     <gap reason="error"/>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A #B">de qual guisa leixades amor</rdg>
               </app>
            </l>
            <l n="4">
               <app>
                  <rdg wit="#A #B">fazer en mi quanto x&#8217; el quer fazer</rdg>
               </app>
            </l>
            <l n="5">
               <app>
                  <rdg wit="#A #B">enton me cuid&#8217; eu de uos a quitar</rdg>
               </app>
            </l>
            <l n="6">
               <app>
                  <rdg wit="#A #B">mais uos pois uos ueg&#8217; e uus ouço falar</rdg>
               </app>
            </l>
            <l n="7">
               <app>
                  <rdg wit="#A #B">outro cuidad&#8217; ar ei log&#8217; a prender</rdg>
               </app>
               <app>
                  <rdg wit="#A">cuidad&#8217;</rdg>
                  <rdg wit="#B" ana="#Vy">cuydad&#8217;</rdg>
               </app>
               <app>
                  <rdg wit="#A #B">ar</rdg>
               </app>
               <app>
                  <rdg wit="#A">ei</rdg>
                  <rdg wit="#B" ana="#Vy">ey</rdg>
               </app>
               <app>
                  <rdg wit="#A #B">log&#8217; a prender</rdg>
               </app>
            </l>
            <l n="8">
               <app>
                  <rdg wit="#A #B">Porque</rdg>
               </app>
               <app>
                  <rdg wit="#A">u<ex>os</ex>
                  </rdg>
                  <rdg wit="#B" ana="#puf">u<ex>er</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A #B">ueio falar</rdg>
               </app>
               <app>
                  <rdg wit="#A">mui</rdg>
                  <rdg wit="#B" ana="#uj">muj</rdg>
               </app>
               <app>
                  <rdg wit="#A">mellor</rdg>
                  <rdg wit="#B" ana="#abb">melh<ex>or</ex></rdg>
               </app>
            </l>
            <l n="9">
               <app>
                  <rdg wit="#A #B">de quantas donas sei</rdg>
               </app>
               <app>
                  <rdg wit="#A" ana="#punct">.</rdg>
               </app>
               <app>
                  <rdg wit="#A" ana="#abb">
                     <ex>e</ex>
                  </rdg>
                  <rdg wit="#B">e</rdg>
               </app>
               <app>
                  <rdg wit="#A" ana="#çe">pareçer</rdg>
                  <rdg wit="#B">parecer</rdg>
               </app>
            </l>
            <l n="10">
               <app>
                  <rdg wit="#A #B">e cuid&#8217; en como sodes sabedor</rdg>
               </app>
            </l>
            <l n="11">
               <app>
                  <rdg wit="#A #B">de quanto ben dona deu a saber</rdg>
               </app>
            </l>
            <l n="12">
               <app>
                  <rdg wit="#A #B">este cuidado me faz destoruar</rdg>
               </app>
            </l>
            <l n="13">
               <app>
                  <rdg wit="#A #B">de quant&#8217; al cuid&#8217; e no<ex>n</ex>me q<ex>ue</ex>r leixar</rdg>
               </app>
            </l>
            <l n="14">
               <app>
                  <rdg wit="#A #B">partir de ne<ex>n</ex> de</rdg>
               </app>
               <app>
                  <rdg wit="#A">u<ex>os</ex>
                  </rdg>
                  <rdg wit="#B" ana="#puf">u<ex>er</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A #B">ben querer</rdg>
               </app>
            </l>
            <l n="15">
               <app>
                  <rdg wit="#A #B">E</rdg>
               </app>               
               <app>
                  <rdg wit="#A">quand&#8217;</rdg>
                  <rdg wit="#B" ana="#abb">q<ex>uan</ex>d&#8217;</rdg>
               </app>               
               <app>
                  <rdg wit="#A">ar</rdg>
                  <rdg wit="#B" ana="#ar">er</rdg>
               </app>               
               <app>
                  <rdg wit="#A #B">soyo</rdg>
               </app>
               <app>
                  <rdg wit="#A">cuidar</rdg>
                  <rdg wit="#B" ana="#uy">cuydar</rdg>
               </app>
               <app>
                  <rdg wit="#A #B">no pauor</rdg>
               </app>
            </l>
            <l n="16">
               <app>
                  <rdg wit="#A #B">que me fazedes mia</rdg>
               </app>
               <app>
                  <rdg wit="#A">sen<ex>n</ex>or</rdg>
                  <rdg wit="#B" ana="#abb">senh<ex>or</ex></rdg>
               </app>
               <app>
                  <rdg wit="#A" ana="#punct">.</rdg>
               </app>
               <app>
                  <rdg wit="#A" ana="#ff">soffrer</rdg>
                  <rdg wit="#B">sofrer</rdg>
               </app>
            </l>
            <l n="17">
               <app>
                  <rdg wit="#A">enton</rdg>
                  <rdg wit="#B" ana="#abb">ento<ex>n</ex></rdg>
               </app>
               <app>
                  <rdg wit="#A #B">cuid&#8217;</rdg>                  
               </app>
               <app>
                  <rdg wit="#A">enquant</rdg>
                  <rdg wit="#B" ana="#abb">enq<ex>uan</ex>t&#8217;</rdg>                  
               </app>
               <app>
                  <rdg wit="#A #B">eu uiuo for</rdg>                  
               </app>
            </l>
            <l n="18">
               <app>
                  <rdg wit="#A #B">que nunca uenn&#8217; ao uosso</rdg>
               </app>
               <app>
                  <rdg wit="#A">poder</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg> cuid' eu ' eu uiuo for
               </app>
            </l>
            <l n="19">
               <app>
                  <rdg wit="#A #B">mais <choice>
                        <orig>tolleme<ex>n</ex>
                        </orig>
                     <reg>tolle-m&#8217; e<ex>n</ex></reg>
                     </choice> log&#8217; aqueste cuidar</rdg>
               </app>
            </l>
            <l n="20">
               <app>
                  <rdg wit="#A #B">uosso bon <ex>e</ex> uosso</rdg>
               </app>
               <app>
                  <rdg wit="#A">semellar</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap/>
                  </rdg>
               </app>
            </l>
            <l n="21">
               <app>
                  <rdg wit="#A #B">e quanto ben de uos ouç0</rdg>
               </app>
               <app>
                  <rdg wit="#A">dizer</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap/>
                  </rdg>
               </app>
            </l>
            <l n="22">
               <app>
                  <rdg wit="#A #B">Mais quen</rdg>
               </app>
               <app>
                  <rdg wit="#A">u<ex>os</ex>
                  </rdg>
                  <rdg wit="#B" ana="#puf">u<ex>er</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A #B">ousa mia se<ex>n</ex>nor catar</rdg>
               </app>
            </l>
            <l n="23">
               <app>
                  <rdg wit="#A #B">d<ex>eu</ex>s como pod&#8217; o coraço<ex>n</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A">quitar</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg>
               </app>
            </l>
            <l n="24">
               <app>
                  <rdg wit="#A #B">de</rdg>
               </app>
               <app>
                  <rdg wit="#A">u<ex>os</ex>
                  </rdg>
                  <rdg wit="#B" ana="#puf">u<ex>er</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A #B">nen os ollos de</rdg>
               </app>
               <app>
                  <rdg wit="#A">u<ex>os</ex>
                  </rdg>
                  <rdg wit="#B" ana="#puf">u<ex>er</ex>
                  </rdg>
               </app>
               <app>
                  <rdg wit="#A">ueer</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg>
               </app>
            </l>
            <l n="25">
               <app>
                  <rdg wit="#A #B">Nen como pod&#8217; al ben</rdg>
               </app>
               <app>
                  <rdg wit="#A">deseiar</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg>
               </app>
            </l>
            <l n="26">
               <app>
                  <rdg wit="#A #B">senon de uos</rdg>
               </app>
               <app>
                  <rdg wit="#A">q<ex>ue</ex>
                  </rdg>
                  <rdg wit="#B" ana="#error">q</rdg>
               </app>
               <app>
                  <rdg wit="#A #B">sol oyr</rdg>
               </app>
               <app>
                  <rdg wit="#A">falar</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg>
               </app>
            </l>
            <l n="27">
               <app>
                  <rdg wit="#A #B">en quanto ben d<ex>eu</ex>s en uos faz</rdg>
               </app>
               <app>
                  <rdg wit="#A">auer</rdg>
                  <rdg wit="#B" ana="#puf">
                     <gap reason="unknown"/>
                  </rdg>
               </app>
            </l>
         </div>
        </body>
    </text>
