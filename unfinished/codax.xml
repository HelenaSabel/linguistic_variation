                        <rdg wit="#R">ergas en uigo no sa<expan>g<ex>ra</ex></expan>do</rdg>
                    </app>
                </l>
                <l n="18">
                    <app>
                        <rdg wit="#V">amor</rdg>
                        <rdg wit="#R">amor ei</rdg>
                    </app>
                </l>
            </div>
            <div type="poem" corresp="#B1284V890R7">
                <head>
                    <title>
                        <app>
                            <rdg wit="#V">V890</rdg>
                            <rdg wit="#R">R7</rdg>
                        </app>
                    </title>
                    <name role="author" ref="#MartCod"/>
                </head>
                <l n="1">
                    <app>
                        <rdg wit="#V">Ay ondas que eu uin ueer</rdg>
                        <rdg wit="#R"><handShift corresp="secondHand"/>y ondas que eu uin<damage>ueer</damage></rdg>
                    </app>
                </l>
                <l n="2">
                    <app>
                        <rdg wit="#V">se mi saberedes dizer</rdg>
                        <rdg wit="#R"><damage>se</damage> <damage>m</damage>e saberedes dizer</rdg>
                    </app>
                </l>
                <l n="3">
                    <app>
                        <rdg wit="#V">por que tarda meu amigo  sen mi</rdg>
                        <rdg wit="#R">por que tarda meu  amigo s<expan>e<ex>n</ex></expan> mj</rdg>
                    </app>
                </l>
                <l n="4">
                    <app>
                        <rdg wit="#V">Ay <del>d</del>ònas q eu uin uirar</rdg>
                        <rdg wit="#R">Ay ondas <expan>q<ex>ue</ex></expan> eu u<expan>i<ex>n</ex></expan> mirar</rdg>
                    </app>
                </l>
                <l n="5">
                    <app>
                        <rdg wit="#V">se mi saberedes contar</rdg>
                        <rdg wit="#R">se me saberedes contar</rdg>
                    </app>
                </l>
                <l n="6">
                    <app>
                        <rdg wit="#V">porq tarda meu amigo</rdg>
                        <rdg wit="#R">por<expan>q<ex>ue</ex></expan>. t. m. a. s. mj</rdg>
                    </app>
                </l>
            </div>
