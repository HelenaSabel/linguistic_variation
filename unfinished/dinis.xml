            <rdg wit="#B">ei</rdg>
            <rdg wit="#V">ey</rdg>
         </app>
         <app>
            <rdg wit="#T"/>
            <rdg wit="#B #V">como m' assaluar</rdg>
         </app>
        </l>
        <l n="4">
            <app>
            <rdg wit="#T"><gap reason="damage"/></rdg>
            <rdg wit="#B">que me no<ex>n</ex></rdg>
            <rdg wit="#V">que me non</rdg>
         </app>
         <app>
            <rdg wit="#T">iu<gap reason="damage"/>g<gap reason="damage"/></rdg>
            <rdg wit="#B">iulgue</rdg>
            <rdg wit="#V">iulge</rdg>
         </app>
         <app>
            <rdg wit="#T">
               <gap/>
            </rdg>
            <rdg wit="#B">por sen traedor</rdg>
            <rdg wit="#V">por seu traedor</rdg>
         </app>
        </l>
        <l n="5">
            <app>
            <rdg wit="#T"><gap reason="damage"/></rdg>
            <rdg wit="#B">Poys</rdg>
            <rdg wit="#V">poys</rdg>
         </app>
         <app>
            <rdg wit="#T">tamanho</rdg>
            <rdg wit="#B">tamhano</rdg>
            <rdg wit="#V">camanho</rdg>
         </app>tenp' <app>
            <rdg wit="#T"><gap reason="damage"/>ci</rdg>
            <rdg wit="#B">a q<ex>ue</ex> guarecy</rdg>
            <rdg wit="#V">a que guareci</rdg>
         </app>
        </l>
        <l n="6">
            <app>
            <rdg wit="#T #V">sen</rdg>
            <rdg wit="#B">se<ex>n</ex></rdg>
         </app>
         <app>
            <rdg wit="#T #B">seu</rdg>
            <rdg wit="#V"> </rdg>
         </app>
         <app>
            <rdg wit="#T">manda<gap reason="damage"/></rdg>
            <rdg wit="#B">mandad' ohir e a no<ex>n</ex></rdg>
            <rdg wit="#V">mandad' ohir e a non</rdg>
         </app>
         <app>
            <rdg wit="#T"><gap reason="damage"/>r</rdg>
            <rdg wit="#B">uir</rdg>
            <rdg wit="#V">uyr</rdg>
         </app>
        </l>
        <l n="7">
            <app>
            <rdg wit="#T">E ssey</rdg>
            <rdg wit="#B #V">Essey</rdg>
         </app>eu mui be<ex>n</ex> no <app>
            <rdg wit="#T">m<gap reason="damage"/></rdg>
            <rdg wit="#B #V">meu coraço<ex>n</ex></rdg>
         </app>
        </l>
        <l n="8">
            <app>
            <rdg wit="#T"><gap reason="damage"/></rdg>
            <rdg wit="#B #V">o q<ex>ue</ex> mha</rdg>
         </app>senh<ex>or</ex> <app>
            <rdg wit="#T #V">f<ex>re</ex>mosa</rdg>
            <rdg wit="#B">fremosa</rdg>
         </app>fara 
        </l>
        <l n="9">
            depoys q<ex>ue</ex> <app>
            <rdg wit="#T">an<gap reason="damage"/></rdg>
            <rdg wit="#B #V">ant' ela for</rdg>
         </app><choice><orig>iulgarma</orig><reg>iulgar-ma</reg></choice> 
        </l>
        <l n="10">
            por seu <app>
            <rdg wit="#T"><gap reason="damage"/></rdg>
            <rdg wit="#B #V">traedor</rdg>
         </app>
         <app>
            <rdg wit="#T #V">co<ex>n</ex></rdg>
            <rdg wit="#B">cu</rdg>
         </app>
         <app>
            <rdg wit="#T">muj</rdg>
            <rdg wit="#B #V">mui</rdg>
         </app>
         <app>
            <rdg wit="#T #V">g<ex>ra</ex>m</rdg>
            <rdg wit="#B">gra<ex>n</ex></rdg>
         </app>razo<ex>n</ex> 
        </l>
        <l n="11">
            <app>
            <rdg wit="#T">Poys</rdg>
            <rdg wit="#B #V">poys</rdg>
         </app>
         <app>
            <rdg wit="#T">tamanho</rdg>
            <rdg wit="#B #V">camanho</rdg>
         </app>te<ex>n</ex>p' a q<ex>ue</ex> <app>
            <rdg wit="#T #V">guareçi</rdg>
            <rdg wit="#B">guareci</rdg>
         </app>
        </l>
        <l n="12">
            E poys tamanho foy o <app>
            <rdg wit="#T #V">erro</rdg>
            <rdg wit="#B">eiro</rdg>
         </app>meu 
        </l>
        <l n="13">
            <app>
            <rdg wit="#T #V">q<ex>ue</ex></rdg>
            <rdg wit="#B">que</rdg>
         </app>lhi <app>
            <rdg wit="#T">fiz</rdg>
            <rdg wit="#B #V">fez</rdg>
         </app>torto ta<ex>n</ex> descomunal 
        </l>
        <l n="14">
            se mh a sa <app>
            <rdg wit="#T #V">g<ex>ra</ex>m</rdg>
            <rdg wit="#B">gra<ex>n</ex></rdg>
         </app>mesura no<ex>n</ex> ual 
        </l>
        <l n="15">
            <app>
            <rdg wit="#T #V"><choice><orig>iulgarma</orig><reg>iulgar-ma</reg></choice></rdg>
            <rdg wit="#B"><choice><orig>julgarma</orig><reg>julgar-ma</reg></choice></rdg>
         </app>por <app>
            <rdg wit="#T #V">en</rdg>
            <rdg wit="#B">e<ex>n</ex></rdg>
         </app>por traedor seu 
        </l>
        <l n="16">
            Poys tamanho <app>
            <rdg wit="#T #B">te<ex>n</ex>p'</rdg>
            <rdg wit="#V">temp'</rdg>
         </app>a q<ex>ue</ex> 
        </l>
        <l n="17">
            Se o <app>
            <rdg wit="#T #V">Iuyzo</rdg>
            <rdg wit="#B">Iuizo</rdg>
         </app>passar assy 
        </l>
        <l n="18">
            ay eu catiu' e q<ex>ue</ex> sera de <app>
            <rdg wit="#T #V">mj<ex>n</ex></rdg>
            <rdg wit="#B">mi<ex>n</ex></rdg>
         </app>
        </l>
    </div>
