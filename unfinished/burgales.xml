<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem" corresp="#A82B186b">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A">De quantos mui coitados son</rdg>
                    <rdg wit="#B">De quant<am>&#42863;</am><ex>os</ex> muj coytados son</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">a que deus coita faz auer</rdg>
                    <rdg wit="#B">a que de<am>&#42863;</am><ex>us</ex> coyta faz auer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">min faz mais coitado uiuer</rdg>
                    <rdg wit="#B">mj faz mays coitado uiuer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">e <choice><orig>direi uus</orig></choice> per qual razon</rdg>
                    <rdg wit="#B">e <choice><orig>direy u<am>&#42863;</am><ex>os</ex></orig></choice> por qual razon</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A"><choice><orig>faz me</orig></choice> querer ben tal sennor</rdg>
                    <rdg wit="#B"><choice><orig>faz mj</orig></choice> querer ben tal senhor</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">a mais fremosa nen mellor</rdg>
                    <rdg wit="#B">a mays fremosa nen melhor</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">do mund e non mi a faz ueer</rdg>
                    <rdg wit="#B">do mu<am>&#772;</am><ex>n</ex>de non mh a faz ueer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A">E da me tal coita que non</rdg>
                    <rdg wit="#B">E ada mj tal coyta q<am>&#772;</am><ex>ue</ex> no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">sei de min consello prender</rdg>
                    <rdg wit="#B">sei de mj<am>&#772;</am><ex>n</ex> co<am>&#772;</am><ex>n</ex>sselho p<am>&#x1DD3;</am><ex>re</ex>nder</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">e <choice><orig>fez me</orig></choice> ia pauor perder</rdg>
                    <rdg wit="#B">e <choice><orig>fez mj</orig></choice> ia pauor p<am>̲</am><ex>er</ex>der</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">de mia mort' ay gran sazon</rdg>
                    <rdg wit="#B">de mha mort' ay g<am>&#x1DD3;</am><ex>ra</ex>m sazon</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">ond' ant' auia g<am>&#x1DD3;</am><ex>ra</ex>n pauor</rdg>
                    <rdg wit="#B">ond' ant' auya gra<am>&#772;</am><ex>n</ex> pauor</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">ueed' ora se a mayor</rdg>
                    <rdg wit="#B">ueed' ora se a mayor</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">coita no mundo de soffrer</rdg>
                    <rdg wit="#B">coyta no mu<am>&#772;</am><ex>n</ex>do de sofrer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A"><gap reason="error"/> nunca me d<am>&#x035B;</am><ex>eu</ex>s quis guisar</rdg>
                    <rdg wit="#B">E nu<am>&#772;</am><ex>n</ex>ca mj d<am>&#x035B;</am><ex>eu</ex>s q<am>&#x365;</am><ex>ui</ex>s g<am>&#x365;</am><ex>ui</ex>sar</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">en quanto cuidado prendi</rdg>
                    <rdg wit="#B">en q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>to cuydado p<am>&#x1DD3;</am><ex>re</ex>ndi</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">u cuidei al en cuidar y</rdg>
                    <rdg wit="#B">hu cuydei al en cuydar hi</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">en como podess' acabar</rdg>
                    <rdg wit="#B">en como podess' acabar</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">do que querria nulla ren</rdg>
                    <rdg wit="#B">do q<am>&#772;</am><ex>ue</ex> q<am>&#772;</am><ex>ue</ex>ria nulha ren</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">mais cuid' en quanto mal mi<am>&#772;</am><ex>n</ex> uen</rdg>
                    <rdg wit="#B">mays cuyd' eu q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>to mal mj ue<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">catiu' e mal dia naci</rdg>
                    <rdg wit="#B">catiu' e mal dia naçi</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="22">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> quant' og' est a meu coidar</rdg>
                    <rdg wit="#B">E qua<am>&#772;</am><ex>n</ex>to i' est a meu cuydar</rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A">ben per sei eu ca no<am>&#772;</am><ex>n</ex> a y</rdg>
                    <rdg wit="#B">be<am>&#772;</am><ex>n</ex> p<am>̲</am><ex>er</ex> sei eu ca no<am>&#772;</am><ex>n</ex> a hi</rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">coita mayor das que a mi</rdg>
                    <rdg wit="#B">coyta mayor das q<am>&#772;</am><ex>ue</ex> a mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="25">
                <app>
                    <rdg wit="#A">faz mia mort' ora deseiar</rdg>
                    <rdg wit="#B">faz mha mort' ora deseiar</rdg>
                </app>
            </l>
            <l n="26">
                <app>
                    <rdg wit="#A">p<am>̲</am><ex>er</ex>o no<am>&#772;</am><ex>n</ex> querria por en</rdg>
                    <rdg wit="#B">p<am>̲</am><ex>er</ex>o no<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex>ria p<am>&#7651;</am><ex>or</ex> en</rdg>
                </app>
            </l>
            <l n="27">
                <app>
                    <rdg wit="#A">morrer se coidasse auer ben</rdg>
                    <rdg wit="#B">mo?rer se cuydass' auer ben</rdg>
                </app>
            </l>
            <l n="28">
                <app>
                    <rdg wit="#A">da que por meu mal dia ui</rdg>
                    <rdg wit="#B">da q<am>&#772;</am><ex>ue</ex> p<am>&#7651;</am><ex>or</ex> meu mal dia uj</rdg>
                </app>
            </l>
        </lg>
    </div>
</tei>
