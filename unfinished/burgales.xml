<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem" corresp="#A82B186b">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A">De quantos mui coitados son</rdg>
                    <rdg wit="#B">De quant<am>&#42863;</am><ex>os</ex> muj coytados son</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">a que deus coita faz auer</rdg>
                    <rdg wit="#B">a que de<am>&#42863;</am><ex>us</ex> coyta faz auer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">min faz mais coitado uiuer</rdg>
                    <rdg wit="#B">mj faz mays coitado uiuer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">e <choice><orig>direi uus</orig></choice> per qual razon</rdg>
                    <rdg wit="#B">e <choice><orig>direy u<am>&#42863;</am><ex>os</ex></orig></choice> por qual razon</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A"><choice><orig>faz me</orig></choice> querer ben tal sennor</rdg>
                    <rdg wit="#B"><choice><orig>faz mj</orig></choice> querer ben tal senhor</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">a mais fremosa nen mellor</rdg>
                    <rdg wit="#B">a mays fremosa nen melhor</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">do mund e non mi a faz ueer</rdg>
                    <rdg wit="#B">do mu<am>&#772;</am><ex>n</ex>de non mh a faz ueer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="8">
                <app>
                    <rdg wit="#A">E da me tal coita que non</rdg>
                    <rdg wit="#B">E ada mj tal coyta q<am>&#772;</am><ex>ue</ex> no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">sei de min consello prender</rdg>
                    <rdg wit="#B">sei de mj<am>&#772;</am><ex>n</ex> co<am>&#772;</am><ex>n</ex>sselho p<am>&#x1DD3;</am><ex>re</ex>nder</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">e <choice><orig>fez me</orig></choice> ia pauor perder</rdg>
                    <rdg wit="#B">e <choice><orig>fez mj</orig></choice> ia pauor p<am>̲</am><ex>er</ex>der</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">de mia mort' ay gran sazon</rdg>
                    <rdg wit="#B">de mha mort' ay g<am>&#x1DD3;</am><ex>ra</ex>m sazon</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">ond' ant' auia g<am>&#x1DD3;</am><ex>ra</ex>n pauor</rdg>
                    <rdg wit="#B">ond' ant' auya gra<am>&#772;</am><ex>n</ex> pauor</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">ueed' ora se a mayor</rdg>
                    <rdg wit="#B">ueed' ora se a mayor</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">coita no mundo de soffrer</rdg>
                    <rdg wit="#B">coyta no mu<am>&#772;</am><ex>n</ex>do de sofrer</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="15">
                <app>
                    <rdg wit="#A"><gap reason="error"/> nunca me d<am>&#x035B;</am><ex>eu</ex>s quis guisar</rdg>
                    <rdg wit="#B">E nu<am>&#772;</am><ex>n</ex>ca mj d<am>&#x035B;</am><ex>eu</ex>s q<am>&#x365;</am><ex>ui</ex>s g<am>&#x365;</am><ex>ui</ex>sar</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">en quanto cuidado prendi</rdg>
                    <rdg wit="#B">en q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>to cuydado p<am>&#x1DD3;</am><ex>re</ex>ndi</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">u cuidei al en cuidar y</rdg>
                    <rdg wit="#B">hu cuydei al en cuydar hi</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">en como podess' acabar</rdg>
                    <rdg wit="#B">en como podess' acabar</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">do que querria nulla ren</rdg>
                    <rdg wit="#B">do q<am>&#772;</am><ex>ue</ex> q<am>&#772;</am><ex>ue</ex>ria nulha ren</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">mais cuid' en quanto mal mi<am>&#772;</am><ex>n</ex> uen</rdg>
                    <rdg wit="#B">mays cuyd' eu q<am>&#x1DD3;&#x0321;</am><ex>uan</ex>to mal mj ue<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">catiu' e mal dia naci</rdg>
                    <rdg wit="#B">catiu' e mal dia naçi</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="22">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> quant' og' est a meu coidar</rdg>
                    <rdg wit="#B">E qua<am>&#772;</am><ex>n</ex>to i' est a meu cuydar</rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A">ben per sei eu ca no<am>&#772;</am><ex>n</ex> a y</rdg>
                    <rdg wit="#B">be<am>&#772;</am><ex>n</ex> p<am>̲</am><ex>er</ex> sei eu ca no<am>&#772;</am><ex>n</ex> a hi</rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">coita mayor das que a mi</rdg>
                    <rdg wit="#B">coyta mayor das q<am>&#772;</am><ex>ue</ex> a mj<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="25">
                <app>
                    <rdg wit="#A">faz mia mort' ora deseiar</rdg>
                    <rdg wit="#B">faz mha mort' ora deseiar</rdg>
                </app>
            </l>
            <l n="26">
                <app>
                    <rdg wit="#A">p<am>̲</am><ex>er</ex>o no<am>&#772;</am><ex>n</ex> querria por en</rdg>
                    <rdg wit="#B">p<am>̲</am><ex>er</ex>o no<am>&#772;</am><ex>n</ex> q<am>&#772;</am><ex>ue</ex>ria p<am>&#7651;</am><ex>or</ex> en</rdg>
                </app>
            </l>
            <l n="27">
                <app>
                    <rdg wit="#A">morrer se coidasse auer ben</rdg>
                    <rdg wit="#B">mo?rer se cuydass' auer ben</rdg>
                </app>
            </l>
            <l n="28">
                <app>
                    <rdg wit="#A">da que por meu mal dia ui</rdg>
                    <rdg wit="#B">da q<am>&#772;</am><ex>ue</ex> p<am>&#7651;</am><ex>or</ex> meu mal dia uj</rdg>
                </app>
            </l>
        </lg>
    </div>
    <div type="poem" corresp="#A83B187b">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A">Pois contra uos non me ual _ mia sennor</rdg>
                    <rdg wit="#B">Poys contra uos non mj ual <del rend="overstrike">l</del> mha senhor</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">de uus seruir nen de uus <del>ben</del> querer ben</rdg>
                    <rdg wit="#B">de u<am>&#42863;</am><ex>os</ex> <am>&#7836;</am><ex>ser</ex>uir nen de u<am>&#42863;</am><ex>os</ex> _ q<am>&#772;</am><ex>ue</ex>rer bem</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">mayor ca mi<am>&#772;</am><ex>n</ex> sennor nen outra ren</rdg>
                    <rdg wit="#B">mayor ca mj senhor nen outra re<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A"><choice><orig>uallame</orig></choice> ia contra uos a mayor</rdg>
                    <rdg wit="#B"><choice><orig>ualh amj</orig></choice> <gap reason="error"/> contra uos a mayor</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">coita que soffro por uos das que deus</rdg>
                    <rdg wit="#B">coyta que sofro por uos das q<am>&#772;</am><ex>ue</ex> de<am>&#42863;</am><ex>us</ex></rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">fezo no mund' ay lume destes me<am>&#42863;</am><ex>us</ex></rdg>
                    <rdg wit="#B">feze no mund' ay lume destes me<am>&#42863;</am><ex>us</ex></rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">ollos e coita do meu coraçon</rdg>
                    <rdg wit="#B">olhos e coyta do meu coraçon</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> se me contra uos non ual se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">E se mj <am>ꝯ</am><ex>con</ex>t<am>&#x1DD3;</am><ex>ra</ex> uos no<am>&#772;</am><ex>n</ex> ual senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">a mui g<am>&#x1DD3;</am><ex>ra</ex>n coita que me por uos uen</rdg>
                    <rdg wit="#B">a muj g<am>&#x1DD3;</am><ex>ra</ex>m coita q<am>&#772;</am><ex>ue</ex> mj p<am>&#7651;</am><ex>or</ex> uos ue<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">p<am>̲</am><ex>er</ex> que perdi o dormir. <am>&#8266;</am><ex>e</ex> o sen</rdg>
                    <rdg wit="#B">p<am>̲</am><ex>er</ex> q<am>&#772;</am><ex>ue</ex> p<am>̲</am><ex>er</ex>di o dormir e o sen</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A"><choice><orig>ualla me</orig></choice> ia contra uos o pauor</rdg>
                    <rdg wit="#B"><choice><orig>ualhamj</orig></choice> ia <am>&#42862;&#772;</am><ex>contra</ex> uos o pauor</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">que de uos ei que nunca ousei diz<am>&#x035B;</am><ex>er</ex></rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> de uos ei q<am>&#772;</am><ex>ue</ex> nu<am>&#772;</am><ex>n</ex> cousei dizer</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">a coita que me fazedes auer</rdg>
                    <rdg wit="#B">a coyta q<am>&#772;</am><ex>ue</ex> me fazedes auer</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">q<am>&#772;</am><ex>ue</ex> neguei senpr'_ay mui g<am>&#x1DD3;</am><ex>ra</ex>n sazon</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> neguei semp<am>&#x1DD3;</am><ex>r<supplied>e</supplied> a</ex> muy g<am>&#x1DD3;</am><ex>ra</ex>m sazon</rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">E se m' esto contra uos mia se<am>&#772;</am><ex>n</ex>nor</rdg>
                    <rdg wit="#B">E se m' esto co<am>&#772;</am><ex>n</ex>t<am>&#x1DD3;</am><ex>ra</ex> uos mha senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">non ual quer' eu a d<am>&#x035B;</am><ex>eu</ex>s rogar por en</rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> ual q<am>&#772;</am><ex>ue</ex>r' eu a d<am>&#x035B;</am><ex>eu</ex>s rogar p<am>&#7651;</am><ex>or</ex> en</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">que me ualla q<am>&#772;</am><ex>ue</ex> uos en poder _ ten</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> mj ualha q<am>&#772;</am><ex>ue</ex> en poder me te<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">e que uos fez das do mundo mellor</rdg>
                    <rdg wit="#B">e q<am>&#772;</am><ex>ue</ex> u<am>&#42863;</am><ex>os</ex> fez das do mu<am>&#772;</am><ex>n</ex>do melh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">falar se<am>&#772;</am><ex>n</ex>nor. <am>&#8266;</am><ex>e</ex> mellor pareçer</rdg>
                    <rdg wit="#B">falar senh<am>&#7651;</am><ex>or</ex> e melhor parecer</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A">e se m' esto contra uos no<am>&#772;</am><ex>n</ex> ualer</rdg>
                    <rdg wit="#B">e se m' esto <am>&#42862;&#772;</am><ex>contra</ex> uos no<am>&#772;</am><ex>n</ex> ualer</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">non me ualrra log' y se mort no<am>&#772;</am><ex>n</ex></rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> me ualrra logu' j se morte no<am>&#772;</am><ex>n</ex></rdg>
                </app>
            </l>
        </lg>
    </div>
    
    <div type="poem" corresp="#A84B188b">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A"><choice><orig>Cvidaua meu</orig></choice> que amigos auia</rdg>
                    <rdg wit="#B"><choice><orig>Cuy daua meu</orig></choice> que amig<am>&#42863;</am><ex>os</ex> auia</rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">muitos no mundo mais mao pecado</rdg>
                    <rdg wit="#B">muyt<am>&#42863;</am><ex>os</ex> no mu<am>&#772;</am><ex>n</ex>do mays mao pecado</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">non ey amigos ca pois tan coitado</rdg>
                    <rdg wit="#B">non ei amig<am>&#42863;</am><ex>os</ex> ca poys tan coytado</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">iaço morrend' alguen se doeria</rdg>
                    <rdg wit="#B">iasco moirend' alguen sse doeria</rdg>
                </app>
            </l>
            <l n="5">
                <app>
                    <rdg wit="#A">de min que moir' e non ouso <del rend="scrape">r</del> dizer</rdg>
                    <rdg wit="#B">de mj que moyr' e non ouso _ dizer</rdg>
                </app>
            </l>
            <l n="6">
                <app>
                    <rdg wit="#A">o de que moyr' e que me faz morrer</rdg>
                    <rdg wit="#B">o de que moyr' e quen me faz moirer</rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">nono dig' eu nen por min ome nado</rdg>
                    <rdg wit="#B">non_lho digu' eu nen por mj homen nado</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">E os amigos en q<am>&#772;</am><ex>ue</ex> m' atreuia</rdg>
                    <rdg wit="#B">E os amig<am>&#42863;</am><ex>os</ex> en q<am>&#772;</am><ex>ue</ex> m' at<am>&#x035B;</am>reuia</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">de q<am>&#772;</am><ex>ue</ex> me tenn' _ en al por aiudado</rdg>
                    <rdg wit="#B">de q<am>&#772;</am><ex>ue</ex> me tenh' e en al p<am>&#7651;</am><ex>or</ex> aiudado</rdg>
                </app>
            </l>
            <l n="10">
                <app>
                    <rdg wit="#A">no llo dizen. mais se tan acordado</rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> lho dizem mais se ta<am>&#772;</am><ex>n</ex> acordado</rdg>
                </app>
            </l>
            <l n="11">
                <app>
                    <rdg wit="#A">foss' algun deles ben mi aiudaria</rdg>
                    <rdg wit="#B">foss' algu<am>&#772;</am><ex>n</ex> deles be<am>&#772;</am><ex>n</ex> m' amjdaria</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">se llo dissesse e nunca y perder</rdg>
                    <rdg wit="#B">se lho dissess' e nu<am>&#772;</am><ex>n</ex>ca hi p<am>̲</am><ex>er</ex>der</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">podia ren e poderia auer</rdg>
                    <rdg wit="#B">poda re<am>&#772;</am><ex>n</ex> e poderia ueer</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">mi p<am>̲</am><ex>er</ex> resto tolleito dun coidado</rdg>
                    <rdg wit="#B">mj p<am>&#x035B;</am><ex>er</ex> sto tolher tu<am>&#772;</am><ex>n</ex> cuydado</rdg>
                </app>
            </l>
            <l n="15">
                <app>
                    <rdg wit="#A">Miais aquest' e cousa mui desguisada</rdg>
                    <rdg wit="#B">Mays aq<am>&#772;</am><ex>ue</ex>st' e cousa muj desg<add place="above">i</add>sada</rdg>
                </app>
            </l>
            <l n="16">
                <app>
                    <rdg wit="#A">ca no<am>&#772;</am><ex>n</ex> sei eu quen tal poder ouuesse</rdg>
                    <rdg wit="#B">ca no<am>&#772;</am><ex>n</ex> sei eu q<am>&#771;&#x0321;</am><ex>uen</ex> tal poder ouuesse</rdg>
                </app>
            </l>
            <l n="17">
                <app>
                    <rdg wit="#A">pois mia se<am>&#772;</am><ex>n</ex>nor uisse q<am>&#772;</am><ex>ue</ex> lle soubesse</rdg>
                    <rdg wit="#B">pois mha senh<am>&#7651;</am><ex>or</ex> uisse q<am>&#772;</am><ex>ue</ex> lhi sobesse</rdg>
                </app>
            </l>
            <l n="18">
                <app>
                    <rdg wit="#A">dizer qual coita pois_la ui mia_dada</rdg>
                    <rdg wit="#B">diz<am>&#x035B;</am><ex>er</ex> q<am>&#x1DD3;</am><ex>ua</ex>l coita poila uj fadada</rdg>
                </app>
            </l>
            <l n="19">
                <app>
                    <rdg wit="#A">ca pois que uiss' o seu bon pareçer</rdg>
                    <rdg wit="#B">ca poys q<am>&#772;</am><ex>ue</ex> uiss' o seu bo<am>&#772;</am><ex>n</ex> parecer</rdg>
                </app>
            </l>
            <l n="20">
                <app>
                    <rdg wit="#A"><choice><orig>auer llia</orig></choice> log' eu descaeçer</rdg>
                    <rdg wit="#B"><choice><orig>au<am>&#772;</am><ex>n</ex>lhia</orig></choice> logu' eu descaecer</rdg>
                </app>
            </l>
            <l n="21">
                <app>
                    <rdg wit="#A">e dizer x' ante por si se podesse</rdg>
                    <rdg wit="#B">e diz<am>&#x035B;</am><ex>er</ex> _ ante p<am>&#7651;</am><ex>or</ex> ssy se podesse</rdg>
                </app>
            </l>
            <l n="22">
                <app>
                    <rdg wit="#A"><hi rend="guide">E</hi> ben coida quant' e meu connoçer</rdg>
                    <rdg wit="#B">E be<am>&#772;</am><ex>n</ex> cuydo quant' e meu conhocer</rdg>
                </app>
            </l>
            <l n="23">
                <app>
                    <rdg wit="#A">que pois fosse u a podesse ueer</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> pois foss' u a podesse ueer</rdg>
                </app>
            </l>
            <l n="24">
                <app>
                    <rdg wit="#A">que rren do meu nen do seu no<am>&#772;</am><ex>n</ex> dissesse</rdg>
                    <rdg wit="#B">q<am>&#772;</am><ex>ue</ex> re<am>&#772;</am><ex>n</ex> do meu ne<am>&#772;</am><ex>n</ex> do seu no<am>&#772;</am><ex>n</ex> dissesse</rdg>
                </app>
            </l>
        </lg>
    </div>
    
</tei>
