<?xml version="1.0" encoding="UTF-8"?>
<tei>
    <div type="poem" corresp="#A74B187">
        <lg type="cobra">
            <l n="1">
                <app>
                    <rdg wit="#A">Qve prol uus a uos mia sennor</rdg>
                    <rdg wit="#B">Que prol u<am>&#42863;</am><ex>os</ex> a uos mha senh<am>&#7651;</am><ex>or</ex></rdg>
                </app>
            </l>
            <l n="2">
                <app>
                    <rdg wit="#A">de me tan muito mal. faz<am>&#x035B;</am><ex>er</ex></rdg>
                    <rdg wit="#B">da mj tan muyto mal fazer</rdg>
                </app>
            </l>
            <l n="3">
                <app>
                    <rdg wit="#A">pois eu non sei al ben querer</rdg>
                    <rdg wit="#B">poys eu no<am>&#772;</am><ex>n</ex> sei al ben querer</rdg>
                </app>
            </l>
            <l n="4">
                <app>
                    <rdg wit="#A">no mundo nen ei d' al sabor</rdg>
                    <rdg wit="#B">no mu<am>&#772;</am><ex>n</ex>do non ey d' al sabor</rdg>
                </app>
            </l>
            <l n="5" type="refrao">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">D</hi>izede me</orig><reg><hi rend="guide">D</hi>izede-me</reg></choice> que prol uus a</rdg>
                    <rdg wit="#B"><choice><orig>dizedemj q<am>&#772;</am><ex>ue</ex></orig></choice> prol u<am>&#42863;</am><ex>os</ex> a</rdg>
                </app>
            </l>
        </lg>
        <lg type="cobra">
            <l n="6">
                <app>
                    <rdg wit="#A">E que prol uus a de fazer</rdg>
                    <rdg wit="#B"><gap reason="error"/> Que prol u<am>&#42863;</am><ex>os</ex> a de faz<am>&#x035B;</am><ex>er</ex></rdg>
                </app>
            </l>
            <l n="7">
                <app>
                    <rdg wit="#A">tan muito mal a q<am>&#772;</am><ex>ue</ex>n uoss' e</rdg>
                    <rdg wit="#B">ta<am>&#772;</am><ex>n</ex> muyto mal a q<am>&#771;&#x0321;</am><ex>uen</ex> uoss' e</rdg>
                </app>
            </l>
            <l n="8">
                <app>
                    <rdg wit="#A">no<am>&#772;</am><ex>n</ex> u<am>&#42863;</am><ex>os</ex> a prol p<am>̲</am><ex>er</ex> boa fe</rdg>
                    <rdg wit="#B">no<am>&#772;</am><ex>n</ex> u<am>&#42863;</am><ex>os</ex> a p<am>̲</am><ex>r</ex>ol p<am>̲</am><ex>er</ex> bo<am>&#772;</am><ex>&#771;</ex>a fe</rdg>
                </app>
            </l>
            <l n="9">
                <app>
                    <rdg wit="#A">e mia se<am>&#772;</am><ex>n</ex>nor se eu morrer</rdg>
                    <rdg wit="#B">e mha senh<am>&#7651;</am><ex>or</ex> se eu moirer</rdg>
                </app>
            </l>
            <l n="10" type="refrao">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">D</hi>izede me</orig></choice> q<am>&#772;</am><ex>ue</ex> p<am>&#807;</am><ex>ro</ex>l uus a</rdg>
                    <rdg wit="#B"><choice><orig>dizedemj</orig></choice> q<am>&#772;</am><ex>ue</ex> prol u<am>&#42863;</am><ex>os</ex> a</rdg>
                </app>
            </l>
        </lg>
        <lg>
            <l n="11">
                <app>
                    <rdg wit="#A">Que p<am>&#807;</am><ex>ro</ex>l uus a d' eu estar</rdg>
                    <rdg wit="#B">Que p<am>̲</am><ex>er</ex>ol u<am>&#42863;</am><ex>os</ex> a d' eu estar</rdg>
                </app>
            </l>
            <l n="12">
                <app>
                    <rdg wit="#A">se<am>&#772;</am><ex>n</ex>pre por uos <gap reason="error"/> g<am>&#x1DD3;</am><ex>ra</ex>nd' afan</rdg>
                    <rdg wit="#B">semp<am>&#x035B;</am><ex>re</ex> p<am>&#7651;</am><ex>or</ex> uos eng<am>&#x1DD3;</am><ex>ra</ex>nd' affam</rdg>
                </app>
            </l>
            <l n="13">
                <app>
                    <rdg wit="#A">e_est_e mui g<am>&#x1DD3;</am><ex>ra</ex>nde de pran</rdg>
                    <rdg wit="#B">este_ei muj g<am>&#x1DD3;</am><ex>ra</ex>nde de pram</rdg>
                </app>
            </l>
            <l n="14">
                <app>
                    <rdg wit="#A">e pois mi o uoss' amor matar</rdg>
                    <rdg wit="#B">e pois mh o uoss' amor matar</rdg>
                </app>
            </l>
            <l n="15" type="refrao">
                <app>
                    <rdg wit="#A"><choice><orig><hi rend="guide">D</hi>izede</orig></choice> que prol uus a</rdg>
                    <rdg wit="#B"><choice><orig>dizedem</orig></choice>j q<am>&#772;</am><ex>ue</ex> p<am>̲</am><ex>er</ex>ol u<am>&#42863;</am><ex>os</ex> a</rdg>
                </app>
            </l>
        </lg>
        <l n="16">
            <app>
                <rdg wit="#A">E uos lum' e dos ollos me<am>&#42863;</am><ex>us</ex></rdg>
                <rdg wit="#B"><gap reason="error"/> Uos lum' e d<am>&#42863;</am><ex>us</ex> olh<am>&#42863;</am><ex>us</ex> me<am>&#42863;</am><ex>us</ex></rdg>
            </app>
        </l>
        <l n="17">
            <app>
                <rdg wit="#A"><choice><orig>oyr uus e des</orig></choice> mal dizer</rdg>
                <rdg wit="#B"><choice><orig>oyruosedes</orig></choice> mal dizer</rdg>
            </app>
        </l>
        <l n="18">
            <app>
                <rdg wit="#A">_ por mi<am>&#772;</am><ex>n</ex> se eu por uos morrer</rdg>
                <rdg wit="#B"><del rend="overstrike">p<am>&#x035B;</am><ex>re</ex></del> p<am>&#7651;</am><ex>or</ex> mj s' eu p<am>&#7651;</am><ex>or</ex> uos moirer</rdg>
            </app>
        </l>
        <l n="19">
            <app>
                <rdg wit="#A">e se<am>&#772;</am><ex>n</ex>nor por lamor de deus</rdg>
                <rdg wit="#B">e senh<am>&#7651;</am><ex>or</ex> p<am>&#7651;</am><ex>or</ex> amor de de<am>&#42863;</am><ex>us</ex></rdg>
            </app>
        </l>
        <l n="20" type="refrao">
            <app>
                <rdg wit="#A"><choice><orig><hi rend="guide">D</hi>izede me</orig></choice> que p<am>&#807;</am><ex>ro</ex>l uus a</rdg>
                <rdg wit="#B"><choice><orig>dizedemj</orig></choice> q<am>&#772;</am><ex>ue</ex> prol u<am>&#42863;</am><ex>os</ex> a</rdg>
            </app>
        </l>
    </div>
</tei>
